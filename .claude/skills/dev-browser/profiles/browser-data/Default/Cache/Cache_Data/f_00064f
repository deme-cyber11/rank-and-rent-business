export function TrendArea({
  data,
  revenueLabel,
  profitLabel,
  primaryLabel,
  secondaryLabel,
  height = 600
}) {
  _s();
  const resolvedRevenueLabel = revenueLabel ?? primaryLabel ?? "Revenue";
  const resolvedProfitLabel = profitLabel ?? secondaryLabel ?? "Profit";
  const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
  const stackedData = useMemo(() => {
    return data.map((item) => {
      const [year, month] = item.date.split("-");
      const monthIndex = parseInt(month, 10) - 1;
      return {
        ...item,
        profit: item.profit ?? 0,
        cost: item.revenue - (item.profit ?? 0),
        formattedDate: `${monthNames[monthIndex]} ${year}`,
        shortLabel: `${monthNames[monthIndex]}`,
        year,
        monthIndex
      };
    });
  }, [data]);
  const yearBoundaries = useMemo(() => {
    const boundaries = [];
    let currentYear = "";
    stackedData.forEach((item, index) => {
      if (item.year !== currentYear) {
        currentYear = item.year;
        boundaries.push({ index, year: item.year });
      }
    });
    return boundaries;
  }, [stackedData]);
  const top5Indices = useMemo(() => {
    const indexed = data.map((item, index) => ({
      value: item.revenue,
      index
    }));
    indexed.sort((a, b) => b.value - a.value);
    return new Set(indexed.slice(0, 5).map((item) => item.index));
  }, [data]);
  const top3Indices = useMemo(() => {
    const indexed = data.map((item, index) => ({
      value: item.revenue,
      index
    }));
    indexed.sort((a, b) => b.value - a.value);
    return new Set(indexed.slice(0, 3).map((item) => item.index));
  }, [data]);
  const dataWithLabels = useMemo(() => {
    return stackedData.map((item, index) => ({
      ...item,
      revenueLabel: top5Indices.has(index) ? item.revenue : null
    }));
  }, [stackedData, top5Indices]);
  const maxValue = useMemo(() => {
    if (!data.length) return 1e4;
    const max = Math.max(...data.map((item) => item.revenue));
    const roundTo = max < 1e5 ? 5e4 : 1e5;
    return Math.ceil(max * 1.12 / roundTo) * roundTo;
  }, [data]);
  const yAxisTicks = useMemo(() => {
    const tickCount = 5;
    const step = maxValue / (tickCount - 1);
    return Array.from({ length: tickCount }, (_, i) => Math.round(i * step));
  }, [maxValue]);
  const customXAxisTick = (props) => {
    const { x, y, payload } = props;
    const dataPoint = stackedData[payload.index];
    if (!dataPoint) return null;
    const isJanuary = dataPoint.monthIndex === 0;
    const isFirstMonth = payload.index === 0;
    const isYearStart = isJanuary || isFirstMonth;
    const isJuly = dataPoint.monthIndex === 6;
    if (isYearStart) {
      return /* @__PURE__ */ jsxDEV("g", { transform: `translate(${x},${y})`, children: /* @__PURE__ */ jsxDEV(
