const defaultFormData = {
  productName: "",
  sku: "",
  productType: "removable",
  material: null,
  diameter: null,
  height: null,
  weightLbs: null,
  boxLength: null,
  boxWidth: null,
  boxHeight: null,
  costPerUnit: 0,
  salesPrice: 0,
  initialOrderQty: null,
  currentStock: 0,
  reorderPoint: null,
  safetyStock: null,
  supplier: "",
  supplierInvoice: null,
  shopifyProductId: null,
  shopifyVariantId: null,
  isActive: true
};
function productToFormData(product) {
  return {
    productName: product.productName,
    sku: product.sku,
    productType: product.productType,
    material: product.material,
    diameter: product.diameter,
    height: product.height,
    weightLbs: product.weightLbs,
    boxLength: product.boxLength,
    boxWidth: product.boxWidth,
    boxHeight: product.boxHeight,
    costPerUnit: product.costPerUnit,
    salesPrice: product.salesPrice,
    initialOrderQty: product.initialOrderQty,
    currentStock: product.currentStock,
    reorderPoint: product.reorderPoint,
    safetyStock: product.safetyStock,
    supplier: product.supplier,
    supplierInvoice: product.supplierInvoice,
    shopifyProductId: product.shopifyProductId,
    shopifyVariantId: product.shopifyVariantId,
    isActive: product.isActive
  };
}
