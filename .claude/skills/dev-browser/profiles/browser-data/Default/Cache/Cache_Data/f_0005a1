function ImportModal({
  open,
  onOpenChange,
  onSuccess,
  showToast
}) {
  _s2();
  const [file, setFile] = useState(null);
  const [importing, setImporting] = useState(false);
  const [error, setError] = useState(null);
  const fileInputRef = useRef(null);
  useEffect(() => {
    if (!open) {
      setFile(null);
      setError(null);
    }
  }, [open]);
  const handleFileChange = (e) => {
    const selectedFile = e.target.files?.[0];
    if (selectedFile) {
      if (!selectedFile.name.endsWith(".csv")) {
        setError("Please select a CSV file");
        setFile(null);
        return;
      }
      setFile(selectedFile);
      setError(null);
    }
  };
  const handleImport = async () => {
    if (!file) return;
    setImporting(true);
    setError(null);
    try {
      const formData = new FormData();
      formData.append("file", file);
      const res = await fetch("/api/bollards/gameplan/products/import", {
        method: "POST",
        body: formData
      });
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to import products");
      }
      const { created, updated, errors } = result.data || {};
      let message = "";
      if (created > 0) message += `${created} created`;
      if (updated > 0)
        message += message ? `, ${updated} updated` : `${updated} updated`;
      if (errors > 0)
        message += message ? `, ${errors} errors` : `${errors} errors`;
      showToast("success", message || "Import completed");
      onSuccess();
      onOpenChange(false);
    } catch (err) {
      setError(err instanceof Error ? err.message : "Import failed");
    } finally {
      setImporting(false);
    }
  };
