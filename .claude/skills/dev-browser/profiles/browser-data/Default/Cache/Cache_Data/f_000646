    const vendorLowerSet = new Set(vendors?.map((v) => v.toLowerCase()) || []);
    const repLowerSet = new Set(reps?.map((r) => r.toLowerCase()) || []);
    return aggregates.filter((agg) => {
      if (vendorLowerSet.size > 0 && !vendorLowerSet.has(agg.vendor.toLowerCase())) return false;
      if (repLowerSet.size > 0 && !repLowerSet.has(agg.rep.toLowerCase())) return false;
      return true;
    });
  }, [aggregates, vendors, reps]);
  const datasetStats = useMemo(() => {
    const totalRevenue = filteredAggregates.reduce((sum, agg) => sum + agg.revenue, 0);
    const totalProfit = filteredAggregates.reduce((sum, agg) => sum + agg.profit, 0);
    const totalOrders = filteredAggregates.reduce((sum, agg) => sum + agg.orders, 0);
    return { count: filteredAggregates.length, totalRevenue, totalProfit, totalOrders };
  }, [filteredAggregates]);
  useEffect(() => {
    console.log("[SalesDashboard] dataset stats from aggregates", datasetStats);
  }, [datasetStats]);
  const yearlyMonthlyAverages = useMemo(() => {
    const yearBuckets = /* @__PURE__ */ new Map();
    YEAR_SCOPE.forEach((year) => yearBuckets.set(year, /* @__PURE__ */ new Map()));
    filteredAggregates.forEach((agg) => {
      const parsedDate = normalizeDate(agg.month);
      if (!parsedDate) return;
      const year = parsedDate.getUTCFullYear();
      if (!YEAR_SCOPE.includes(year)) return;
      const month = monthKey(parsedDate);
      const buckets = yearBuckets.get(year);
      const bucket = buckets.get(month) ?? { revenue: 0, profit: 0, orders: 0 };
      bucket.revenue += agg.revenue;
      bucket.profit += agg.profit;
      bucket.orders += agg.orders;
      buckets.set(month, bucket);
    });
    const result = {};
    YEAR_SCOPE.forEach((year) => {
      const entries = Array.from(yearBuckets.get(year).values());
      if (!entries.length) {
        result[year] = { revenue: null, profit: null, orders: null, margin: null };
        return;
      }
      const monthCount = entries.length;
      const totalRevenue = entries.reduce((sum, entry) => sum + entry.revenue, 0);
      const totalProfit = entries.reduce((sum, entry) => sum + entry.profit, 0);
      const totalOrders = entries.reduce((sum, entry) => sum + entry.orders, 0);
      const marginSamples = entries.filter((entry) => entry.revenue > 0).map((entry) => entry.profit / entry.revenue);
      const marginAvg = marginSamples.length ? marginSamples.reduce((sum, value) => sum + value, 0) / marginSamples.length : null;
      result[year] = {
        revenue: totalRevenue / monthCount,
        profit: totalProfit / monthCount,
        orders: totalOrders / monthCount,
        margin: marginAvg
      };
    });
    return result;
  }, [filteredAggregates]);
  const revenueSideMetrics = useMemo(
    () => YEAR_SCOPE.map((year) => {
      const value = yearlyMonthlyAverages[year]?.revenue ?? null;
