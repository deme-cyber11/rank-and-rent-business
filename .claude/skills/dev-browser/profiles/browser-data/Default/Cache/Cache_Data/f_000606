export function ScheduledUpdatesPage() {
  _s();
  const auth = useAuth();
  const navigate = useNavigate();
  const [scheduledUpdates, setScheduledUpdates] = useState([]);
  const [isLoading, setIsLoading] = useState(true);
  const [error, setError] = useState(null);
  const [filters, setFilters] = useState({
    status: "scheduled",
    dateFrom: "",
    dateTo: "",
    sku: ""
  });
  const [isRefreshing, setIsRefreshing] = useState(false);
  const token = auth.session?.access_token;
  const summaryStats = useMemo(() => {
    const activeUpdates = scheduledUpdates.filter(
      (u) => u.status === "scheduled" || u.status === "pending"
    );
    return {
      total: scheduledUpdates.length,
      scheduled: activeUpdates.length,
      today: activeUpdates.filter((u) => isToday(u.effectiveDate)).length,
      thisWeek: activeUpdates.filter((u) => isWithinWeek(u.effectiveDate)).length
    };
  }, [scheduledUpdates]);
  const filteredUpdates = useMemo(() => {
    return scheduledUpdates.filter((update) => {
      if (filters.status !== "all" && update.status !== filters.status) {
        return false;
      }
      if (filters.dateFrom && update.effectiveDate) {
        if (update.effectiveDate < filters.dateFrom) {
          return false;
        }
      }
      if (filters.dateTo && update.effectiveDate) {
        if (update.effectiveDate > filters.dateTo) {
          return false;
        }
      }
      if (filters.sku) {
        const searchLower = filters.sku.toLowerCase();
        const matchesSku = update.sku.toLowerCase().includes(searchLower);
        const matchesTitle = update.productTitle.toLowerCase().includes(searchLower);
        if (!matchesSku && !matchesTitle) {
          return false;
        }
      }
      return true;
    });
  }, [scheduledUpdates, filters]);
  const fetchScheduledUpdates = useCallback(async (showRefreshState = false) => {
    if (!token) return;
    if (showRefreshState) {
      setIsRefreshing(true);
    } else {
      setIsLoading(true);
    }
    setError(null);
    try {
      const params = new URLSearchParams();
      const response = await fetch(`/api/marketing/scheduled-updates?${params.toString()}`, {
        method: "GET",
        headers: {
          Authorization: `Bearer ${token}`
        }
      });
      const data = await response.json();
      if (!response.ok) {
        throw new Error(data.error || `Failed to fetch scheduled updates (${response.status})`);
      }
      if (!data.success) {
        throw new Error(data.error || "Failed to fetch scheduled updates");
      }
      setScheduledUpdates(data.updates || []);
      console.log(`[ScheduledUpdatesPage] Loaded ${data.updates?.length || 0} updates`);
    } catch (err) {
      console.error("[ScheduledUpdatesPage] Error fetching updates:", err);
      setError(err instanceof Error ? err.message : "Failed to load scheduled updates");
    } finally {
      setIsLoading(false);
      setIsRefreshing(false);
    }
  }, [token]);
  useEffect(() => {
    if (token) {
      fetchScheduledUpdates();
    }
  }, [token, fetchScheduledUpdates]);
  const handleCancelUpdate = useCallback(async (id) => {
    if (!token) return;
    try {
      const response = await fetch("/api/marketing/scheduled-updates", {
        method: "PUT",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({ id, status: "cancelled" })
      });
      const data = await response.json();
      if (!response.ok) {
        throw new Error(data.error || "Failed to cancel update");
      }
      setScheduledUpdates(
        (prev) => prev.map(
          (update) => update.id === id ? { ...update, status: "cancelled" } : update
        )
      );
      console.log(`[ScheduledUpdatesPage] Cancelled update ${id}`);
    } catch (err) {
      console.error("[ScheduledUpdatesPage] Error cancelling update:", err);
      throw err;
    }
  }, [token]);
  const handleRescheduleUpdate = useCallback(async (id, newDate) => {
    if (!token) return;
    try {
      const response = await fetch("/api/marketing/scheduled-updates", {
        method: "PUT",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({ id, effectiveDate: newDate })
      });
      const data = await response.json();
      if (!response.ok) {
        throw new Error(data.error || "Failed to reschedule update");
      }
      setScheduledUpdates(
        (prev) => prev.map(
          (update) => update.id === id ? { ...update, effectiveDate: newDate, status: "scheduled" } : update
        )
      );
      console.log(`[ScheduledUpdatesPage] Rescheduled update ${id} to ${newDate}`);
    } catch (err) {
      console.error("[ScheduledUpdatesPage] Error rescheduling update:", err);
      throw err;
    }
  }, [token]);
  const handleBulkCancelUpdates = useCallback(async (ids) => {
    if (!token) return;
    try {
      const response = await fetch("/api/marketing/scheduled-updates", {
        method: "DELETE",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({ ids })
      });
      const data = await response.json();
      if (!response.ok) {
        throw new Error(data.error || "Failed to cancel updates");
      }
      setScheduledUpdates(
        (prev) => prev.map(
          (update) => ids.includes(update.id) ? { ...update, status: "cancelled" } : update
        )
      );
      console.log(`[ScheduledUpdatesPage] Bulk cancelled ${data.cancelled} update(s)`);
      if (data.failures && data.failures.length > 0) {
        console.warn(`[ScheduledUpdatesPage] Failed to cancel ${data.failed} update(s):`, data.failures);
      }
    } catch (err) {
      console.error("[ScheduledUpdatesPage] Error bulk cancelling updates:", err);
      throw err;
    }
  }, [token]);
  const handleStatusChange = (value) => {
    setFilters((prev) => ({ ...prev, status: value }));
  };
  const handleDateFromChange = (e) => {
    setFilters((prev) => ({ ...prev, dateFrom: e.target.value }));
  };
  const handleDateToChange = (e) => {
    setFilters((prev) => ({ ...prev, dateTo: e.target.value }));
  };
  const handleSkuChange = (e) => {
    setFilters((prev) => ({ ...prev, sku: e.target.value }));
  };
  const clearFilters = () => {
    setFilters({
      status: "scheduled",
      dateFrom: "",
      dateTo: "",
      sku: ""
    });
  };
  const hasActiveFilters = filters.status !== "scheduled" || filters.dateFrom !== "" || filters.dateTo !== "" || filters.sku !== "";
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV(
