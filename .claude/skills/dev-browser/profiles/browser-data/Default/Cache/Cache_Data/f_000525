function useDebounce(value, delay) {
  _s();
  const [debouncedValue, setDebouncedValue] = useState(value);
  useEffect(() => {
    const timer = setTimeout(() => setDebouncedValue(value), delay);
    return () => clearTimeout(timer);
  }, [value, delay]);
  return debouncedValue;
}
_s(useDebounce, "KDuPAtDOgxm8PU6legVJOb3oOmA=");
export function BlogWriterPage() {
  _s2();
  const auth = useAuth();
  const editorRef = useRef(null);
  const [keywords, setKeywords] = useState("");
  const [editorContent, setEditorContent] = useState("");
  const [seoMetadata, setSeoMetadata] = useState({
    title: "",
    slug: "",
    metaTitle: "",
    metaDescription: "",
    focusKeyword: ""
  });
  const [generationState, setGenerationState] = useState({
    isGenerating: false,
    error: null,
    content: null,
    aiGenerated: false,
    aiPrompt: null
  });
  const [toast, setToast] = useState({
    message: "",
    visible: false,
    variant: "success"
  });
  const [saveDraftState, setSaveDraftState] = useState({
    isSaving: false,
    savedPostId: null,
    postStatus: null,
    error: null,
    shopifyArticleId: null,
    shopifySyncStatus: null
  });
  const [submitReviewState, setSubmitReviewState] = useState({
    isSubmitting: false,
    error: null
  });
  const [adminActionState, setAdminActionState] = useState({
    isApproving: false,
    isRequestingChanges: false,
    error: null
  });
  const [requestChangesDialog, setRequestChangesDialog] = useState({
    open: false,
    note: ""
  });
  const [shopifyPublishDialog, setShopifyPublishDialog] = useState({
    open: false,
    blogs: [],
    isLoadingBlogs: false,
    selectedBlogId: "",
    publishMode: "now",
    scheduleDate: "",
    scheduleTime: "",
    isPublishing: false,
    error: null
  });
  const [shareDraftDialog, setShareDraftDialog] = useState({
    open: false,
    isLoading: false,
    isGenerating: false,
    isRevoking: false,
    shareToken: null,
    shareTokenExpiresAt: null,
    isExpired: false,
    error: null,
    copied: false
  });
  const isAdmin = useMemo(() => {
    return auth.role ? roleMeetsMinimum(auth.role, "admin") : false;
  }, [auth.role]);
  const debouncedContent = useDebounce(editorContent, 500);
  const debouncedTitle = useDebounce(seoMetadata.title, 500);
  const plainTextContent = useMemo(() => {
    if (!debouncedContent) return "";
    return debouncedContent.replace(/<[^>]*>/g, " ").replace(/&nbsp;/g, " ").replace(/\s+/g, " ").trim();
  }, [debouncedContent]);
  const showToast = useCallback((message, variant = "success", link) => {
    setToast({ message, visible: true, variant, link });
    setTimeout(() => setToast({ message: "", visible: false, variant: "success" }), link ? 5e3 : 3e3);
  }, []);
  const handleKeywordClick = useCallback(
    (keyword) => {
      setKeywords((prev) => {
        if (prev.trim()) {
          return `${prev.trim()}, ${keyword}`;
        }
        return keyword;
      });
      showToast(`Added "${keyword}" to target keywords`);
    },
    [showToast]
  );
  const handleSeoMetaChange = useCallback((field, value) => {
    setSeoMetadata((prev) => ({
      ...prev,
      [field]: value
    }));
  }, []);
  const handleEditorReady = useCallback((editor) => {
    editorRef.current = editor;
  }, []);
  const handleImageInsert = useCallback((image) => {
    const editor = editorRef.current;
    if (editor) {
      editor.chain().focus().setImage({
        src: image.dataUrl,
        alt: image.promptUsed
      }).run();
      showToast("Image inserted at cursor position");
    } else {
      const imgTag = `<img src="${image.dataUrl}" alt="${image.promptUsed}" />`;
      setEditorContent((prev) => prev + imgTag);
      showToast("Image appended to content");
    }
  }, [showToast]);
  const handleSaveDraft = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token) {
      showToast("Please log in to save drafts.", "info");
      return;
    }
    if (!editorContent) {
      showToast("Add content before saving.", "info");
      return;
    }
    const title = seoMetadata.title || generationState.content?.title || "Untitled Draft";
    setSaveDraftState((prev) => ({ ...prev, isSaving: true, error: null }));
    try {
      const keywordsArray = keywords.split(",").map((k) => k.trim()).filter(Boolean);
      const requestBody = {
        title,
        content: editorContent,
        excerpt: generationState.content?.excerpt || null,
        seo_title: seoMetadata.metaTitle || null,
        seo_description: seoMetadata.metaDescription || null,
        target_keywords: keywordsArray.length > 0 ? keywordsArray : null,
        source_gsc_query: seoMetadata.focusKeyword || null,
        ai_generated: generationState.aiGenerated,
        ai_prompt: generationState.aiPrompt ? JSON.stringify(generationState.aiPrompt) : null
      };
      const response = await fetch("/api/marketing/blog/drafts", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify(requestBody)
      });
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.message || result.error || "Failed to save draft");
      }
      const postId = result.data?.post?.id;
      const postStatus = result.data?.post?.status || "draft";
      setSaveDraftState((prev) => ({
        ...prev,
        isSaving: false,
        savedPostId: postId,
        postStatus,
        error: null
      }));
      showToast(
        "Draft saved successfully!",
        "success",
        postId ? { href: `/marketing/blog/posts/${postId}`, text: "View Draft" } : void 0
      );
      console.log("[BlogWriter] Draft saved:", postId);
    } catch (error) {
      console.error("[BlogWriter] Error saving draft:", error);
      setSaveDraftState((prev) => ({
        ...prev,
        isSaving: false,
        error: error.message || "Failed to save draft"
      }));
      showToast(error.message || "Failed to save draft", "info");
    }
  }, [auth.session?.access_token, editorContent, seoMetadata, generationState, keywords, showToast]);
  const handleSubmitForReview = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token) {
      showToast("Please log in to submit for review.", "info");
      return;
    }
    if (!saveDraftState.savedPostId) {
      showToast("Please save the draft first.", "info");
      return;
    }
    setSubmitReviewState({ isSubmitting: true, error: null });
    try {
      const response = await fetch("/api/marketing/blog/submit-for-review", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({ post_id: saveDraftState.savedPostId })
      });
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.message || result.error || "Failed to submit for review");
      }
      setSaveDraftState((prev) => ({
        ...prev,
        postStatus: "pending_review"
      }));
      setSubmitReviewState({ isSubmitting: false, error: null });
      showToast("Post submitted for review successfully!");
      console.log("[BlogWriter] Post submitted for review:", saveDraftState.savedPostId);
    } catch (error) {
      console.error("[BlogWriter] Error submitting for review:", error);
      setSubmitReviewState({
        isSubmitting: false,
        error: error.message || "Failed to submit for review"
      });
      showToast(error.message || "Failed to submit for review", "info");
    }
  }, [auth.session?.access_token, saveDraftState.savedPostId, showToast]);
  const handleApprove = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token) {
      showToast("Please log in to approve.", "info");
      return;
    }
    if (!saveDraftState.savedPostId) {
      showToast("No post to approve.", "info");
      return;
    }
    setAdminActionState({ isApproving: true, isRequestingChanges: false, error: null });
    try {
      const response = await fetch("/api/marketing/blog/approve", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({ post_id: saveDraftState.savedPostId })
      });
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.message || result.error || "Failed to approve post");
      }
      setSaveDraftState((prev) => ({
        ...prev,
        postStatus: "approved"
      }));
      setAdminActionState({ isApproving: false, isRequestingChanges: false, error: null });
      showToast("Post approved successfully!");
      console.log("[BlogWriter] Post approved:", saveDraftState.savedPostId);
    } catch (error) {
      console.error("[BlogWriter] Error approving post:", error);
      setAdminActionState({
        isApproving: false,
        isRequestingChanges: false,
        error: error.message || "Failed to approve post"
      });
      showToast(error.message || "Failed to approve post", "info");
    }
  }, [auth.session?.access_token, saveDraftState.savedPostId, showToast]);
  const handleOpenRequestChanges = useCallback(() => {
    setRequestChangesDialog({ open: true, note: "" });
  }, []);
  const handleRequestChanges = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token) {
      showToast("Please log in to request changes.", "info");
      return;
    }
    if (!saveDraftState.savedPostId) {
      showToast("No post to request changes on.", "info");
      return;
    }
    if (!requestChangesDialog.note.trim()) {
      showToast("Please provide feedback for the author.", "info");
      return;
    }
    setAdminActionState({ isApproving: false, isRequestingChanges: true, error: null });
    try {
      const response = await fetch("/api/marketing/blog/request-changes", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({
          post_id: saveDraftState.savedPostId,
          note: requestChangesDialog.note.trim()
        })
      });
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.message || result.error || "Failed to request changes");
      }
      setSaveDraftState((prev) => ({
        ...prev,
        postStatus: "draft"
      }));
      setAdminActionState({ isApproving: false, isRequestingChanges: false, error: null });
      setRequestChangesDialog({ open: false, note: "" });
      showToast("Changes requested - post returned to author.");
      console.log("[BlogWriter] Changes requested for post:", saveDraftState.savedPostId);
    } catch (error) {
      console.error("[BlogWriter] Error requesting changes:", error);
      setAdminActionState({
        isApproving: false,
        isRequestingChanges: false,
        error: error.message || "Failed to request changes"
      });
      showToast(error.message || "Failed to request changes", "info");
    }
  }, [auth.session?.access_token, saveDraftState.savedPostId, requestChangesDialog.note, showToast]);
  const loadShopifyBlogs = useCallback(async () => {
    setShopifyPublishDialog((prev) => ({ ...prev, isLoadingBlogs: true, error: null }));
    try {
      const response = await fetch("/api/marketing/blog/shopify-blogs");
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.error || "Failed to load Shopify blogs");
      }
      const blogs = result.data || [];
      setShopifyPublishDialog((prev) => ({
        ...prev,
        isLoadingBlogs: false,
        blogs,
        selectedBlogId: blogs.length > 0 ? blogs[0].id : ""
      }));
    } catch (error) {
      console.error("[BlogWriter] Error loading Shopify blogs:", error);
      setShopifyPublishDialog((prev) => ({
        ...prev,
        isLoadingBlogs: false,
        error: error.message || "Failed to load Shopify blogs"
      }));
    }
  }, []);
  const handleOpenShopifyPublish = useCallback(() => {
    setShopifyPublishDialog((prev) => ({
      ...prev,
      open: true,
      publishMode: "now",
      scheduleDate: "",
      scheduleTime: "",
      error: null
    }));
    loadShopifyBlogs();
  }, [loadShopifyBlogs]);
  const handlePublishToShopify = useCallback(
    async () => {
      const token = auth.session?.access_token;
      if (!token) {
        showToast("Please log in to publish.", "info");
        return;
      }
      if (!saveDraftState.savedPostId) {
        showToast("No post to publish.", "info");
        return;
      }
      if (!shopifyPublishDialog.selectedBlogId) {
        showToast("Please select a Shopify blog.", "info");
        return;
      }
      if (shopifyPublishDialog.publishMode === "schedule") {
        if (!shopifyPublishDialog.scheduleDate || !shopifyPublishDialog.scheduleTime) {
          showToast("Please select a date and time for scheduling.", "info");
          return;
        }
      }
      setShopifyPublishDialog((prev) => ({ ...prev, isPublishing: true, error: null }));
      try {
        const publishImmediately = shopifyPublishDialog.publishMode === "now";
        const response = await fetch("/api/marketing/blog/shopify-publish", {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
            Authorization: `Bearer ${token}`
          },
          body: JSON.stringify({
            postId: saveDraftState.savedPostId,
            shopifyBlogId: shopifyPublishDialog.selectedBlogId,
            publishImmediately
          })
        });
        const result = await response.json();
        if (!response.ok || !result.success) {
          throw new Error(result.error || "Failed to publish to Shopify");
        }
        setSaveDraftState((prev) => ({
          ...prev,
          postStatus: "published",
          shopifyArticleId: result.data?.shopifyArticleId || null,
          shopifySyncStatus: "synced"
        }));
        setShopifyPublishDialog((prev) => ({
          ...prev,
          open: false,
          isPublishing: false
        }));
        showToast(
          publishImmediately ? "Published to Shopify successfully!" : "Saved to Shopify as draft!",
          "success"
        );
        console.log("[BlogWriter] Published to Shopify:", result.data);
      } catch (error) {
        console.error("[BlogWriter] Error publishing to Shopify:", error);
        setShopifyPublishDialog((prev) => ({
          ...prev,
          isPublishing: false,
          error: error.message || "Failed to publish to Shopify"
        }));
        showToast(error.message || "Failed to publish to Shopify", "info");
      }
    },
    [
      auth.session?.access_token,
      saveDraftState.savedPostId,
      shopifyPublishDialog.selectedBlogId,
      shopifyPublishDialog.publishMode,
      shopifyPublishDialog.scheduleDate,
      shopifyPublishDialog.scheduleTime,
      showToast
    ]
  );
  const loadShareTokenInfo = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token || !saveDraftState.savedPostId) return;
    setShareDraftDialog((prev) => ({ ...prev, isLoading: true, error: null }));
    try {
      const response = await fetch(
        `/api/marketing/blog/share-token?post_id=${saveDraftState.savedPostId}`,
        {
          headers: { Authorization: `Bearer ${token}` }
        }
      );
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.error || "Failed to load share token info");
      }
      setShareDraftDialog((prev) => ({
        ...prev,
        isLoading: false,
        shareToken: result.data.share_token || null,
        shareTokenExpiresAt: result.data.share_token_expires_at || null,
        isExpired: result.data.is_expired || false
      }));
    } catch (error) {
      console.error("[BlogWriter] Error loading share token info:", error);
      setShareDraftDialog((prev) => ({
        ...prev,
        isLoading: false,
        error: error.message || "Failed to load share token info"
      }));
    }
  }, [auth.session?.access_token, saveDraftState.savedPostId]);
  const handleOpenShareDraft = useCallback(() => {
    setShareDraftDialog((prev) => ({
      ...prev,
      open: true,
      error: null,
      copied: false
    }));
    loadShareTokenInfo();
  }, [loadShareTokenInfo]);
  const handleGenerateShareToken = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token || !saveDraftState.savedPostId) {
      showToast("Please save the draft first.", "info");
      return;
    }
    setShareDraftDialog((prev) => ({ ...prev, isGenerating: true, error: null }));
    try {
      const response = await fetch("/api/marketing/blog/share-token", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({ post_id: saveDraftState.savedPostId })
      });
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.error || "Failed to generate share token");
      }
      setShareDraftDialog((prev) => ({
        ...prev,
        isGenerating: false,
        shareToken: result.data.share_token,
        shareTokenExpiresAt: result.data.share_token_expires_at,
        isExpired: false
      }));
      showToast("Share link generated successfully!");
      console.log("[BlogWriter] Share token generated:", result.data.share_token);
    } catch (error) {
      console.error("[BlogWriter] Error generating share token:", error);
      setShareDraftDialog((prev) => ({
        ...prev,
        isGenerating: false,
        error: error.message || "Failed to generate share token"
      }));
      showToast(error.message || "Failed to generate share token", "info");
    }
  }, [auth.session?.access_token, saveDraftState.savedPostId, showToast]);
  const handleRevokeShareToken = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token || !saveDraftState.savedPostId) return;
    setShareDraftDialog((prev) => ({ ...prev, isRevoking: true, error: null }));
    try {
      const response = await fetch("/api/marketing/blog/share-token", {
        method: "DELETE",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({ post_id: saveDraftState.savedPostId })
      });
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.error || "Failed to revoke share token");
      }
      setShareDraftDialog((prev) => ({
        ...prev,
        isRevoking: false,
        shareToken: null,
        shareTokenExpiresAt: null,
        isExpired: false
      }));
      showToast("Share link revoked.");
      console.log("[BlogWriter] Share token revoked");
    } catch (error) {
      console.error("[BlogWriter] Error revoking share token:", error);
      setShareDraftDialog((prev) => ({
        ...prev,
        isRevoking: false,
        error: error.message || "Failed to revoke share token"
      }));
      showToast(error.message || "Failed to revoke share token", "info");
    }
  }, [auth.session?.access_token, saveDraftState.savedPostId, showToast]);
  const handleCopyShareUrl = useCallback(async () => {
    if (!shareDraftDialog.shareToken) return;
    const shareUrl = `${window.location.origin}/marketing/blog/preview/${shareDraftDialog.shareToken}`;
    try {
      await navigator.clipboard.writeText(shareUrl);
      setShareDraftDialog((prev) => ({ ...prev, copied: true }));
      showToast("Share link copied to clipboard!");
      setTimeout(() => {
        setShareDraftDialog((prev) => ({ ...prev, copied: false }));
      }, 2e3);
    } catch (error) {
      console.error("[BlogWriter] Error copying to clipboard:", error);
      showToast("Failed to copy link", "info");
    }
  }, [shareDraftDialog.shareToken, showToast]);
  const handleGenerate = useCallback(
    async (formData) => {
      const token = auth.session?.access_token;
      if (!token) {
        throw new Error("Please log in to generate content.");
      }
      const toneMap = {
        professional: "professional",
        conversational: "friendly",
        educational: "technical"
      };
      const lengthMap = {
        "500": "short",
        "1000": "medium",
        "1500": "medium",
        "2000": "long"
      };
      const keywordsArray = formData.keywords.split(",").map((k) => k.trim()).filter(Boolean);
      const requestBody = {
        topic: formData.topic,
        keywords: keywordsArray,
        tone: toneMap[formData.tone] || "professional",
        length: lengthMap[formData.wordCount] || "medium"
      };
      setGenerationState((prev) => ({
        ...prev,
        isGenerating: true,
        error: null
      }));
      try {
        const response = await fetch("/api/marketing/blog/generate", {
          method: "POST",
          headers: {
            "Content-Type": "application/json",
            Authorization: `Bearer ${token}`
          },
          body: JSON.stringify(requestBody)
        });
        const result = await response.json();
        if (!response.ok || !result.success) {
          throw new Error(result.message || result.error || "Failed to generate content");
        }
        const content = {
          title: result.data.title,
          content: result.data.content,
          excerpt: result.data.excerpt,
          seoTitle: result.data.seoTitle,
          seoDescription: result.data.seoDescription
        };
        setGenerationState({
          isGenerating: false,
          error: null,
          content,
          aiGenerated: true,
          aiPrompt: formData
        });
        showToast("Blog draft generated successfully!", "success");
        return content;
      } catch (error) {
        setGenerationState((prev) => ({
          ...prev,
          isGenerating: false,
          error: error.message || "Failed to generate content"
        }));
        throw error;
      }
    },
    [auth.session?.access_token, showToast]
  );
  const handleContentGenerated = useCallback((content) => {
    setEditorContent(content.content);
    setSeoMetadata((prev) => ({
      ...prev,
      title: content.title,
      metaTitle: content.seoTitle || "",
      metaDescription: content.seoDescription || ""
    }));
    console.log("[BlogWriter] Content generated and loaded into editor:", content.title);
  }, []);
