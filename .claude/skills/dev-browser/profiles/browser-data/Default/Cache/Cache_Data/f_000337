export default function VerifyPositionModal({
  isOpen,
  onClose,
  onSuccess,
  dealId,
  detectedPosition,
  existingPositionCount
}) {
  _s();
  const [loading, setLoading] = useState(false);
  const [error, setError] = useState(null);
  const [formData, setFormData] = useState({
    position_number: String(existingPositionCount + 1),
    lender_name: "",
    payment_amount: "",
    frequency: "daily",
    balance_remaining: "",
    payoff_date: "",
    notes: ""
  });
  useEffect(() => {
    if (detectedPosition) {
      setFormData({
        position_number: String(existingPositionCount + 1),
        lender_name: detectedPosition.lender_name,
        payment_amount: String(detectedPosition.amount),
        frequency: detectedPosition.frequency === "weekly" ? "weekly" : detectedPosition.frequency === "monthly" ? "monthly" : "daily",
        balance_remaining: "",
        payoff_date: "",
        notes: ""
      });
    } else {
      setFormData({
        position_number: String(existingPositionCount + 1),
        lender_name: "",
        payment_amount: "",
        frequency: "daily",
        balance_remaining: "",
        payoff_date: "",
        notes: ""
      });
    }
  }, [detectedPosition, existingPositionCount]);
  const handleChange = (field, value) => {
    setFormData((prev) => ({ ...prev, [field]: value }));
    setError(null);
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    setError(null);
    if (!formData.lender_name.trim()) {
      setError("Lender name is required");
      return;
    }
    if (!formData.payment_amount || parseFloat(formData.payment_amount) <= 0) {
      setError("Valid payment amount is required");
      return;
    }
    if (!formData.position_number || parseInt(formData.position_number) < 1) {
      setError("Position number must be at least 1");
      return;
    }
    try {
      setLoading(true);
      const { error: insertError } = await supabase.from("deal_actual_positions").insert({
        deal_id: dealId,
        detected_position_id: detectedPosition?.id || null,
        position_number: parseInt(formData.position_number),
        lender_name: formData.lender_name.trim(),
        payment_amount: parseFloat(formData.payment_amount),
        frequency: formData.frequency,
        balance_remaining: formData.balance_remaining ? parseFloat(formData.balance_remaining) : null,
        payoff_date: formData.payoff_date || null,
        notes: formData.notes.trim() || null
      });
      if (insertError) throw insertError;
      onSuccess();
    } catch (err) {
      console.error("Error saving position:", err);
      setError(err.message || "Failed to save position");
    } finally {
      setLoading(false);
    }
  };
  if (!isOpen) return null;
  const isVerifying = !!detectedPosition;
