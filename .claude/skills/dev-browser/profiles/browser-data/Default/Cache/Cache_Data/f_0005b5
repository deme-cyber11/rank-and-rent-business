function StarRating({ rating, size = "sm" }) {
  const sizeClasses = {
    sm: "h-4 w-4",
    md: "h-5 w-5",
    lg: "h-6 w-6"
  };
  const stars = [];
  const fullStars = Math.floor(rating);
  const hasHalfStar = rating % 1 >= 0.5;
  for (let i = 0; i < 5; i++) {
    if (i < fullStars) {
      stars.push(
        /* @__PURE__ */ jsxDEV(Star, { className: cn(sizeClasses[size], "fill-yellow-500 text-yellow-500") }, i, false, {
          fileName: "C:/Projects/Source 4 Industries/src/pages/marketing/reviews/reviews-client.tsx",
          lineNumber: 114,
          columnNumber: 9
        }, this)
      );
    } else if (i === fullStars && hasHalfStar) {
      stars.push(
        /* @__PURE__ */ jsxDEV(StarHalf, { className: cn(sizeClasses[size], "fill-yellow-500 text-yellow-500") }, i, false, {
          fileName: "C:/Projects/Source 4 Industries/src/pages/marketing/reviews/reviews-client.tsx",
          lineNumber: 118,
          columnNumber: 9
        }, this)
      );
    } else {
      stars.push(/* @__PURE__ */ jsxDEV(Star, { className: cn(sizeClasses[size], "text-muted-foreground") }, i, false, {
        fileName: "C:/Projects/Source 4 Industries/src/pages/marketing/reviews/reviews-client.tsx",
        lineNumber: 121,
        columnNumber: 18
      }, this));
    }
  }
  return /* @__PURE__ */ jsxDEV("div", { className: "flex items-center gap-0.5", children: stars }, void 0, false, {
    fileName: "C:/Projects/Source 4 Industries/src/pages/marketing/reviews/reviews-client.tsx",
    lineNumber: 125,
    columnNumber: 10
  }, this);
}
