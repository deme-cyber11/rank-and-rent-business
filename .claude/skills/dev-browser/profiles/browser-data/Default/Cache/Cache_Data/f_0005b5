export function ReviewsDashboardClient({
  reviews,
  analytics,
  trends,
  products
}) {
  _s();
  const [platformFilter, setPlatformFilter] = useState("all");
  const [ratingFilter, setRatingFilter] = useState("all");
  const [dateRangeFilter, setDateRangeFilter] = useState("all");
  const [productFilter, setProductFilter] = useState("all");
  const [searchQuery, setSearchQuery] = useState("");
  const [isSyncingStamped, setIsSyncingStamped] = useState(false);
  const [isSyncingGoogle, setIsSyncingGoogle] = useState(false);
  const [syncMessage, setSyncMessage] = useState(
    null
  );
  const alerts = [];
  if (reviews.source === "sample") {
    alerts.push({
      key: "source",
      message: "Showing sample review data. Connect your platforms to see real reviews.",
      variant: "info"
    });
  }
  if (analytics.warning) {
    alerts.push({ key: "analytics", message: analytics.warning, variant: "warning" });
  }
  const filteredReviews = useMemo(() => {
    let filtered = reviews.data || [];
    if (platformFilter !== "all") {
      filtered = filtered.filter((r) => r.platform === platformFilter);
    }
    if (ratingFilter !== "all") {
      filtered = filtered.filter((r) => r.rating === parseInt(ratingFilter));
    }
    if (dateRangeFilter !== "all") {
      const now = /* @__PURE__ */ new Date();
      const daysAgo = {
        "7d": 7,
        "30d": 30,
        "90d": 90,
        "1y": 365
      }[dateRangeFilter] || 0;
      const cutoffDate = new Date(now.getTime() - daysAgo * 24 * 60 * 60 * 1e3);
      filtered = filtered.filter((r) => new Date(r.reviewDate) >= cutoffDate);
    }
    if (productFilter !== "all") {
      filtered = filtered.filter((r) => r.productId === productFilter);
    }
    if (searchQuery.trim()) {
      const query = searchQuery.toLowerCase();
      filtered = filtered.filter(
        (r) => r.reviewText.toLowerCase().includes(query) || r.reviewerName.toLowerCase().includes(query) || r.productName?.toLowerCase().includes(query)
      );
    }
    return filtered;
  }, [reviews.data, platformFilter, ratingFilter, dateRangeFilter, productFilter, searchQuery]);
  const ratingDistributionData = useMemo(() => {
    const dist = analytics.data.ratingDistribution;
    return [
      { rating: "1★", count: dist[1], fill: "#ef4444" },
      { rating: "2★", count: dist[2], fill: "#f97316" },
      { rating: "3★", count: dist[3], fill: "#eab308" },
      { rating: "4★", count: dist[4], fill: "#84cc16" },
      { rating: "5★", count: dist[5], fill: "#22c55e" }
    ];
  }, [analytics.data.ratingDistribution]);
  const trendsChartData = useMemo(() => {
    const grouped = {};
    trends.data.forEach((trend) => {
      if (!grouped[trend.date]) {
        grouped[trend.date] = { date: trend.date, stamped: 0, google: 0 };
      }
      if (trend.platform === "stamped") {
        grouped[trend.date].stamped = trend.reviewCount;
      } else {
        grouped[trend.date].google = trend.reviewCount;
      }
    });
    return Object.values(grouped).sort((a, b) => a.date.localeCompare(b.date)).map((item) => ({
      ...item,
      date: new Date(item.date).toLocaleDateString("en-US", { month: "short", day: "numeric" })
    }));
  }, [trends.data]);
  const handleSync = async (platform) => {
    const setLoading = platform === "stamped" ? setIsSyncingStamped : setIsSyncingGoogle;
    setLoading(true);
    setSyncMessage(null);
    try {
      const endpoint = platform === "stamped" ? "/api/sync/stamped" : "/api/sync/google-reviews";
      const response = await fetch(endpoint, {
        method: "POST",
        headers: { "Content-Type": "application/json" }
      });
      const result = await response.json();
      if (result.success) {
        setSyncMessage({
          type: "success",
          text: `${platform === "stamped" ? "Stamped.io" : "Google"} sync completed: ${result.recordsSynced || 0} reviews synced`
        });
        setTimeout(() => window.location.reload(), 2e3);
      } else {
        setSyncMessage({
          type: "error",
          text: result.message || "Sync failed. Check console for details."
        });
      }
    } catch (error) {
      setSyncMessage({
        type: "error",
        text: "Failed to start sync. Please try again."
      });
    } finally {
      setLoading(false);
    }
  };
  const trendDirection = analytics.data.recentTrend.change >= 0 ? "up" : "down";
  const TrendIcon = trendDirection === "up" ? TrendingUp : TrendingDown;
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-10", children: [
    /* @__PURE__ */ jsxDEV(
      SectionHeader,
      {
        title: "Reviews",
        description: "Monitor and analyze customer reviews from Stamped.io and Google Reviews to track satisfaction and brand reputation.",
        badge: "Marketing",
        actions: /* @__PURE__ */ jsxDEV("div", { className: "flex items-center gap-2", children: [
          /* @__PURE__ */ jsxDEV(
            Button,
            {
              onClick: () => handleSync("stamped"),
              disabled: isSyncingStamped,
              variant: "outline",
              size: "sm",
              className: "gap-2",
              children: [
                /* @__PURE__ */ jsxDEV(RefreshCw, { className: cn("h-4 w-4", isSyncingStamped && "animate-spin") }, void 0, false, {
