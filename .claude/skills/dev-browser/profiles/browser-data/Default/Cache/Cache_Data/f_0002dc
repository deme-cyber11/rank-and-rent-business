  if (filterScore !== "All") {
    filteredDeals = filteredDeals.filter((d) => {
      const effectiveScore = d.score ?? (d.statements ? calculateDealScore(d, d.statements) : null);
      return effectiveScore === filterScore;
    });
  }
  if (sortBy === "amount") {
    filteredDeals.sort((a, b) => (b.desired_loan_amount || 0) - (a.desired_loan_amount || 0));
  } else if (sortBy === "status") {
    const statusOrder = {
      New: 0,
      Analyzing: 1,
      Matched: 2,
      Submitted: 3,
      Pending: 4,
      Approved: 5,
      Funded: 6,
      Declined: 7
    };
    filteredDeals.sort((a, b) => statusOrder[a.status] - statusOrder[b.status]);
  }
  const handleDeleteDeal = async (dealId) => {
    if (!confirm("Are you sure you want to delete this deal?")) return;
    try {
      const { error: error2 } = await supabase.from("deals").delete().eq("id", dealId);
      if (error2) throw error2;
      setDeals(deals.filter((d) => d.id !== dealId));
    } catch (err) {
      console.error("Error deleting deal:", err);
      alert("Failed to delete deal");
    }
  };
  const handleScoreChange = async (dealId, newScore) => {
    try {
      const { data: { user } } = await supabase.auth.getUser();
      if (!user) throw new Error("Not authenticated");
      const updateData = newScore ? {
        score: newScore,
        score_override_by: user.id,
        score_override_at: (/* @__PURE__ */ new Date()).toISOString()
      } : {
        score: null,
        score_override_by: null,
        score_override_at: null
      };
      const { error: error2 } = await supabase.from("deals").update(updateData).eq("id", dealId);
      if (error2) throw error2;
      setDeals(deals.map(
        (d) => d.id === dealId ? { ...d, ...updateData } : d
      ));
    } catch (err) {
      console.error("Error updating deal score:", err);
      alert("Failed to update score");
    }
