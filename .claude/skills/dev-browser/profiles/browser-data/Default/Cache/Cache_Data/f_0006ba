export function ImpactAnalysis({ records, salesData, showDetails = false }) {
  _s();
  const [detailsExpanded, setDetailsExpanded] = useState(showDetails);
  const summary = useMemo(() => {
    const increases = records.filter((r) => r.priceChange > 0);
    const decreases = records.filter((r) => r.priceChange < 0);
    const unchanged = records.filter((r) => r.priceChange === 0);
    const avgPriceChange = records.length > 0 ? records.reduce((sum, r) => sum + r.priceChange, 0) / records.length : 0;
    const avgPriceChangePercent = records.length > 0 ? records.reduce((sum, r) => sum + r.priceChangePercent, 0) / records.length : 0;
    const totalPriceIncrease = increases.reduce((sum, r) => sum + r.priceChange, 0);
    const totalPriceDecrease = Math.abs(
      decreases.reduce((sum, r) => sum + r.priceChange, 0)
    );
    const largestIncrease = increases.length > 0 ? increases.reduce((max, r) => r.priceChange > max.priceChange ? r : max) : null;
    const largestDecrease = decreases.length > 0 ? decreases.reduce((min, r) => r.priceChange < min.priceChange ? r : min) : null;
    let estimatedRevenueImpact = null;
    if (salesData && salesData.size > 0) {
      estimatedRevenueImpact = records.reduce((total, record) => {
        const sales = salesData.get(record.sku);
        if (sales && sales.unitsSold > 0) {
          return total + record.priceChange * sales.unitsSold;
        }
        return total;
      }, 0);
    }
    return {
      totalProducts: records.length,
      priceIncreases: increases.length,
      priceDecreases: decreases.length,
      priceUnchanged: unchanged.length,
      avgPriceChange,
      avgPriceChangePercent,
      totalPriceIncrease,
      totalPriceDecrease,
      largestIncrease,
      largestDecrease,
      estimatedRevenueImpact
    };
  }, [records, salesData]);
  const topChanges = useMemo(() => {
    const increases = records.filter((r) => r.priceChange > 0);
    const decreases = records.filter((r) => r.priceChange < 0);
    const sortByImpact = (a, b) => {
      if (salesData && salesData.size > 0) {
        const aSales = salesData.get(a.sku);
        const bSales = salesData.get(b.sku);
        const aImpact = aSales ? a.priceChange * aSales.unitsSold : 0;
        const bImpact = bSales ? b.priceChange * bSales.unitsSold : 0;
        return Math.abs(bImpact) - Math.abs(aImpact);
      }
      return Math.abs(b.priceChange) - Math.abs(a.priceChange);
    };
    const topIncreases = increases.sort(sortByImpact).slice(0, 5).map((record) => {
      const sales = salesData?.get(record.sku);
      const estimatedImpact = sales ? record.priceChange * sales.unitsSold : void 0;
      return { record, estimatedImpact };
    });
    const topDecreases = decreases.sort(sortByImpact).slice(0, 5).map((record) => {
      const sales = salesData?.get(record.sku);
      const estimatedImpact = sales ? record.priceChange * sales.unitsSold : void 0;
      return { record, estimatedImpact };
    });
    return { increases: topIncreases, decreases: topDecreases };
  }, [records, salesData]);
  const increaseRatio = summary.priceIncreases + summary.priceDecreases > 0 ? summary.priceIncreases / (summary.priceIncreases + summary.priceDecreases) : 0.5;
