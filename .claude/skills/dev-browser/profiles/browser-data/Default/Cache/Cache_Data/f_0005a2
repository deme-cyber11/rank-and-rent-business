const defaultAdjustmentFormData = {
  action: "restock",
  quantity: 1,
  referenceId: "",
  notes: ""
};
function StockAdjustmentModal({
  open,
  onOpenChange,
  product,
  onSuccess,
  showToast
}) {
  _s2();
  const [formData, setFormData] = useState(defaultAdjustmentFormData);
  const [saving, setSaving] = useState(false);
  const [error, setError] = useState(null);
  useEffect(() => {
    if (open) {
      setFormData(defaultAdjustmentFormData);
      setError(null);
    }
  }, [open, product]);
  const handleChange = (field, value) => {
    setFormData((prev) => ({ ...prev, [field]: value }));
  };
  const validateForm = () => {
    if (formData.quantity <= 0) {
      return "Quantity must be greater than 0";
    }
    if (!formData.action) {
      return "Please select an action";
    }
    return null;
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    if (!product) {
      setError("No product selected");
      return;
    }
    const validationError = validateForm();
    if (validationError) {
      setError(validationError);
      return;
    }
    setSaving(true);
    setError(null);
    try {
      const res = await fetch("/api/bollards/gameplan/supply-chain", {
        method: "POST",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({
          productId: product.id,
          action: formData.action,
          quantity: formData.quantity,
          referenceId: formData.referenceId || null,
          notes: formData.notes || null
        })
      });
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to update inventory");
      }
      const actionLabel = formData.action === "restock" ? "restocked" : formData.action === "sale" ? "sold" : formData.action === "return" ? "returned" : "adjusted";
      showToast("success", `${product.productName} ${actionLabel}: ${formData.quantity} units`);
      onSuccess();
      onOpenChange(false);
    } catch (err) {
      setError(err instanceof Error ? err.message : "An error occurred");
    } finally {
      setSaving(false);
    }
  };
  const previewStock = useMemo(() => {
    if (!product) return null;
    const qty = formData.quantity || 0;
    if (formData.action === "restock" || formData.action === "return") {
      return product.currentStock + qty;
    }
    if (formData.action === "sale") {
      return Math.max(0, product.currentStock - qty);
    }
    return product.currentStock + qty;
  }, [product, formData.action, formData.quantity]);
  return /* @__PURE__ */ jsxDEV(Dialog, { open, onOpenChange, children: /* @__PURE__ */ jsxDEV(DialogContent, { className: "max-w-md", children: [
    /* @__PURE__ */ jsxDEV(DialogHeader, { children: [
      /* @__PURE__ */ jsxDEV(DialogTitle, { children: "Adjust Stock" }, void 0, false, {
        fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/SupplyChainTab.tsx",
        lineNumber: 362,
        columnNumber: 11
      }, this),
      /* @__PURE__ */ jsxDEV(DialogDescription, { children: product ? /* @__PURE__ */ jsxDEV(Fragment, { children: [
        "Update inventory for ",
        /* @__PURE__ */ jsxDEV("span", { className: "font-medium text-foreground", children: product.productName }, void 0, false, {
          fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/SupplyChainTab.tsx",
          lineNumber: 365,
          columnNumber: 36
        }, this)
      ] }, void 0, true, {
        fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/SupplyChainTab.tsx",
        lineNumber: 365,
        columnNumber: 13
      }, this) : "Update inventory for this product." }, void 0, false, {
