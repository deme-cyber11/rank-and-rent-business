export function EmailConnectionDialog({
  open,
  onOpenChange,
  connectionStatus,
  onConnectionChange
}) {
  _s();
  const auth = useAuth();
  const [step, setStep] = useState("select");
  const [error, setError] = useState(null);
  const [disconnecting, setDisconnecting] = useState(false);
  useEffect(() => {
    if (open) {
      setStep(connectionStatus?.isConnected ? "success" : "select");
      setError(null);
    }
  }, [open, connectionStatus]);
  const handleConnect = async (provider) => {
    setStep("connecting");
    setError(null);
    try {
      const { error: oauthError } = await signInWithOAuth(provider, {
        redirectTo: window.location.pathname
      });
      if (oauthError) {
        throw oauthError;
      }
      setError("Failed to initiate OAuth flow. Please try again.");
      setStep("error");
    } catch (err) {
      console.error("Error connecting email:", err);
      setError(err instanceof Error ? err.message : "Failed to connect email account");
      setStep("error");
    }
  };
  const handleDisconnect = async () => {
    if (!connectionStatus?.isConnected) return;
    setDisconnecting(true);
    setError(null);
    try {
      const token = auth.session?.access_token;
      if (!token) {
        throw new Error("Not authenticated");
      }
      const res = await fetch("/api/sales/follow-ups/email-connection", {
        method: "DELETE",
        headers: {
          "Authorization": `Bearer ${token}`
        }
      });
      const jsonBody = await res.json();
      if (!res.ok || !jsonBody.success) {
        throw new Error(jsonBody.error || "Failed to disconnect email account");
      }
      onConnectionChange();
      onOpenChange(false);
    } catch (err) {
      console.error("Error disconnecting email:", err);
      setError(err instanceof Error ? err.message : "Failed to disconnect email account");
    } finally {
      setDisconnecting(false);
    }
