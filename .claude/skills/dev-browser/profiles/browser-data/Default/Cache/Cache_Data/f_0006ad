export function IssueDetailPanel({ issueId, onClose }) {
  _s();
  const [loading, setLoading] = useState(false);
  const [error, setError] = useState(null);
  const [data, setData] = useState(null);
  useEffect(() => {
    if (!issueId) {
      setData(null);
      setError(null);
      return;
    }
    let cancelled = false;
    setLoading(true);
    setError(null);
    const fetchIssueDetails = async () => {
      try {
        const res = await fetch(`/api/sales/customer-service/${issueId}`);
        const jsonBody = await res.json();
        if (cancelled) return;
        if (!res.ok || !jsonBody.success || !jsonBody.data) {
          setError(jsonBody.error || `Failed to load issue details (${res.status})`);
          setData(null);
          setLoading(false);
          return;
        }
        setData(jsonBody.data);
        setLoading(false);
      } catch (err) {
        if (cancelled) return;
        setError(err instanceof Error ? err.message : "Unknown error");
        setData(null);
        setLoading(false);
      }
    };
    fetchIssueDetails();
    return () => {
      cancelled = true;
    };
  }, [issueId]);
