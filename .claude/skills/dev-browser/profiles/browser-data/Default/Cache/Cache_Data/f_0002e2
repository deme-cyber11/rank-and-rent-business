  };
  const handleMarkAllRead = async () => {
    try {
      setMarkingAllRead(true);
      const unreadIds = filteredEvents.filter((e) => !e.is_read).map((e) => e.id);
      if (unreadIds.length === 0) return;
      const { error: error2 } = await supabase.from("radar_events").update({ is_read: true, read_at: (/* @__PURE__ */ new Date()).toISOString() }).in("id", unreadIds);
      if (error2) throw error2;
    } catch (err) {
      console.error("Error marking all as read:", err);
