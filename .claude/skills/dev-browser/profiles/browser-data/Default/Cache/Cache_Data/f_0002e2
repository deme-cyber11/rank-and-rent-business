    try {
      const { error: error2 } = await supabase.from("radar_events").update({ is_read: true, read_at: (/* @__PURE__ */ new Date()).toISOString() }).eq("id", eventId);
      if (error2) throw error2;
    } catch (err) {
      console.error("Error marking event as read:", err);
