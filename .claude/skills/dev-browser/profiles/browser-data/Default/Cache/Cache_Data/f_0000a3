const statusVariant = {
  won: "success",
  lost: "danger",
  open: "warning"
};
const statusOrder = {
  open: 0,
  won: 1,
  lost: 2
};
export function QuotesClient({ quotes, warning, source, refreshedAt }) {
  _s();
  const [viewMode, setViewMode] = useState("reps");
  const [selectedRep, setSelectedRep] = useState("all");
  const allReps = useMemo(() => {
    const reps = Array.from(new Set(quotes.map((q) => normalizeRepName(q.rep)))).sort();
    return reps;
  }, [quotes]);
  const filteredQuotes = useMemo(() => {
    if (selectedRep === "all") return quotes;
    return quotes.filter((q) => normalizeRepName(q.rep) === selectedRep);
  }, [quotes, selectedRep]);
  const pipelineValue = filteredQuotes.reduce((sum, quote) => quote.status === "open" ? sum + quote.value : sum, 0);
  const wonValue = filteredQuotes.reduce((sum, quote) => quote.status === "won" ? sum + quote.value : sum, 0);
  const lostValue = filteredQuotes.reduce((sum, quote) => quote.status === "lost" ? sum + quote.value : sum, 0);
  const closedCount = filteredQuotes.filter((quote) => quote.status !== "open").length;
  const winRate = closedCount ? filteredQuotes.filter((quote) => quote.status === "won").length / closedCount : 0;
  const normalizedReps = Array.from(new Set(filteredQuotes.map((quote) => normalizeRepName(quote.rep))));
  const byRep = normalizedReps.map((normalizedName) => {
    const repQuotes = filteredQuotes.filter((quote) => normalizeRepName(quote.rep) === normalizedName);
    const repOpen = repQuotes.filter((quote) => quote.status === "open");
    const repClosed = repQuotes.filter((quote) => quote.status !== "open");
    const repWin = repQuotes.filter((quote) => quote.status === "won");
    const repLost = repQuotes.filter((quote) => quote.status === "lost");
    return {
      name: normalizedName,
      totalValue: repQuotes.reduce((sum, quote) => sum + quote.value, 0),
      openValue: repOpen.reduce((sum, quote) => sum + quote.value, 0),
      wonValue: repWin.reduce((sum, quote) => sum + quote.value, 0),
      lostValue: repLost.reduce((sum, quote) => sum + quote.value, 0),
      winRate: repClosed.length ? repWin.length / repClosed.length : 0,
      activeCount: repOpen.length
    };
  }).sort((a, b) => b.openValue - a.openValue);
  const vendors = Array.from(new Set(filteredQuotes.map((quote) => quote.vendor)));
  const byVendor = vendors.map((vendor) => {
    const vendorQuotes = filteredQuotes.filter((quote) => quote.vendor === vendor);
    const vendorOpen = vendorQuotes.filter((quote) => quote.status === "open");
    const vendorClosed = vendorQuotes.filter((quote) => quote.status !== "open");
    const vendorWin = vendorQuotes.filter((quote) => quote.status === "won");
    const vendorLost = vendorQuotes.filter((quote) => quote.status === "lost");
    return {
      name: vendor,
      totalValue: vendorQuotes.reduce((sum, quote) => sum + quote.value, 0),
      openValue: vendorOpen.reduce((sum, quote) => sum + quote.value, 0),
      wonValue: vendorWin.reduce((sum, quote) => sum + quote.value, 0),
      lostValue: vendorLost.reduce((sum, quote) => sum + quote.value, 0),
      winRate: vendorClosed.length ? vendorWin.length / vendorClosed.length : 0,
      activeCount: vendorOpen.length
    };
  }).sort((a, b) => b.openValue - a.openValue);
  const alerts = [];
  if (source === "sample") {
    alerts.push({
      key: "source",
      message: "Quotes data currently uses sample records. Connect Supabase sheets for live pipeline visibility.",
      variant: "info"
    });
  }
  if (warning) {
    alerts.push({ key: "warning", message: warning, variant: "warning" });
  }
  const currentData = viewMode === "reps" ? byRep : byVendor;
  const currentLabel = viewMode === "reps" ? "Sales Rep" : "Vendor";
  const currentCount = viewMode === "reps" ? normalizedReps.length : vendors.length;
  const quotesWithFollowUps = filteredQuotes.filter((q) => (q.followUpCount ?? 0) > 0);
  const avgFollowUpsPerQuote = filteredQuotes.length > 0 ? filteredQuotes.reduce((sum, q) => sum + (q.followUpCount ?? 0), 0) / filteredQuotes.length : 0;
  const quotesNeedingFollowUp = filteredQuotes.filter((q) => {
    if (q.status !== "open") return false;
    if (!q.nextFollowUp) return true;
    const nextDate = new Date(q.nextFollowUp);
    return nextDate <= /* @__PURE__ */ new Date();
  });
  const vendorWinLoss = vendors.map((vendor) => {
    const vendorQuotes = filteredQuotes.filter((q) => q.vendor === vendor);
    const closedQuotes = vendorQuotes.filter((q) => q.status !== "open");
    const wonQuotes = vendorQuotes.filter((q) => q.status === "won");
    return {
      name: vendor,
      won: wonQuotes.length,
      lost: closedQuotes.length - wonQuotes.length,
      winRate: closedQuotes.length > 0 ? wonQuotes.length / closedQuotes.length : 0,
      totalValue: vendorQuotes.reduce((sum, q) => sum + q.value, 0)
    };
  }).sort((a, b) => b.totalValue - a.totalValue).slice(0, 10);
  const repWinLoss = normalizedReps.map((rep) => {
    const repQuotes = filteredQuotes.filter((q) => normalizeRepName(q.rep) === rep);
    const closedQuotes = repQuotes.filter((q) => q.status !== "open");
    const wonQuotes = repQuotes.filter((q) => q.status === "won");
    return {
      name: rep,
      won: wonQuotes.length,
      lost: closedQuotes.length - wonQuotes.length,
      winRate: closedQuotes.length > 0 ? wonQuotes.length / closedQuotes.length : 0,
      totalValue: repQuotes.reduce((sum, q) => sum + q.value, 0)
    };
  }).sort((a, b) => b.totalValue - a.totalValue).slice(0, 10);
  const lostQuotes = filteredQuotes.filter((q) => q.status === "lost");
  const lossReasonCounts = lostQuotes.reduce((acc, q) => {
    const reason = q.lossReason || "Unknown";
    acc[reason] = (acc[reason] || 0) + 1;
    return acc;
  }, {});
  const lossReasonData = Object.entries(lossReasonCounts).map(([name, value]) => ({ name, value })).sort((a, b) => b.value - a.value);
  const COLORS = ["#3b82f6", "#22d3ee", "#f59e0b", "#8b5cf6", "#10b981", "#ef4444"];
