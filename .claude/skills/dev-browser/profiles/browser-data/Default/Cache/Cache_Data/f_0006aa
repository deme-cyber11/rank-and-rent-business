        case "sku":
          comparison = a.sku.localeCompare(b.sku);
          break;
        case "productTitle":
          comparison = a.productTitle.localeCompare(b.productTitle);
          break;
        case "available":
          comparison = a.available - b.available;
          break;
        case "locationName":
          comparison = a.locationName.localeCompare(b.locationName);
          break;
      }
      return sortDirection === "asc" ? comparison : -comparison;
