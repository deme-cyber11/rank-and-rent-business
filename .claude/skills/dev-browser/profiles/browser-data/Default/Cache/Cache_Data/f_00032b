    /* @__PURE__ */ jsxDEV("div", { className: "p-6 border-b border-gray-700 flex items-center justify-between bg-gray-800/50 rounded-t-xl flex-shrink-0", children: [
      /* @__PURE__ */ jsxDEV("h2", { className: "text-lg font-semibold text-white flex items-center gap-2", children: [
        /* @__PURE__ */ jsxDEV(Flag, { className: "w-5 h-5 text-orange-400" }, void 0, false, {
