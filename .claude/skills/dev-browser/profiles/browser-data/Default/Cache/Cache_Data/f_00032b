import { supabase } from "/src/lib/supabase.ts";
const ISSUE_TYPES = [
  { value: "incorrect_amount", label: "Incorrect Amount", description: "A dollar value is wrong" },
  { value: "wrong_month", label: "Wrong Month", description: "Statement month is incorrect" },
  { value: "missing_data", label: "Missing Data", description: "Data was not extracted" },
  { value: "other", label: "Other Issue", description: "Any other problem" }
];
const FIELD_OPTIONS = [
  { value: "credits", label: "Credits (Total Deposits)" },
  { value: "debits", label: "Debits (Total Withdrawals)" },
  { value: "nsfs", label: "NSF Count" },
  { value: "negative_days", label: "Negative Days" },
  { value: "average_daily_balance", label: "Average Daily Balance" },
  { value: "deposit_count", label: "Deposit Count" },
  { value: "statement_month", label: "Statement Month" },
  { value: "bank_name", label: "Bank Name" },
  { value: "other", label: "Other Field" }
];
export default function ReportStatementIssueModal({
  isOpen,
  onClose,
  statement,
  dealId,
  onSuccess
}) {
  _s();
  const [issueType, setIssueType] = useState("incorrect_amount");
  const [fieldName, setFieldName] = useState("credits");
  const [correctValue, setCorrectValue] = useState("");
  const [description, setDescription] = useState("");
  const [saving, setSaving] = useState(false);
  const [error, setError] = useState(null);
  const [success, setSuccess] = useState(false);
  if (!isOpen) return null;
  const getFieldValue = (field) => {
    switch (field) {
      case "credits":
        return statement.credits ? `$${Math.round(statement.credits).toLocaleString()}` : "N/A";
      case "debits":
        return statement.debits ? `$${Math.round(statement.debits).toLocaleString()}` : "N/A";
      case "nsfs":
        return String(statement.nsfs ?? 0);
      case "negative_days":
        return String(statement.negative_days ?? 0);
      case "average_daily_balance":
        return statement.average_daily_balance ? `$${Math.round(statement.average_daily_balance).toLocaleString()}` : "N/A";
      case "deposit_count":
        return String(statement.deposit_count ?? "N/A");
      case "statement_month":
        return statement.statement_month;
      case "bank_name":
        return statement.bank_name;
      default:
        return "N/A";
    }
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    setError(null);
    if (issueType !== "other" && !fieldName) {
      setError("Please select which field has the issue");
      return;
    }
    if (!description.trim() && !correctValue.trim()) {
      setError("Please provide either a correct value or describe the issue");
      return;
    }
    try {
      setSaving(true);
      const { data: { user } } = await supabase.auth.getUser();
      const { error: insertError } = await supabase.from("statement_feedback").insert({
        deal_id: dealId,
        statement_id: statement.id,
        field_name: fieldName,
        reported_value: getFieldValue(fieldName),
        correct_value: correctValue.trim() || null,
        issue_type: issueType,
        description: description.trim() || null,
        reported_by: user?.id || null,
        status: "pending"
      });
      if (insertError) throw insertError;
      setSuccess(true);
      setTimeout(() => {
        onSuccess?.();
        onClose();
      }, 1500);
    } catch (err) {
      console.error("Error submitting statement feedback:", err);
      setError(err.message || "Failed to submit feedback. Please try again.");
    } finally {
      setSaving(false);
    }
  };
  const handleClose = () => {
    if (!saving) {
      setIssueType("incorrect_amount");
      setFieldName("credits");
      setCorrectValue("");
      setDescription("");
      setError(null);
      setSuccess(false);
      onClose();
    }
  };
