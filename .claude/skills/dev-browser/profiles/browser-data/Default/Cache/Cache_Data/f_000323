export default function AddFundingPositionModal({
  isOpen,
  onClose,
  statements,
  onSuccess
}) {
  _s();
  const [lenderName, setLenderName] = useState("");
  const [amount, setAmount] = useState("");
  const [frequency, setFrequency] = useState("weekly");
  const [status, setStatus] = useState("active");
  const [selectedStatementId, setSelectedStatementId] = useState(
    statements.length > 0 ? statements[0].id : ""
  );
  const [detectedDates, setDetectedDates] = useState([""]);
  const [saving, setSaving] = useState(false);
  const [error, setError] = useState(null);
  if (!isOpen) return null;
  const handleAddDate = () => {
    setDetectedDates([...detectedDates, ""]);
  };
  const handleRemoveDate = (index) => {
    if (detectedDates.length > 1) {
      setDetectedDates(detectedDates.filter((_, i) => i !== index));
    }
  };
  const handleDateChange = (index, value) => {
    const newDates = [...detectedDates];
    newDates[index] = value;
    setDetectedDates(newDates);
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    const parsedAmount = parseFloat(amount);
    if (isNaN(parsedAmount) || parsedAmount <= 0) {
      setError("Please enter a valid amount greater than 0");
      return;
    }
    if (!lenderName.trim()) {
      setError("Please enter a lender name");
      return;
    }
    if (!selectedStatementId) {
      setError("Please select a bank statement");
      return;
    }
    const validDates = detectedDates.filter((d) => d.trim() !== "");
    if (validDates.length === 0) {
      setError("Please enter at least one payment date");
      return;
    }
    try {
      setSaving(true);
      setError(null);
      const { error: insertError } = await supabase.from("deal_funding_positions").insert({
        statement_id: selectedStatementId,
        lender_name: lenderName.trim(),
        amount: parsedAmount,
        frequency,
        status,
        detected_dates: validDates,
        is_manual: true,
        created_at: (/* @__PURE__ */ new Date()).toISOString(),
        updated_at: (/* @__PURE__ */ new Date()).toISOString()
      });
      if (insertError) throw insertError;
      onSuccess();
      onClose();
    } catch (err) {
      console.error("Error adding funding position:", err);
      setError(err.message || "Failed to add funding position");
    } finally {
      setSaving(false);
    }
  };
  const frequencyOptions = ["daily", "weekly", "monthly"];
