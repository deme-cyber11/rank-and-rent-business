import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/components/DebtScheduler/DebtScheduleForm.tsx");import __vite__cjsImport0_react_jsxDevRuntime from "/node_modules/.vite/deps/react_jsx-dev-runtime.js?v=aadb9797"; const jsxDEV = __vite__cjsImport0_react_jsxDevRuntime["jsxDEV"];
var _s = $RefreshSig$();
import __vite__cjsImport1_react from "/node_modules/.vite/deps/react.js?v=aadb9797"; const useState = __vite__cjsImport1_react["useState"]; const useEffect = __vite__cjsImport1_react["useEffect"];
function validateForm(data) {
  const errors = {};
  if (!data.fundedAmount || data.fundedAmount <= 0) {
    errors.fundedAmount = "Funded amount must be greater than 0";
  }
  if (data.scheduleType === "mca") {
    if (!data.factorRate || data.factorRate <= 1) {
      errors.factorRate = "Factor rate must be greater than 1";
    }
  }
  if (data.scheduleType === "term_loan") {
    if (!data.interestRate || data.interestRate <= 0 || data.interestRate >= 100) {
      errors.interestRate = "Interest rate must be between 0 and 100";
    }
  }
  if (!data.paymentAmount || data.paymentAmount <= 0) {
    errors.paymentAmount = "Payment amount must be greater than 0";
  }
  if (!data.firstPaymentDate) {
    errors.firstPaymentDate = "First payment date is required";
  }
  if (!data.termLength || data.termLength <= 0) {
    errors.termLength = "Term length must be greater than 0";
  }
  return errors;
}
export function DebtScheduleForm({
  dealId: _dealId,
  // Reserved for future use (auto-linking schedules)
  initialData,
  lenders = [],
  onSubmit,
  onCancel,
  isLoading = false
}) {
  _s();
  const [formData, setFormData] = useState({
    scheduleType: initialData?.scheduleType || "mca",
    fundedAmount: initialData?.fundedAmount || 0,
    factorRate: initialData?.factorRate || 1.35,
    interestRate: initialData?.interestRate || 12,
    paymentAmount: initialData?.paymentAmount || 0,
    paymentFrequency: initialData?.paymentFrequency || "daily",
    termLength: initialData?.termLength || 0,
    termUnit: initialData?.termUnit || "days",
    firstPaymentDate: initialData?.firstPaymentDate || "",
    lenderId: initialData?.lenderId || "",
    earlyPayoffType: initialData?.earlyPayoffType || "prorated",
    notes: initialData?.notes || ""
  });
  const [errors, setErrors] = useState({});
  const [submitError, setSubmitError] = useState(null);
  useEffect(() => {
    if (formData.scheduleType === "mca" && !formData.factorRate) {
      setFormData((prev) => ({ ...prev, factorRate: 1.35 }));
    }
    if (formData.scheduleType === "term_loan" && !formData.interestRate) {
      setFormData((prev) => ({ ...prev, interestRate: 12 }));
    }
  }, [formData.scheduleType]);
  const handleChange = (field, value) => {
    setFormData((prev) => ({
      ...prev,
      [field]: value
    }));
    if (errors[field]) {
      setErrors((prev) => ({
        ...prev,
        [field]: void 0
      }));
    }
  };
  const handleCurrencyInput = (field, value) => {
    const numericValue = parseFloat(value.replace(/[^0-9.]/g, "")) || 0;
    handleChange(field, numericValue);
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    setSubmitError(null);
    const validationErrors = validateForm(formData);
    if (Object.keys(validationErrors).length > 0) {
      setErrors(validationErrors);
      return;
    }
    try {
      await onSubmit(formData);
    } catch (err) {
      setSubmitError(err instanceof Error ? err.message : "Failed to save debt schedule");
    }
  };
  const inputBaseClass = "w-full px-3 py-2 bg-gray-800/50 border border-gray-700/30 rounded-lg text-white placeholder-gray-500 focus:outline-none focus:ring-2 focus:ring-indigo-500 transition-colors";
