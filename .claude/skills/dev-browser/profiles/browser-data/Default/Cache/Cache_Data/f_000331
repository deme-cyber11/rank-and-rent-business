function findBestScenarioIndex(scenarios) {
  if (scenarios.length === 0) return -1;
  let bestIndex = 0;
  let lowestCost = scenarios[0].scheduleResult.totalPayback;
  for (let i = 1; i < scenarios.length; i++) {
    const cost = scenarios[i].scheduleResult.totalPayback;
    if (cost < lowestCost) {
      lowestCost = cost;
      bestIndex = i;
    }
  }
  return bestIndex;
}
function formatDate(date) {
  return new Intl.DateTimeFormat("en-US", {
    month: "short",
    day: "numeric",
    year: "numeric"
  }).format(date);
}
function getRateDisplay(scenario) {
  if (scenario.formData.factorRate !== void 0) {
    return {
      label: "Factor Rate",
      value: scenario.formData.factorRate.toFixed(2)
    };
  }
  if (scenario.formData.interestRate !== void 0) {
    return {
      label: "Interest Rate",
      value: `${scenario.formData.interestRate.toFixed(1)}%`
    };
  }
  return {
    label: "Rate",
    value: "N/A"
  };
}
