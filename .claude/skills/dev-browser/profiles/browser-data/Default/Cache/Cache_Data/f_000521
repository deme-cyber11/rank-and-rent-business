import { InlineAlert } from "/src/components/ui/inline-alert.tsx?t=1768449300285";
import { useAuth } from "/src/vite/auth/auth-provider.tsx";
export function BlogEditorPage() {
  _s();
  const auth = useAuth();
  const navigate = useNavigate();
  const { id } = useParams();
  const [loading, setLoading] = useState(!!id);
  const [saving, setSaving] = useState(false);
  const [generating, setGenerating] = useState(false);
  const [error, setError] = useState(null);
  const [success, setSuccess] = useState(null);
  const [post, setPost] = useState({
    title: "",
    slug: "",
    content: "",
    excerpt: "",
    featured_image_url: "",
    status: "draft",
    published_at: null,
    scheduled_for: null,
    seo_title: "",
    seo_description: "",
    seo_keywords: [],
    categories: [],
    tags: [],
    ai_generated: false,
    ai_prompt: ""
  });
  const [aiTopic, setAiTopic] = useState("");
  const [aiKeywords, setAiKeywords] = useState("");
  const [aiTone, setAiTone] = useState("professional");
  const [aiLength, setAiLength] = useState("long-form");
  const [seoScore, setSeoScore] = useState(0);
  const [lastSaved, setLastSaved] = useState(null);
  useEffect(() => {
    if (!id) return;
    const token = auth.session?.access_token;
    if (!token) return;
    let cancelled = false;
    setLoading(true);
    fetch(`/api/marketing/blog/posts/${id}`, {
      headers: {
        Authorization: `Bearer ${token}`
      }
    }).then(async (res) => {
      const body = await res.json();
      if (cancelled) return;
      if (!res.ok || !body.success || !body.data) {
        setError(body.error ?? "Failed to load post");
        setLoading(false);
        return;
      }
      setPost(body.data);
      setLoading(false);
    }).catch((e) => {
      if (cancelled) return;
      setError(e instanceof Error ? e.message : "Unknown error");
      setLoading(false);
    });
    return () => {
      cancelled = true;
    };
  }, [id, auth.session?.access_token]);
  useEffect(() => {
    if (!post.title || post.slug) return;
    const slug = post.title.toLowerCase().replace(/[^a-z0-9]+/g, "-").replace(/^-|-$/g, "");
    setPost((prev) => ({ ...prev, slug }));
  }, [post.title]);
  useEffect(() => {
    if (!post.title) return;
    const timer = setTimeout(() => {
      handleAutoSave();
    }, 3e4);
    return () => clearTimeout(timer);
  }, [post]);
  useEffect(() => {
    let score = 0;
    if (post.seo_title.length >= 50 && post.seo_title.length <= 60) score += 20;
    else if (post.seo_title.length > 0) score += 10;
    if (post.seo_description.length >= 150 && post.seo_description.length <= 160) score += 20;
    else if (post.seo_description.length > 0) score += 10;
    if (post.seo_keywords.length >= 3) score += 20;
    else if (post.seo_keywords.length > 0) score += 10;
    const wordCount = post.content.split(/\s+/).length;
    if (wordCount >= 1500) score += 20;
    else if (wordCount >= 800) score += 15;
    else if (wordCount >= 300) score += 10;
    if (post.featured_image_url) score += 20;
    setSeoScore(score);
  }, [post]);
  const handleAutoSave = async () => {
    if (!post.title) return;
    try {
      const token = auth.session?.access_token;
      const url = id ? `/api/marketing/blog/posts/${id}` : "/api/marketing/blog/posts";
      const method = id ? "PUT" : "POST";
      await fetch(url, {
        method,
        headers: {
          Authorization: `Bearer ${token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({ ...post, status: "draft" })
      });
      setLastSaved(/* @__PURE__ */ new Date());
    } catch (e) {
      console.error("Auto-save failed:", e);
    }
  };
  const handleGenerateContent = async () => {
    if (!aiTopic) {
      setError("Please enter a topic to generate content");
      return;
    }
    setGenerating(true);
    setError(null);
    try {
      const token = auth.session?.access_token;
      const res = await fetch("/api/marketing/blog/generate", {
        method: "POST",
        headers: {
          Authorization: `Bearer ${token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({
          topic: aiTopic,
          keywords: aiKeywords.split(",").map((k) => k.trim()),
          tone: aiTone,
          length: aiLength
        })
      });
      const body = await res.json();
      if (!res.ok || !body.success) {
        setError(body.error ?? "Failed to generate content");
        setGenerating(false);
        return;
      }
      setPost((prev) => ({
        ...prev,
        title: body.data.title,
        content: body.data.content,
        excerpt: body.data.excerpt,
        seo_title: body.data.seo_title,
        seo_description: body.data.seo_description,
        seo_keywords: body.data.seo_keywords,
        ai_generated: true,
        ai_prompt: `Topic: ${aiTopic}, Keywords: ${aiKeywords}, Tone: ${aiTone}, Length: ${aiLength}`
      }));
      setSuccess("Content generated successfully! Review and edit as needed.");
      setGenerating(false);
    } catch (e) {
      setError(e instanceof Error ? e.message : "Unknown error");
      setGenerating(false);
    }
  };
  const handleSave = async (publishStatus) => {
    if (!post.title || !post.content) {
      setError("Title and content are required");
      return;
    }
    setSaving(true);
    setError(null);
    try {
      const token = auth.session?.access_token;
      const url = id ? `/api/marketing/blog/posts/${id}` : "/api/marketing/blog/posts";
      const method = id ? "PUT" : "POST";
      const res = await fetch(url, {
        method,
        headers: {
          Authorization: `Bearer ${token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({ ...post, status: publishStatus })
      });
      const body = await res.json();
      if (!res.ok || !body.success) {
        setError(body.error ?? "Failed to save post");
        setSaving(false);
        return;
      }
      setSuccess(
        publishStatus === "published" ? "Post published successfully!" : publishStatus === "scheduled" ? "Post scheduled successfully!" : "Post saved as draft"
      );
      setSaving(false);
      setTimeout(() => {
        navigate("/marketing/blog/dashboard");
      }, 1500);
    } catch (e) {
      setError(e instanceof Error ? e.message : "Unknown error");
      setSaving(false);
    }
  };
  const handleAddKeyword = (keyword) => {
    if (!keyword || post.seo_keywords.includes(keyword)) return;
    setPost((prev) => ({
      ...prev,
      seo_keywords: [...prev.seo_keywords, keyword]
    }));
  };
  const handleRemoveKeyword = (keyword) => {
    setPost((prev) => ({
      ...prev,
      seo_keywords: prev.seo_keywords.filter((k) => k !== keyword)
    }));
  };
