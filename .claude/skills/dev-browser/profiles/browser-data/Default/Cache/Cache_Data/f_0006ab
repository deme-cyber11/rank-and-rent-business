export function ProductsTab({ data }) {
  _s();
  const [viewMode, setViewMode] = useState("revenue");
  const [searchQuery, setSearchQuery] = useState("");
  const products = viewMode === "revenue" ? data.byRevenue : data.byQuantity;
  const totalRevenue = useMemo(
    () => data.byRevenue.reduce((sum, p) => sum + p.revenue, 0),
    [data.byRevenue]
  );
  const totalQuantity = useMemo(
    () => data.byQuantity.reduce((sum, p) => sum + p.quantity, 0),
    [data.byQuantity]
  );
  const filteredProducts = useMemo(() => {
    if (!searchQuery.trim()) return products;
    const query = searchQuery.toLowerCase();
    return products.filter(
      (p) => p.sku.toLowerCase().includes(query) || p.title.toLowerCase().includes(query)
    );
  }, [products, searchQuery]);
  const summaryStats = useMemo(() => {
    const top3Revenue = data.byRevenue.slice(0, 3).reduce((sum, p) => sum + p.revenue, 0);
    const top3Quantity = data.byQuantity.slice(0, 3).reduce((sum, p) => sum + p.quantity, 0);
    return {
      totalProducts: data.byRevenue.length,
      totalRevenue,
      totalQuantity,
      top3RevenueShare: totalRevenue > 0 ? top3Revenue / totalRevenue * 100 : 0,
      top3QuantityShare: totalQuantity > 0 ? top3Quantity / totalQuantity * 100 : 0,
      avgRevenuePerProduct: data.byRevenue.length > 0 ? totalRevenue / data.byRevenue.length : 0
    };
  }, [data, totalRevenue, totalQuantity]);
