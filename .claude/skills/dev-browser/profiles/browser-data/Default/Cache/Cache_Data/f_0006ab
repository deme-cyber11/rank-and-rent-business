function getMedalStyle(rank) {
  switch (rank) {
    case 1:
      return { bg: "bg-yellow-500", text: "text-yellow-500", icon: "text-yellow-500" };
    case 2:
      return { bg: "bg-gray-400", text: "text-gray-400", icon: "text-gray-400" };
    case 3:
      return { bg: "bg-orange-600", text: "text-orange-600", icon: "text-orange-600" };
    default:
      return null;
  }
}
