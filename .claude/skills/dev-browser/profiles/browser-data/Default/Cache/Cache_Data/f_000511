export function EmailDashboardClient({
  overview,
  campaigns,
  flows,
  lists,
  dailyMetrics
}) {
  _s();
  const [activeTab, setActiveTab] = useState("campaigns");
  const [isSyncing, setIsSyncing] = useState(false);
  const [syncMessage, setSyncMessage] = useState(null);
  const [flowStatusFilter, setFlowStatusFilter] = useState("all");
  const alerts = [];
  if (overview.source === "sample") {
    alerts.push({
      key: "source",
      message: "Showing sample Klaviyo data. Connect your Klaviyo API key to see real metrics.",
      variant: "info"
    });
  }
  if (overview.warning) {
    alerts.push({ key: "overview", message: overview.warning, variant: "warning" });
  }
  if (campaigns.warning && campaigns.source !== "sample") {
    alerts.push({ key: "campaigns", message: campaigns.warning, variant: "warning" });
  }
  const totalSubscribers = useMemo(() => {
    return lists.data.reduce((sum, list) => sum + list.subscriber_count, 0);
  }, [lists.data]);
  const filteredFlows = useMemo(() => {
    if (flowStatusFilter === "all") return flows.data;
    return flows.data.filter((flow) => flow.status === flowStatusFilter);
  }, [flows.data, flowStatusFilter]);
  const flowStatuses = useMemo(() => {
    const statuses = new Set(flows.data.map((flow) => flow.status || "unknown"));
    return Array.from(statuses).sort();
  }, [flows.data]);
  const chartData = useMemo(() => {
    return dailyMetrics.data.map((day) => ({
      date: new Date(day.date).toLocaleDateString("en-US", { month: "short", day: "numeric" }),
      "Emails Sent": day.emails_sent,
      Opens: day.opens,
      Clicks: day.clicks
    }));
  }, [dailyMetrics.data]);
  const handleSync = async () => {
    setIsSyncing(true);
    setSyncMessage(null);
    try {
      const response = await fetch("/api/sync/klaviyo", {
        method: "POST",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({ fullSync: false })
      });
      const result = await response.json();
      if (result.success) {
        setSyncMessage({
          type: "success",
          text: `Sync completed: ${result.recordsSynced?.total || 0} records updated`
        });
        setTimeout(() => window.location.reload(), 2e3);
      } else {
        setSyncMessage({
          type: "error",
          text: result.message || "Sync failed. Check console for details."
        });
      }
    } catch (error) {
      setSyncMessage({
        type: "error",
        text: "Failed to start sync. Please try again."
      });
    } finally {
      setIsSyncing(false);
    }
  };
  const formatDate = (dateStr) => {
    if (!dateStr) return "--";
    return new Date(dateStr).toLocaleDateString("en-US", {
      month: "short",
      day: "numeric",
      year: "numeric"
    });
  };
  const formatDateTime = (dateStr) => {
    if (!dateStr) return "--";
    return new Date(dateStr).toLocaleString("en-US", {
      month: "short",
      day: "numeric",
      hour: "numeric",
      minute: "2-digit"
    });
  };
