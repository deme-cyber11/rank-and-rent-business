          /* @__PURE__ */ jsxDEV("label", { htmlFor: "quote-upload", children: /* @__PURE__ */ jsxDEV(
            Button,
            {
              type: "button",
              variant: "outline",
              disabled: uploading,
              className: "cursor-pointer text-base px-6 py-5 h-auto",
              onClick: () => document.getElementById("quote-upload")?.click(),
              children: uploading ? /* @__PURE__ */ jsxDEV(Fragment, { children: [
                /* @__PURE__ */ jsxDEV(Loader2, { className: "h-5 w-5 mr-2 animate-spin" }, void 0, false, {
                  fileName: "C:/Projects/Source 4 Industries/src/pages/sales/quote-calculator/quote-calculator-client.tsx",
