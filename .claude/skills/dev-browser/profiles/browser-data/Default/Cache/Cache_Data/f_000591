import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/pages/sales/quote-calculator/quote-calculator-client.tsx");import __vite__cjsImport0_react_jsxDevRuntime from "/node_modules/.vite/deps/react_jsx-dev-runtime.js?v=9303f522"; const Fragment = __vite__cjsImport0_react_jsxDevRuntime["Fragment"]; const jsxDEV = __vite__cjsImport0_react_jsxDevRuntime["jsxDEV"];
import * as RefreshRuntime from "/@react-refresh";
const inWebWorker = typeof WorkerGlobalScope !== "undefined" && self instanceof WorkerGlobalScope;
let prevRefreshReg;
let prevRefreshSig;
if (import.meta.hot && !inWebWorker) {
  if (!window.$RefreshReg$) {
    throw new Error(
      "@vitejs/plugin-react can't detect preamble. Something is wrong."
    );
  }
  prevRefreshReg = window.$RefreshReg$;
  prevRefreshSig = window.$RefreshSig$;
  window.$RefreshReg$ = RefreshRuntime.getRefreshReg("C:/Projects/Source 4 Industries/src/pages/sales/quote-calculator/quote-calculator-client.tsx");
  window.$RefreshSig$ = RefreshRuntime.createSignatureFunctionForTransform;
}
var _s = $RefreshSig$();
import __vite__cjsImport3_react from "/node_modules/.vite/deps/react.js?v=9303f522"; const useState = __vite__cjsImport3_react["useState"];
import { SectionHeader } from "/src/components/section-header.tsx";
import { Card } from "/src/components/ui/card.tsx";
import { Input } from "/src/components/ui/input.tsx";
import { Button } from "/src/components/ui/button.tsx";
import { formatCurrency, formatPercent } from "/src/lib/utils.ts";
import { Trash2, Plus, Copy, Check, Upload, Loader2 } from "/node_modules/.vite/deps/lucide-react.js?v=9303f522";
const MARGIN_PRESETS = [0.2, 0.25, 0.3, 0.35];
export function QuoteCalculatorClient() {
  _s();
  const [lineItems, setLineItems] = useState(
    [
      {
        id: crypto.randomUUID(),
        vendor: "",
        itemName: "",
        sku: "",
        quantity: 1,
        unitCost: 0,
        msrp: 0,
        margin: 0.25
        // Default 25%
      }
    ]
  );
  const [freightMarkup, setFreightMarkup] = useState(10);
  const [discount, setDiscount] = useState(10);
  const [totalFreight, setTotalFreight] = useState(0);
  const [globalMargin, setGlobalMargin] = useState(25);
  const [copied, setCopied] = useState(false);
  const [uploading, setUploading] = useState(false);
  const [uploadError, setUploadError] = useState(null);
  const addLineItem = () => {
    setLineItems(
      [
        ...lineItems,
        {
          id: crypto.randomUUID(),
          vendor: "",
          itemName: "",
          sku: "",
          quantity: 1,
          unitCost: 0,
          msrp: 0,
          margin: 0.25
        }
      ]
    );
  };
  const removeLineItem = (id) => {
    if (lineItems.length === 1) return;
    setLineItems(lineItems.filter((item) => item.id !== id));
  };
  const updateLineItem = (id, field, value) => {
    setLineItems(
      lineItems.map(
        (item) => item.id === id ? { ...item, [field]: value } : item
      )
    );
  };
  const calculateLineItem = (item) => {
    const vendorAmount = item.unitCost * item.quantity;
    let source4Amount;
    if (item.msrp > 0) {
      source4Amount = item.msrp * item.quantity;
    } else if (globalMargin > 0) {
      source4Amount = vendorAmount / (1 - globalMargin / 100);
    } else {
      source4Amount = vendorAmount;
    }
    const actualMargin = source4Amount > 0 ? (source4Amount - vendorAmount) / source4Amount : 0;
    return {
      vendorAmount,
      source4Amount,
      margin: actualMargin
    };
  };
  const calculateTotals = () => {
    const subtotal = lineItems.reduce((acc, item) => {
      const calc = calculateLineItem(item);
      return {
        vendorSubtotal: acc.vendorSubtotal + calc.vendorAmount,
        source4Subtotal: acc.source4Subtotal + calc.source4Amount
      };
    }, { vendorSubtotal: 0, source4Subtotal: 0 });
    const freightWithMarkup = totalFreight * (1 + freightMarkup / 100);
    const discountAmount = (subtotal.source4Subtotal + freightWithMarkup) * (discount / 100);
    const vendorTotal = subtotal.vendorSubtotal + totalFreight;
    const source4Total = subtotal.source4Subtotal + freightWithMarkup - discountAmount;
    const overallMargin = source4Total > 0 ? (source4Total - vendorTotal) / source4Total : 0;
    return {
      vendorSubtotal: subtotal.vendorSubtotal,
      source4Subtotal: subtotal.source4Subtotal,
      freightWithMarkup,
      discountAmount,
      vendorTotal,
      source4Total,
      overallMargin
    };
  };
  const totals = calculateTotals();
  const handleFileUpload = async (event) => {
    const file = event.target.files?.[0];
    if (!file) return;
    if (file.type !== "application/pdf") {
      setUploadError("Please upload a PDF file");
      return;
    }
    setUploading(true);
    setUploadError(null);
    try {
      const formData = new FormData();
      formData.append("file", file);
      const response = await fetch("/api/extract-quote", {
        method: "POST",
        body: formData
      });
      if (!response.ok) {
        const errorData = await response.json();
        throw new Error(errorData.error || "Failed to extract quote");
      }
      const data = await response.json();
      const newLineItems = data.lineItems.map((item) => ({
        id: crypto.randomUUID(),
        vendor: data.vendorName || "",
        itemName: item.itemName,
        sku: item.sku,
        quantity: item.quantity,
        unitCost: item.unitCost,
        msrp: item.msrp,
        margin: 0.25
        // Default 25%
      }));
      setLineItems(newLineItems);
      if (data.totalFreight) {
        setTotalFreight(data.totalFreight);
      }
      event.target.value = "";
    } catch (error) {
      console.error("Error uploading quote:", error);
      setUploadError(error instanceof Error ? error.message : "Failed to extract quote data");
    } finally {
      setUploading(false);
    }
  };
  const copyToClipboard = () => {
    let text = "QUOTE SUMMARY\n";
    text += "=" + "=".repeat(100) + "\n\n";
    text += `${"Vendor".padEnd(15)} ${"Item Name".padEnd(35)} ${"SKU".padEnd(10)} ${"Qty".padStart(5)} ${"CPU".padStart(12)} ${"Vendor $".padStart(12)} ${"MSRP".padStart(12)} ${"Source 4 $".padStart(12)} ${"Margin".padStart(10)}
`;
    text += "-" + "-".repeat(100) + "\n";
    lineItems.forEach((item) => {
      const calc = calculateLineItem(item);
      text += `${(item.vendor || "").padEnd(15)} ${(item.itemName || "").padEnd(35)} ${(item.sku || "").padEnd(10)} ${String(item.quantity).padStart(5)} ${formatCurrency(item.unitCost).padStart(12)} ${formatCurrency(calc.vendorAmount).padStart(12)} ${formatCurrency(item.msrp || 0).padStart(12)} ${formatCurrency(calc.source4Amount).padStart(12)} ${formatPercent(calc.margin).padStart(10)}
`;
    });
    text += "\n-" + "-".repeat(100) + "\n";
    text += `${"Subtotal".padEnd(82)} ${formatCurrency(totals.vendorSubtotal).padStart(12)} ${formatCurrency(totals.source4Subtotal).padStart(12)}
`;
    text += `${"Freight".padEnd(72)} ${freightMarkup}% ${formatCurrency(totalFreight).padStart(12)} ${formatCurrency(totals.freightWithMarkup).padStart(12)}
`;
    text += `${"Discount".padEnd(72)} ${discount}% ${" ".padStart(27)} -${formatCurrency(totals.discountAmount)}
`;
    text += "-" + "-".repeat(100) + "\n";
    text += `${"Total".padEnd(82)} ${formatCurrency(totals.vendorTotal).padStart(12)} ${formatCurrency(totals.source4Total).padStart(12)} ${formatPercent(totals.overallMargin).padStart(10)}
`;
    navigator.clipboard.writeText(text);
    setCopied(true);
    setTimeout(() => setCopied(false), 2e3);
  };
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV(
