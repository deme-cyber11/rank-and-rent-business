export function BlogCommentsPage() {
  _s();
  const auth = useAuth();
  const [loading, setLoading] = useState(true);
  const [error, setError] = useState(null);
  const [stats, setStats] = useState(null);
  const [comments, setComments] = useState([]);
  const [statusFilter, setStatusFilter] = useState("pending");
  const [searchQuery, setSearchQuery] = useState("");
  const [selectedComments, setSelectedComments] = useState(/* @__PURE__ */ new Set());
  useEffect(() => {
    loadComments();
  }, [auth.session?.access_token]);
  const loadComments = async () => {
    const token = auth.session?.access_token;
    if (!token) return;
    setLoading(true);
    setError(null);
    try {
      const res = await fetch("/api/marketing/blog/comments", {
        headers: {
          Authorization: `Bearer ${token}`
        }
      });
      const body = await res.json();
      if (!res.ok || !body.success || !body.data) {
        setError(body.error ?? "Failed to load comments");
        setStats(null);
        setComments([]);
        setLoading(false);
        return;
      }
      setStats(body.data.stats);
      setComments(body.data.comments);
      setLoading(false);
    } catch (e) {
      setError(e instanceof Error ? e.message : "Unknown error");
      setStats(null);
      setComments([]);
      setLoading(false);
    }
  };
  const handleUpdateStatus = async (commentId, newStatus) => {
    try {
      const token = auth.session?.access_token;
      const res = await fetch(`/api/marketing/blog/comments/${commentId}`, {
        method: "PATCH",
        headers: {
          Authorization: `Bearer ${token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({ status: newStatus })
      });
      const body = await res.json();
      if (!res.ok || !body.success) {
        setError(body.error ?? "Failed to update comment");
        return;
      }
      setComments(
        (prev) => prev.map((c) => c.id === commentId ? { ...c, status: newStatus } : c)
      );
      if (stats) {
        const oldComment = comments.find((c) => c.id === commentId);
        if (oldComment) {
          const newStats = { ...stats };
          newStats[oldComment.status]--;
          newStats[newStatus]++;
          setStats(newStats);
        }
      }
    } catch (e) {
      setError(e instanceof Error ? e.message : "Unknown error");
    }
  };
  const handleBulkAction = async (action) => {
    if (selectedComments.size === 0) return;
    const statusMap = {
      approve: "approved",
      spam: "spam",
      trash: "trash",
      delete: null
    };
    try {
      const token = auth.session?.access_token;
      for (const commentId of selectedComments) {
        if (action === "delete") {
          await fetch(`/api/marketing/blog/comments/${commentId}`, {
            method: "DELETE",
            headers: {
              Authorization: `Bearer ${token}`
            }
          });
        } else {
          await handleUpdateStatus(commentId, statusMap[action]);
        }
      }
      if (action === "delete") {
        setComments((prev) => prev.filter((c) => !selectedComments.has(c.id)));
      }
      setSelectedComments(/* @__PURE__ */ new Set());
      loadComments();
    } catch (e) {
      setError(e instanceof Error ? e.message : "Unknown error");
    }
  };
  const handleToggleSelect = (commentId) => {
    setSelectedComments((prev) => {
      const next = new Set(prev);
      if (next.has(commentId)) {
        next.delete(commentId);
      } else {
        next.add(commentId);
      }
      return next;
    });
  };
  const handleSelectAll = () => {
    if (selectedComments.size === filteredComments.length) {
      setSelectedComments(/* @__PURE__ */ new Set());
    } else {
      setSelectedComments(new Set(filteredComments.map((c) => c.id)));
    }
  };
  const filteredComments = comments.filter((comment) => {
    const matchesStatus = statusFilter === "all" || comment.status === statusFilter;
    const matchesSearch = !searchQuery || comment.author_name.toLowerCase().includes(searchQuery.toLowerCase()) || comment.author_email.toLowerCase().includes(searchQuery.toLowerCase()) || comment.content.toLowerCase().includes(searchQuery.toLowerCase()) || comment.post_title.toLowerCase().includes(searchQuery.toLowerCase());
    return matchesStatus && matchesSearch;
  });
