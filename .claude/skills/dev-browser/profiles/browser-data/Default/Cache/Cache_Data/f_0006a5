const statusVariant = {
  new: "secondary",
  assigned: "secondary",
  contacted: "warning",
  resolved: "success",
  closed: "success"
};
const priorityVariant = {
  low: "secondary",
  medium: "secondary",
  high: "warning",
  urgent: "danger"
};
export function FollowUpDetailPanel({ followUpId, onClose }) {
  _s();
  const auth = useAuth();
  const [loading, setLoading] = useState(false);
  const [error, setError] = useState(null);
  const [data, setData] = useState(null);
  const [newComment, setNewComment] = useState("");
  const [addingComment, setAddingComment] = useState(false);
  const [selectedStatus, setSelectedStatus] = useState("");
  const [updatingStatus, setUpdatingStatus] = useState(false);
  useEffect(() => {
    if (!followUpId) {
      setData(null);
      setError(null);
      return;
    }
    let cancelled = false;
    setLoading(true);
    setError(null);
    const fetchFollowUpDetails = async () => {
      try {
        const token = auth.session?.access_token;
        if (!token) return;
        const res = await fetch(`/api/sales/follow-ups/${followUpId}`, {
          headers: {
            Authorization: `Bearer ${token}`
          }
        });
        const jsonBody = await res.json();
        if (cancelled) return;
        if (!res.ok || !jsonBody.success || !jsonBody.data) {
          setError(jsonBody.error || `Failed to load follow-up details (${res.status})`);
          setData(null);
          setLoading(false);
          return;
        }
        setData(jsonBody.data);
        setSelectedStatus(jsonBody.data.followUp.status);
        setLoading(false);
      } catch (err) {
        if (cancelled) return;
        setError(err instanceof Error ? err.message : "Unknown error");
        setData(null);
        setLoading(false);
      }
    };
    fetchFollowUpDetails();
    return () => {
      cancelled = true;
    };
  }, [followUpId, auth.session?.access_token]);
  const handleAddComment = async () => {
    if (!newComment.trim() || !followUpId) return;
    setAddingComment(true);
    try {
      const token = auth.session?.access_token;
      if (!token) return;
      const res = await fetch(`/api/sales/follow-ups/${followUpId}`, {
        method: "PATCH",
        headers: {
          "Authorization": `Bearer ${token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({
          operation: "add-comment",
          comment: newComment,
          isInternal: false
        })
      });
      const jsonBody = await res.json();
      if (!res.ok || !jsonBody.success) {
        throw new Error(jsonBody.error || "Failed to add comment");
      }
      setNewComment("");
      const refreshRes = await fetch(`/api/sales/follow-ups/${followUpId}`, {
        headers: {
          Authorization: `Bearer ${token}`
        }
      });
      const refreshBody = await refreshRes.json();
      if (refreshRes.ok && refreshBody.success && refreshBody.data) {
        setData(refreshBody.data);
      }
    } catch (err) {
      console.error("Error adding comment:", err);
      alert(err instanceof Error ? err.message : "Failed to add comment");
    } finally {
      setAddingComment(false);
    }
  };
  const handleUpdateStatus = async () => {
    if (!selectedStatus || !followUpId || selectedStatus === data?.followUp.status) return;
    setUpdatingStatus(true);
    try {
      const token = auth.session?.access_token;
      if (!token) return;
      const res = await fetch(`/api/sales/follow-ups/${followUpId}`, {
        method: "PATCH",
        headers: {
          "Authorization": `Bearer ${token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({
          operation: "update-status",
          status: selectedStatus
        })
      });
      const jsonBody = await res.json();
      if (!res.ok || !jsonBody.success) {
        throw new Error(jsonBody.error || "Failed to update status");
      }
      const refreshRes = await fetch(`/api/sales/follow-ups/${followUpId}`, {
        headers: {
          Authorization: `Bearer ${token}`
        }
      });
      const refreshBody = await refreshRes.json();
      if (refreshRes.ok && refreshBody.success && refreshBody.data) {
        setData(refreshBody.data);
      }
    } catch (err) {
      console.error("Error updating status:", err);
      alert(err instanceof Error ? err.message : "Failed to update status");
    } finally {
      setUpdatingStatus(false);
    }
  };
