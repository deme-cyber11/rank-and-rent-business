export function AbandonedCartsPage() {
  _s();
  const auth = useAuth();
  const [carts, setCarts] = useState([]);
  const [loading, setLoading] = useState(true);
  const [meta, setMeta] = useState({});
  const [error, setError] = useState(null);
  useEffect(() => {
    const token = auth.session?.access_token;
    if (!token) return;
    const fetchCarts = async () => {
      try {
        setLoading(true);
        const res = await fetch("/api/sales/abandoned-carts", {
          headers: { Authorization: `Bearer ${token}` }
        });
        const json = await res.json();
        if (json.success) {
          setCarts(json.data || []);
          setMeta(json.meta || {});
        } else {
          setError(json.error || "Failed to fetch carts");
        }
      } catch (err) {
        setError(err.message);
      } finally {
        setLoading(false);
      }
    };
    fetchCarts();
  }, [auth.session]);
  const actionableCarts = carts.filter((cart) => cart.status === "following_up");
  const actionableValue = actionableCarts.reduce((sum, cart) => sum + cart.value, 0);
  const convertedCarts = carts.filter((cart) => cart.status === "converted");
  const convertedValue = convertedCarts.reduce((sum, cart) => sum + cart.value, 0);
  const averageDays = carts.length ? carts.reduce((sum, cart) => sum + cart.daysSinceAbandoned, 0) / carts.length : 0;
  const conversionRate = carts.length ? convertedCarts.length / carts.length : 0;
  const handleStatusChange = async (cartId, newStatus) => {
    try {
      const res = await fetch(`/api/sales/abandoned-carts/${cartId}`, {
        method: "PATCH",
        headers: {
          "Authorization": `Bearer ${auth.session?.access_token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({ status: newStatus })
      });
      const json = await res.json();
      if (json.success) {
        setCarts(carts.map(
          (cart) => cart.id === cartId ? { ...cart, status: newStatus } : cart
        ));
      } else {
        console.error("Failed to update status:", json.error);
      }
    } catch (err) {
      console.error("Error updating status:", err);
    }
  };
  const alerts = [];
  if (meta.source === "sample") {
    alerts.push({ key: "source", message: "Abandoned cart data currently uses sample inputs. Connect Supabase upload to track live recovery progress.", variant: "info" });
  }
  if (meta.warning) {
    alerts.push({ key: "warning", message: meta.warning, variant: "warning" });
  }
  if (error) {
    alerts.push({ key: "error", message: error, variant: "warning" });
  }
  if (loading) {
