function getFinancialStatusBadge(status) {
  const normalized = status.toLowerCase();
  switch (normalized) {
    case "paid":
      return { variant: "success", label: "Paid" };
    case "pending":
    case "authorized":
      return { variant: "warning", label: "Pending" };
    case "refunded":
    case "partially_refunded":
      return { variant: "danger", label: "Refunded" };
    case "voided":
      return { variant: "secondary", label: "Voided" };
    default:
      return { variant: "secondary", label: status };
  }
}
function getFulfillmentStatusBadge(status) {
  const normalized = status?.toLowerCase() || "unfulfilled";
  switch (normalized) {
    case "fulfilled":
      return { variant: "success", label: "Fulfilled" };
    case "partial":
    case "partially_fulfilled":
      return { variant: "warning", label: "Partial" };
    case "unfulfilled":
    case null:
    case "":
      return { variant: "secondary", label: "Unfulfilled" };
    default:
      return { variant: "secondary", label: status || "Unfulfilled" };
  }
}
function formatOrderDate(dateString) {
  const date = new Date(dateString);
  return date.toLocaleDateString("en-US", {
    month: "short",
    day: "numeric",
    year: "numeric",
    hour: "numeric",
    minute: "2-digit"
  });
}
export function OrdersTab({ data, onPageChange }) {
