  const [fulfillmentFilter, setFulfillmentFilter] = useState("all");
  const [searchQuery, setSearchQuery] = useState("");
  const { orders, totalCount, pagination } = data;
  const filteredOrders = useMemo(() => {
    return orders.filter((order) => {
      if (financialFilter !== "all") {
        const normalizedStatus = order.financialStatus.toLowerCase();
        if (financialFilter === "paid" && normalizedStatus !== "paid") return false;
        if (financialFilter === "pending" && !["pending", "authorized"].includes(normalizedStatus)) return false;
        if (financialFilter === "refunded" && !["refunded", "partially_refunded"].includes(normalizedStatus)) return false;
      }
      if (fulfillmentFilter !== "all") {
        const normalizedStatus = (order.fulfillmentStatus || "unfulfilled").toLowerCase();
        if (fulfillmentFilter === "fulfilled" && normalizedStatus !== "fulfilled") return false;
        if (fulfillmentFilter === "unfulfilled" && !["unfulfilled", "", null].includes(normalizedStatus)) return false;
        if (fulfillmentFilter === "partial" && !["partial", "partially_fulfilled"].includes(normalizedStatus)) return false;
      }
      if (searchQuery.trim()) {
        const query = searchQuery.toLowerCase();
        const matchesName = order.customerName.toLowerCase().includes(query);
        const matchesEmail = order.customerEmail.toLowerCase().includes(query);
        if (!matchesName && !matchesEmail) return false;
      }
      return true;
    });
  }, [orders, financialFilter, fulfillmentFilter, searchQuery]);
