export function RollbackDialog({
  open,
  onOpenChange,
  historyRecord,
  onRollback
}) {
  _s();
  const [rollbackStatus, setRollbackStatus] = useState("idle");
  const [rollbackResult, setRollbackResult] = useState(null);
  const formatCurrency = (value) => {
    return new Intl.NumberFormat("en-US", {
      style: "currency",
      currency: "USD",
      minimumFractionDigits: 2,
      maximumFractionDigits: 2
    }).format(value);
  };
  const formatDateTime = (dateString) => {
    const date = new Date(dateString);
    return date.toLocaleDateString("en-US", {
      month: "short",
      day: "numeric",
      year: "numeric"
    }) + " at " + date.toLocaleTimeString("en-US", {
      hour: "numeric",
      minute: "2-digit",
      hour12: true
    });
  };
  const handleRollback = async () => {
    if (!historyRecord) return;
    setRollbackStatus("rolling_back");
    try {
      await onRollback(historyRecord.id);
      setRollbackResult({
        success: true
      });
      setRollbackStatus("success");
    } catch (error) {
      setRollbackResult({
        success: false,
        message: error instanceof Error ? error.message : "Failed to rollback price change"
      });
      setRollbackStatus("error");
    }
  };
  const handleClose = () => {
    setRollbackStatus("idle");
    setRollbackResult(null);
    onOpenChange(false);
  };
