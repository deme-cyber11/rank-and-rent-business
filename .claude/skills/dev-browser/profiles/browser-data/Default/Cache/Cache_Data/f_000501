export function ApplyPricingDialog({
  open,
  onOpenChange,
  records,
  onApply,
  hasErrors = false
}) {
  _s();
  const [applyStatus, setApplyStatus] = useState("idle");
  const [applyResult, setApplyResult] = useState(null);
  const [progress, setProgress] = useState(0);
  const [applyMode, setApplyMode] = useState("now");
  const [scheduledDate, setScheduledDate] = useState("");
  const tomorrow = addDays(startOfDay(/* @__PURE__ */ new Date()), 1);
  const minDateString = format(tomorrow, "yyyy-MM-dd");
  const isScheduledDateValid = applyMode === "now" || scheduledDate && !isBefore(new Date(scheduledDate), tomorrow);
  const summary = {
    totalProducts: records.length,
    priceIncreases: records.filter((r) => r.priceChange > 0).length,
    priceDecreases: records.filter((r) => r.priceChange < 0).length,
    unchanged: records.filter((r) => r.priceChange === 0).length,
    totalIncrease: records.filter((r) => r.priceChange > 0).reduce((sum, r) => sum + r.priceChange, 0),
    totalDecrease: records.filter((r) => r.priceChange < 0).reduce((sum, r) => sum + Math.abs(r.priceChange), 0),
    avgChange: records.length > 0 ? records.reduce((sum, r) => sum + r.priceChange, 0) / records.length : 0
  };
  const handleApply = async () => {
    if (hasErrors || !isScheduledDateValid) return;
    setApplyStatus("applying");
    setProgress(0);
    try {
      const progressInterval = setInterval(() => {
        setProgress((prev) => {
          if (prev >= 90) {
            clearInterval(progressInterval);
            return 90;
          }
          return prev + 10;
        });
      }, 200);
      const effectiveDate = applyMode === "scheduled" ? scheduledDate : void 0;
      await onApply(records, effectiveDate);
      clearInterval(progressInterval);
      setProgress(100);
      setApplyResult({
        success: true,
        applied: records.length,
        failed: 0
      });
      setApplyStatus("success");
    } catch (error) {
      setApplyResult({
        success: false,
        applied: 0,
        failed: records.length,
        message: error instanceof Error ? error.message : "Failed to apply pricing updates"
      });
      setApplyStatus("error");
    }
  };
  const handleClose = () => {
    setApplyStatus("idle");
    setApplyResult(null);
    setProgress(0);
    setApplyMode("now");
    setScheduledDate("");
    onOpenChange(false);
  };
  const formattedScheduledDate = scheduledDate ? format(new Date(scheduledDate), "MMMM d, yyyy") : "";
  const formatCurrency = (value) => {
    return new Intl.NumberFormat("en-US", {
      style: "currency",
      currency: "USD",
      minimumFractionDigits: 2,
      maximumFractionDigits: 2
    }).format(value);
  };
