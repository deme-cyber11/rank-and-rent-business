  const [overdueFilter, setOverdueFilter] = useState("all");
  const [searchQuery, setSearchQuery] = useState("");
  useEffect(() => {
    const fetchEmailStatus = async () => {
      try {
        const token = auth.session?.access_token;
        if (!token) {
          setLoadingEmailStatus(false);
          return;
        }
        const res = await fetch("/api/sales/follow-ups/email-connection", {
          headers: {
            "Authorization": `Bearer ${token}`
          }
        });
        const jsonBody = await res.json();
        if (res.ok && jsonBody.success) {
          setEmailConnectionStatus(jsonBody.data);
        }
      } catch (err) {
        console.error("Error fetching email connection status:", err);
      } finally {
        setLoadingEmailStatus(false);
      }
    };
    fetchEmailStatus();
  }, [auth.session]);
  const handleEmailConnectionChange = async () => {
    setLoadingEmailStatus(true);
