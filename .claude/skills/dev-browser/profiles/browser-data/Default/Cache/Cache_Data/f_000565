        return false;
      }
      if (typeFilter !== "all" && followUp.type !== typeFilter) {
        return false;
      }
      if (assigneeFilter !== "all" && followUp.assignedToName !== assigneeFilter) {
        return false;
      }
      if (priorityFilter !== "all" && followUp.priority !== priorityFilter) {
        return false;
      }
      if (overdueFilter === "overdue" && !followUp.isOverdue) {
        return false;
      } else if (overdueFilter === "not-overdue" && followUp.isOverdue) {
        return false;
      }
      if (searchQuery.trim()) {
        const query = searchQuery.toLowerCase();
        const matchesCustomer = followUp.customerName?.toLowerCase().includes(query);
        const matchesCompany = followUp.companyName?.toLowerCase().includes(query);
        const matchesReason = followUp.reason?.toLowerCase().includes(query);
        return matchesCustomer || matchesCompany || matchesReason;
      }
      return true;
    });
  }, [followUps, statusFilter, typeFilter, assigneeFilter, priorityFilter, overdueFilter, searchQuery]);
