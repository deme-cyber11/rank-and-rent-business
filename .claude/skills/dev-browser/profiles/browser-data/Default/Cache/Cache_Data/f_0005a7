function getCategoryBadgeConfig(category) {
  switch (category) {
    case "product_image":
      return { variant: "default", label: "Product Image" };
    case "spec_sheet":
      return { variant: "success", label: "Spec Sheet" };
    case "warranty":
      return { variant: "warning", label: "Warranty" };
    case "install_guide":
      return { variant: "secondary", label: "Install Guide" };
    case "marketing":
      return { variant: "default", label: "Marketing" };
    case "supplier_doc":
      return { variant: "outline", label: "Supplier Doc" };
    case "reference":
      return { variant: "outline", label: "Reference" };
    case "other":
    default:
      return { variant: "secondary", label: category || "Other" };
  }
}
