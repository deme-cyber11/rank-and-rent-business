const defaultFileFormData = {
  fileName: "",
  category: "other",
  productId: null,
  storagePath: "",
  fileType: null
};
function UploadModal({
  open,
  onOpenChange,
  products,
  onSuccess,
  showToast
}) {
  _s2();
  const [formData, setFormData] = useState(defaultFileFormData);
  const [saving, setSaving] = useState(false);
  const [error, setError] = useState(null);
  useEffect(() => {
    if (open) {
      setFormData(defaultFileFormData);
      setError(null);
    }
  }, [open]);
  const handleChange = (field, value) => {
    setFormData((prev) => ({ ...prev, [field]: value }));
  };
  const validateForm = () => {
    if (!formData.fileName.trim()) {
      return "File name is required";
    }
    if (!formData.storagePath.trim()) {
      return "Storage path is required";
    }
    return null;
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    const validationError = validateForm();
    if (validationError) {
      setError(validationError);
      return;
    }
    setSaving(true);
    setError(null);
    try {
      const res = await fetch("/api/bollards/gameplan/files", {
        method: "POST",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({
          fileName: formData.fileName.trim(),
          category: formData.category,
          productId: formData.productId || null,
          storagePath: formData.storagePath.trim(),
          fileType: formData.fileType,
          source: "manual"
        })
      });
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to create file record");
      }
      showToast("success", "File record created successfully");
      onSuccess();
      onOpenChange(false);
    } catch (err) {
      setError(err instanceof Error ? err.message : "An error occurred");
    } finally {
      setSaving(false);
    }
  };
  return /* @__PURE__ */ jsxDEV(Dialog, { open, onOpenChange, children: /* @__PURE__ */ jsxDEV(DialogContent, { className: "max-w-lg", children: [
    /* @__PURE__ */ jsxDEV(DialogHeader, { children: [
      /* @__PURE__ */ jsxDEV(DialogTitle, { children: "Add File Record" }, void 0, false, {
        fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/FilesTab.tsx",
        lineNumber: 458,
        columnNumber: 11
      }, this),
      /* @__PURE__ */ jsxDEV(DialogDescription, { children: "Create a new file record in the database." }, void 0, false, {
        fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/FilesTab.tsx",
        lineNumber: 459,
        columnNumber: 11
      }, this)
    ] }, void 0, true, {
      fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/FilesTab.tsx",
      lineNumber: 457,
      columnNumber: 9
    }, this),
    /* @__PURE__ */ jsxDEV("form", { onSubmit: handleSubmit, children: [
      error && /* @__PURE__ */ jsxDEV("div", { className: "mb-4 p-3 rounded-md bg-danger/10 border border-danger/30 text-danger text-sm", children: error }, void 0, false, {
        fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/FilesTab.tsx",
        lineNumber: 466,
        columnNumber: 11
      }, this),
      /* @__PURE__ */ jsxDEV("div", { className: "mb-4 p-3 rounded-md bg-warning/10 border border-warning/30 text-warning-foreground text-sm flex items-start gap-2", children: [
        /* @__PURE__ */ jsxDEV(AlertCircle, { className: "h-4 w-4 mt-0.5 flex-shrink-0 text-warning" }, void 0, false, {
