import { createHotContext as __vite__createHotContext } from "/@vite/client";import.meta.hot = __vite__createHotContext("/src/components/profit-loss/excel-upload-form.tsx");import __vite__cjsImport0_react_jsxDevRuntime from "/node_modules/.vite/deps/react_jsx-dev-runtime.js?v=9303f522"; const Fragment = __vite__cjsImport0_react_jsxDevRuntime["Fragment"]; const jsxDEV = __vite__cjsImport0_react_jsxDevRuntime["jsxDEV"];
import * as RefreshRuntime from "/@react-refresh";
const inWebWorker = typeof WorkerGlobalScope !== "undefined" && self instanceof WorkerGlobalScope;
let prevRefreshReg;
let prevRefreshSig;
if (import.meta.hot && !inWebWorker) {
  if (!window.$RefreshReg$) {
    throw new Error(
      "@vitejs/plugin-react can't detect preamble. Something is wrong."
    );
  }
  prevRefreshReg = window.$RefreshReg$;
  prevRefreshSig = window.$RefreshSig$;
  window.$RefreshReg$ = RefreshRuntime.getRefreshReg("C:/Projects/Source 4 Industries/src/components/profit-loss/excel-upload-form.tsx");
  window.$RefreshSig$ = RefreshRuntime.createSignatureFunctionForTransform;
}
var _s = $RefreshSig$();
import __vite__cjsImport3_react from "/node_modules/.vite/deps/react.js?v=9303f522"; const useState = __vite__cjsImport3_react["useState"]; const useRef = __vite__cjsImport3_react["useRef"]; const useEffect = __vite__cjsImport3_react["useEffect"];
import { Upload, FileSpreadsheet, Loader2, Trash2, Calendar } from "/node_modules/.vite/deps/lucide-react.js?v=9303f522";
import { Button } from "/src/components/ui/button.tsx";
import { Input } from "/src/components/ui/input.tsx";
import { Label } from "/src/components/ui/label.tsx";
import { Card, CardHeader, CardTitle, CardDescription } from "/src/components/ui/card.tsx";
import { InlineAlert } from "/src/components/ui/inline-alert.tsx";
import { supabase } from "/src/lib/supabase/client.ts";
import {
  uploadProfitLoss,
  fetchPeriodHistory,
  deletePeriod,
  generatePeriodLabel,
  validatePeriodDates,
  formatCurrency,
  formatPercent
} from "/src/lib/data/profit-loss.ts";
export function ExcelUploadForm({ onUploadSuccess }) {
  _s();
  const [file, setFile] = useState(null);
  const [periodStartDate, setPeriodStartDate] = useState("");
  const [periodEndDate, setPeriodEndDate] = useState("");
  const [periodType, setPeriodType] = useState("monthly");
  const [periodLabel, setPeriodLabel] = useState("");
  const [uploading, setUploading] = useState(false);
  const [uploadResult, setUploadResult] = useState(null);
  const [uploadHistory, setUploadHistory] = useState([]);
  const [loadingHistory, setLoadingHistory] = useState(false);
  const [deletingPeriodId, setDeletingPeriodId] = useState(null);
  const fileInputRef = useRef(null);
  useEffect(() => {
    loadUploadHistory();
  }, []);
  useEffect(() => {
    if (periodStartDate && periodEndDate) {
      const label = generatePeriodLabel(periodStartDate, periodEndDate, periodType);
      setPeriodLabel(label);
    }
  }, [periodStartDate, periodEndDate, periodType]);
  const loadUploadHistory = async () => {
    setLoadingHistory(true);
    try {
      const session = await supabase.auth.getSession();
      const token = session.data.session?.access_token;
      if (!token) {
        console.error("No session token available");
        return;
      }
      const result = await fetchPeriodHistory(token);
      if (result.success && result.data) {
        setUploadHistory(result.data);
      }
    } catch (error) {
      console.error("Failed to load upload history:", error);
    } finally {
      setLoadingHistory(false);
    }
  };
  const handleFileChange = (e) => {
    const selectedFile = e.target.files?.[0];
    if (selectedFile) {
      if (!selectedFile.name.endsWith(".xlsx")) {
        setUploadResult({
          success: false,
          error: "Please select a valid Excel file (.xlsx)"
        });
        return;
      }
      if (selectedFile.size > 10 * 1024 * 1024) {
        setUploadResult({
          success: false,
          error: "File size exceeds 10MB limit"
        });
        return;
      }
      setFile(selectedFile);
      setUploadResult(null);
    }
  };
  const handleUpload = async () => {
    if (!file) {
      setUploadResult({
        success: false,
        error: "Please select a file to upload"
      });
      return;
    }
    if (!periodStartDate || !periodEndDate) {
      setUploadResult({
        success: false,
        error: "Please select start and end dates"
      });
      return;
    }
    if (!periodLabel) {
      setUploadResult({
        success: false,
        error: "Please enter a period label"
      });
      return;
    }
    const dateValidation = validatePeriodDates(periodStartDate, periodEndDate, periodType);
    if (!dateValidation.valid) {
      setUploadResult({
        success: false,
        error: dateValidation.error
      });
      return;
    }
    setUploading(true);
    setUploadResult(null);
    try {
      const session = await supabase.auth.getSession();
      const token = session.data.session?.access_token;
      if (!token) {
        setUploadResult({
          success: false,
          error: "Authentication required. Please log in again."
        });
        return;
      }
      const result = await uploadProfitLoss(
        {
          file,
          periodStartDate,
          periodEndDate,
          periodType,
          periodLabel
        },
        token
      );
      setUploadResult(result);
      if (result.success && result.data) {
        setFile(null);
        setPeriodStartDate("");
        setPeriodEndDate("");
        setPeriodLabel("");
        if (fileInputRef.current) {
          fileInputRef.current.value = "";
        }
        await loadUploadHistory();
        if (onUploadSuccess) {
          onUploadSuccess(result.data.periodId);
        }
      }
    } catch (error) {
      setUploadResult({
        success: false,
        error: error.message || "Upload failed"
      });
    } finally {
      setUploading(false);
    }
  };
  const handleDeletePeriod = async (periodId) => {
    if (!confirm("Are you sure you want to delete this period? This will delete all associated line items and summary data.")) {
      return;
    }
    setDeletingPeriodId(periodId);
    try {
      const session = await supabase.auth.getSession();
      const token = session.data.session?.access_token;
      if (!token) {
        alert("Authentication required. Please log in again.");
        return;
      }
      const result = await deletePeriod(periodId, token);
      if (result.success) {
        await loadUploadHistory();
      } else {
        alert(`Failed to delete period: ${result.error}`);
      }
    } catch (error) {
      alert(`Error: ${error.message}`);
    } finally {
      setDeletingPeriodId(null);
    }
  };
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV(Card, { children: [
