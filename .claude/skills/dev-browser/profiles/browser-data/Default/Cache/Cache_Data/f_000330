export default function EarlyPayoffCalculator({
  schedule,
  payoffType,
  onPayoffTypeChange,
  onPaymentChange
}) {
  _s();
  const [sliderValue, setSliderValue] = useState(0);
  useEffect(() => {
    setSliderValue(0);
  }, [schedule]);
  const totalPayments = schedule?.payments.length || 0;
  const payoffResult = useMemo(() => {
    if (!schedule || totalPayments === 0) return null;
    const validPaymentsMade = Math.min(Math.max(0, sliderValue), totalPayments - 1);
    try {
      return calculateEarlyPayoff(schedule, validPaymentsMade, payoffType);
    } catch (error) {
      console.error("Error calculating early payoff:", error);
      return null;
    }
  }, [schedule, sliderValue, payoffType, totalPayments]);
  const cumulativeStats = useMemo(() => {
    if (!schedule || !schedule.payments.length) {
      return {
        principalPaid: 0,
        interestPaid: 0,
        paymentsMade: 0,
        paymentsRemaining: 0
      };
    }
    let principalPaid = 0;
    let interestPaid = 0;
    for (let i = 0; i < sliderValue && i < schedule.payments.length; i++) {
      principalPaid += schedule.payments[i].principal;
      interestPaid += schedule.payments[i].interest;
    }
    return {
      principalPaid: Math.round(principalPaid * 100) / 100,
      interestPaid: Math.round(interestPaid * 100) / 100,
      paymentsMade: sliderValue,
      paymentsRemaining: totalPayments - sliderValue
    };
  }, [schedule, sliderValue, totalPayments]);
  const handleSliderChange = (e) => {
    const value = parseInt(e.target.value, 10);
    setSliderValue(value);
    onPaymentChange?.(value);
  };
  const handlePayoffTypeChange = (type) => {
    onPayoffTypeChange?.(type);
  };
  const sliderPercentage = totalPayments > 0 ? Math.round(sliderValue / totalPayments * 100) : 0;
  if (!schedule) {
