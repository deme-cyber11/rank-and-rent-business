import { cn } from "/src/lib/utils.ts";
export function SEOPanel({
  title,
  content,
  metaTitle = "",
  metaDescription = "",
  slug = "",
  focusKeyword = "",
  onMetaChange,
  className
}) {
  _s();
  const [seoScore, setSeoScore] = useState({
    overall: 0,
    issues: [],
    readability: 0,
    keywordDensity: 0
  });
  useEffect(() => {
    const calculateScore = () => {
      const issues = [];
      let score = 0;
      if (!title || title.length === 0) {
        issues.push({ type: "error", message: "Post title is required" });
      } else if (title.length < 30) {
        issues.push({ type: "warning", message: "Title is too short (min 30 characters recommended)" });
        score += 10;
      } else if (title.length > 60) {
        issues.push({ type: "warning", message: "Title is too long (max 60 characters recommended)" });
        score += 10;
      } else {
        issues.push({ type: "success", message: "Title length is optimal" });
        score += 20;
      }
      if (!metaTitle) {
        issues.push({ type: "warning", message: "Meta title not set (will default to post title)" });
        score += 10;
      } else if (metaTitle.length > 60) {
        issues.push({ type: "warning", message: "Meta title too long (max 60 characters)" });
        score += 10;
      } else {
        issues.push({ type: "success", message: "Meta title is well optimized" });
        score += 15;
      }
      if (!metaDescription) {
        issues.push({ type: "error", message: "Meta description is missing" });
      } else if (metaDescription.length < 120) {
        issues.push({ type: "warning", message: "Meta description is too short (min 120 characters)" });
        score += 10;
      } else if (metaDescription.length > 160) {
        issues.push({ type: "warning", message: "Meta description is too long (max 160 characters)" });
        score += 10;
      } else {
        issues.push({ type: "success", message: "Meta description is optimal" });
        score += 20;
      }
      if (!slug) {
        issues.push({ type: "error", message: "URL slug is required" });
      } else if (slug.length > 75) {
        issues.push({ type: "warning", message: "URL slug is too long" });
        score += 5;
      } else {
        issues.push({ type: "success", message: "URL slug looks good" });
        score += 15;
      }
      const wordCount = content.split(/\s+/).filter(Boolean).length;
      if (wordCount < 300) {
        issues.push({ type: "warning", message: `Content is short (${wordCount} words). Aim for 800+` });
        score += 5;
      } else if (wordCount < 800) {
        issues.push({ type: "warning", message: `Content could be longer (${wordCount} words). Aim for 800+` });
        score += 10;
      } else {
        issues.push({ type: "success", message: `Content length is good (${wordCount} words)` });
        score += 20;
      }
      if (focusKeyword) {
        const keywordLower = focusKeyword.toLowerCase();
        const contentLower = content.toLowerCase();
        const titleLower = title.toLowerCase();
        if (titleLower.includes(keywordLower)) {
          issues.push({ type: "success", message: "Focus keyword found in title" });
          score += 10;
        } else {
          issues.push({ type: "warning", message: "Focus keyword not in title" });
        }
        const keywordCount = (contentLower.match(new RegExp(keywordLower, "g")) || []).length;
        const density = wordCount > 0 ? keywordCount / wordCount * 100 : 0;
        if (density < 0.5) {
          issues.push({ type: "warning", message: "Focus keyword density is low" });
          score += 5;
        } else if (density > 3) {
          issues.push({ type: "warning", message: "Focus keyword density is too high (keyword stuffing)" });
          score += 5;
        } else {
          issues.push({ type: "success", message: "Focus keyword density is optimal" });
          score += 10;
        }
      }
      const sentences = content.split(/[.!?]+/).filter(Boolean).length;
      const syllables = content.split(/\s+/).length * 1.5;
      const readability = sentences > 0 ? 206.835 - 1.015 * (wordCount / sentences) - 84.6 * (syllables / wordCount) : 0;
      setSeoScore({
        overall: Math.min(100, score),
        issues,
        readability: Math.max(0, Math.min(100, readability)),
        keywordDensity: focusKeyword && wordCount > 0 ? (content.toLowerCase().match(new RegExp(focusKeyword.toLowerCase(), "g")) || []).length / wordCount * 100 : 0
      });
    };
    calculateScore();
  }, [title, content, metaTitle, metaDescription, slug, focusKeyword]);
  const getScoreColor = (score) => {
    if (score >= 80) return "text-success";
    if (score >= 60) return "text-warning";
    return "text-danger";
  };
  const getScoreBadge = (score) => {
    if (score >= 80) return "success";
    if (score >= 60) return "warning";
    return "danger";
  };
  const autoGenerateSlug = () => {
    const generatedSlug = title.toLowerCase().replace(/[^a-z0-9\s-]/g, "").replace(/\s+/g, "-").replace(/-+/g, "-").substring(0, 75);
    onMetaChange("slug", generatedSlug);
  };
  return /* @__PURE__ */ jsxDEV(Card, { className: cn("", className), children: [
    /* @__PURE__ */ jsxDEV(CardHeader, { children: [
      /* @__PURE__ */ jsxDEV("div", { className: "flex items-center justify-between", children: [
