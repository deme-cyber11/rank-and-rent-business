export function MonthlyReviewClient({
  reviewData,
  availableMonths,
  onMonthChange,
  isLoading
}) {
  const { current, averages, percentChanges } = reviewData.data;
  const vendorChartData = current.topVendors.slice(0, 5).map((v) => ({
    name: v.name.length > 15 ? v.name.substring(0, 15) + "..." : v.name,
    revenue: v.revenue,
    profit: v.profit
  }));
  const repChartData = current.topReps.slice(0, 5).map((r) => ({
    name: r.name.length > 15 ? r.name.substring(0, 15) + "..." : r.name,
