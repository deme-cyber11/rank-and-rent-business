const defaultSummary = {
  total: 0,
  byStatus: {
    planned: 0,
    in_progress: 0,
    active: 0,
    paused: 0,
    completed: 0,
    cancelled: 0
  },
  byType: {
    email: 0,
    sms: 0,
    ads: 0,
    content: 0,
    trade_show: 0,
    blog: 0,
    social: 0
  },
  totalBudget: 0,
  activeCampaigns: 0
};
function formatCurrency(value) {
  if (value === null || value === void 0) return "-";
  return new Intl.NumberFormat("en-US", {
    style: "currency",
    currency: "USD",
    minimumFractionDigits: 2,
    maximumFractionDigits: 2
  }).format(value);
}
function formatDate(dateString) {
  if (!dateString) return "-";
  const date = new Date(dateString);
  return date.toLocaleDateString("en-US", {
    month: "short",
    day: "numeric",
    year: "numeric"
  });
}
export function MarketingTab() {
  _s3();
  const [loading, setLoading] = useState(true);
  const [error, setError] = useState(null);
  const [campaigns, setCampaigns] = useState([]);
  const [summary, setSummary] = useState(defaultSummary);
  const [statusFilter, setStatusFilter] = useState("all");
  const [typeFilter, setTypeFilter] = useState("all");
  const [expandedRows, setExpandedRows] = useState(/* @__PURE__ */ new Set());
  const [formModalOpen, setFormModalOpen] = useState(false);
  const [editingCampaign, setEditingCampaign] = useState(null);
  const [deleteDialogOpen, setDeleteDialogOpen] = useState(false);
  const [campaignToDelete, setCampaignToDelete] = useState(null);
  const [deleting, setDeleting] = useState(false);
  const [toast, setToast] = useState({
    show: false,
    type: "success",
    message: ""
  });
  const showToast = (type, message) => {
    setToast({ show: true, type, message });
  };
  const hideToast = () => {
    setToast((prev) => ({ ...prev, show: false }));
  };
  const fetchData = async () => {
    setLoading(true);
    setError(null);
    try {
      const res = await fetch("/api/bollards/gameplan/marketing");
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to load marketing campaigns");
      }
      if (result.data) {
        setCampaigns(result.data.campaigns || []);
        setSummary(result.data.summary || defaultSummary);
      }
    } catch (err) {
      setError(err instanceof Error ? err.message : "Unknown error");
    } finally {
      setLoading(false);
    }
  };
  useEffect(() => {
    fetchData();
  }, []);
  const filteredCampaigns = useMemo(() => {
    let result = campaigns;
    if (statusFilter !== "all") {
      result = result.filter((c) => c.status === statusFilter);
    }
    if (typeFilter !== "all") {
      result = result.filter((c) => c.campaignType === typeFilter);
    }
    return result;
  }, [campaigns, statusFilter, typeFilter]);
  const statusCounts = useMemo(() => {
    const counts = {
      all: campaigns.length,
      planned: 0,
      in_progress: 0,
      active: 0,
      paused: 0,
      completed: 0,
      cancelled: 0
    };
    campaigns.forEach((c) => {
      counts[c.status] = (counts[c.status] || 0) + 1;
    });
    return counts;
  }, [campaigns]);
  const typeCounts = useMemo(() => {
    const counts = {
      all: campaigns.length,
      email: 0,
      sms: 0,
      ads: 0,
      content: 0,
      trade_show: 0,
      blog: 0,
      social: 0
    };
    campaigns.forEach((c) => {
      if (c.campaignType) {
        counts[c.campaignType] = (counts[c.campaignType] || 0) + 1;
      }
    });
    return counts;
  }, [campaigns]);
  const toggleRowExpansion = (campaignId) => {
    setExpandedRows((prev) => {
      const next = new Set(prev);
      if (next.has(campaignId)) {
        next.delete(campaignId);
      } else {
        next.add(campaignId);
      }
      return next;
    });
  };
  const handleAddCampaign = () => {
    setEditingCampaign(null);
    setFormModalOpen(true);
  };
  const handleEditCampaign = (campaign, e) => {
    e?.stopPropagation();
    setEditingCampaign(campaign);
    setFormModalOpen(true);
  };
  const handleDeleteCampaignClick = (campaign, e) => {
    e.stopPropagation();
    setCampaignToDelete(campaign);
    setDeleteDialogOpen(true);
  };
  const handleConfirmDeleteCampaign = async () => {
    if (!campaignToDelete) return;
    setDeleting(true);
    try {
      const res = await fetch(
        `/api/bollards/gameplan/marketing/${campaignToDelete.id}`,
        {
          method: "DELETE"
        }
      );
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to delete campaign");
      }
      showToast("success", "Campaign deleted successfully");
      setDeleteDialogOpen(false);
      setCampaignToDelete(null);
      fetchData();
    } catch (err) {
      showToast(
        "error",
        err instanceof Error ? err.message : "Delete failed"
      );
    } finally {
      setDeleting(false);
    }
