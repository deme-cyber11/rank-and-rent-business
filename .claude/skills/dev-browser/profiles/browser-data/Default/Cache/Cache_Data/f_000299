export function CreateFollowUpDialog({ open, onOpenChange }) {
  _s();
  const auth = useAuth();
  const [creating, setCreating] = useState(false);
  const [formData, setFormData] = useState({
    customerName: "",
    companyName: "",
    contactEmail: "",
    contactPhone: "",
    type: "sales",
    priority: "medium",
    reason: "",
    notes: "",
    dueDate: ""
  });
  const [errors, setErrors] = useState({});
  const handleChange = (field, value) => {
    setFormData((prev) => ({ ...prev, [field]: value }));
    if (errors[field]) {
      setErrors((prev) => {
        const newErrors = { ...prev };
        delete newErrors[field];
        return newErrors;
      });
    }
  };
  const validateForm = () => {
    const newErrors = {};
    if (!formData.customerName.trim()) {
      newErrors.customerName = "Customer name is required";
    }
    if (!formData.reason.trim()) {
      newErrors.reason = "Reason is required";
    }
    if (!formData.dueDate) {
      newErrors.dueDate = "Due date is required";
    }
    if (formData.contactEmail && !formData.contactEmail.match(/^[^\s@]+@[^\s@]+\.[^\s@]+$/)) {
      newErrors.contactEmail = "Invalid email format";
    }
    setErrors(newErrors);
    return Object.keys(newErrors).length === 0;
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    if (!validateForm()) {
      return;
    }
    setCreating(true);
    try {
      const token = auth.session?.access_token;
      if (!token) {
        throw new Error("Not authenticated");
      }
      const res = await fetch("/api/sales/follow-ups", {
        method: "POST",
        headers: {
          "Authorization": `Bearer ${token}`,
          "Content-Type": "application/json"
        },
        body: JSON.stringify({
          customerName: formData.customerName,
          companyName: formData.companyName || void 0,
          contactEmail: formData.contactEmail || void 0,
          contactPhone: formData.contactPhone || void 0,
          type: formData.type,
          priority: formData.priority,
          reason: formData.reason,
          notes: formData.notes || void 0,
          dueDate: formData.dueDate
        })
      });
      const jsonBody = await res.json();
      if (!res.ok || !jsonBody.success) {
        throw new Error(jsonBody.error || "Failed to create follow-up");
      }
      setFormData({
        customerName: "",
        companyName: "",
        contactEmail: "",
        contactPhone: "",
        type: "sales",
        priority: "medium",
        reason: "",
        notes: "",
        dueDate: ""
      });
      onOpenChange(false);
      window.location.reload();
    } catch (err) {
      console.error("Error creating follow-up:", err);
      alert(err instanceof Error ? err.message : "Failed to create follow-up");
    } finally {
      setCreating(false);
    }
  };
  const handleCancel = () => {
    setFormData({
      customerName: "",
      companyName: "",
      contactEmail: "",
      contactPhone: "",
      type: "sales",
      priority: "medium",
      reason: "",
      notes: "",
      dueDate: ""
    });
    setErrors({});
    onOpenChange(false);
  };
