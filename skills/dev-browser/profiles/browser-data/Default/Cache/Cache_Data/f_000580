      if (rep && run.rep !== rep) return;
      if (!matchesRep(run.rep)) return;
      const stats = yearlyStats.get(year);
      stats.homeRuns += 1;
    });
    return Array.from(yearlyStats.entries()).map(([year, stats]) => ({
      year,
      revenue: stats.revenue,
      profit: stats.profit,
      orders: stats.orders,
      margin: stats.revenue > 0 ? stats.profit / stats.revenue : 0,
      homeRuns: stats.homeRuns
