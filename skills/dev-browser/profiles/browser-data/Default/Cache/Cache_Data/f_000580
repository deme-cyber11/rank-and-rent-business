const SALES_DATA_START = /* @__PURE__ */ new Date("2022-11-01T00:00:00.000Z");
const DUMMY_REPS = /* @__PURE__ */ new Set(["Alice Johnson", "Bob Smith", "Carol Davis"]);
const YEAR_SCOPE = [2025, 2024, 2023];
export function RepsDashboardClient({ aggregates, carts, homeRuns }) {
  _s();
  const { timeRange, vendor, rep, customRange } = useDashboardFilters();
  const [selectedReps, setSelectedReps] = useState([]);
  const allReps = useMemo(() => {
    const set = /* @__PURE__ */ new Set();
    aggregates.forEach((agg) => {
      if (agg.rep && !DUMMY_REPS.has(agg.rep)) {
        set.add(agg.rep);
      }
    });
    carts.forEach((cart) => {
      if (cart.rep && !DUMMY_REPS.has(cart.rep)) {
        set.add(cart.rep);
      }
    });
    homeRuns.forEach((run) => {
      if (run.rep && !DUMMY_REPS.has(run.rep)) {
        set.add(run.rep);
      }
    });
    return Array.from(set.values()).sort((a, b) => a.localeCompare(b));
  }, [aggregates, carts, homeRuns]);
  const { start: rangeStart, end: rangeEnd } = useMemo(() => {
    const candidateDates = [
      ...aggregates.map((agg) => parseDate(agg.month)),
      ...carts.map((cart) => parseDate(cart.date || cart.createdAt)),
      ...homeRuns.map((run) => parseDate(run.closedAt))
    ];
    return getRangeBounds(timeRange, candidateDates, customRange);
  }, [customRange, carts, homeRuns, aggregates, timeRange]);
  const matchesRep = (repName) => {
    if (selectedReps.length === 0) return true;
    if (!repName) return false;
    return selectedReps.includes(repName);
  };
  const filteredAggregates = useMemo(() => {
    return aggregates.filter((agg) => {
      const date = parseDate(agg.month);
      if (!date || date < SALES_DATA_START) return false;
      if (!withinRange(date, rangeStart, rangeEnd)) return false;
      if (vendor && agg.vendor !== vendor) return false;
      if (rep && agg.rep !== rep) return false;
      if (!matchesRep(agg.rep)) return false;
      return true;
    });
  }, [aggregates, rangeEnd, rangeStart, vendor, rep, selectedReps]);
  const filteredCarts = useMemo(() => {
    return carts.filter((cart) => {
      const date = parseDate(cart.date || cart.createdAt);
      if (!withinRange(date, rangeStart, rangeEnd)) return false;
      if (vendor && cart.vendor !== vendor) return false;
      if (rep && cart.rep !== rep) return false;
      if (!matchesRep(cart.rep)) return false;
      return true;
    });
  }, [carts, rangeEnd, rangeStart, vendor, rep, selectedReps]);
  const filteredHomeRuns = useMemo(() => {
    return homeRuns.filter((run) => {
      const date = parseDate(run.closedAt);
      if (!withinRange(date, rangeStart, rangeEnd)) return false;
      if (vendor && !run.vendors?.includes(vendor)) return false;
      if (rep && run.rep !== rep) return false;
      if (!matchesRep(run.rep)) return false;
      return true;
    });
  }, [homeRuns, rangeEnd, rangeStart, vendor, rep, selectedReps]);
  const reps = useMemo(() => {
    const set = /* @__PURE__ */ new Set();
    filteredAggregates.forEach((agg) => {
      if (agg.rep && !DUMMY_REPS.has(agg.rep)) {
        set.add(agg.rep);
      }
    });
    filteredCarts.forEach((cart) => {
      if (cart.rep && !DUMMY_REPS.has(cart.rep)) {
        set.add(cart.rep);
      }
    });
    filteredHomeRuns.forEach((run) => {
      if (run.rep && !DUMMY_REPS.has(run.rep)) {
        set.add(run.rep);
      }
    });
    if (rep && !DUMMY_REPS.has(rep)) {
      set.add(rep);
    }
    return Array.from(set.values()).sort((a, b) => a.localeCompare(b));
  }, [filteredCarts, filteredHomeRuns, filteredAggregates, rep]);
  const salesMetricsByRep = useMemo(() => {
    const map = /* @__PURE__ */ new Map();
    filteredAggregates.forEach((agg) => {
      const entry = map.get(agg.rep) ?? {
        revenue: 0,
        profit: 0,
        orders: 0
      };
      entry.revenue += agg.revenue ?? 0;
      entry.profit += agg.profit ?? 0;
      entry.orders += agg.orders > 0 ? agg.orders : agg.orderQuantity > 0 ? agg.orderQuantity : 0;
      map.set(agg.rep, entry);
    });
    return map;
  }, [filteredAggregates]);
  const cartsByRep = useMemo(() => {
    const map = /* @__PURE__ */ new Map();
    filteredCarts.forEach((cart) => {
      if (cart.status === "following_up") {
        const entry = map.get(cart.rep) ?? { actionableValue: 0 };
        entry.actionableValue += cart.value ?? 0;
        map.set(cart.rep, entry);
      }
    });
    return map;
  }, [filteredCarts]);
  const homeRunMetricsByRep = useMemo(() => {
    const map = /* @__PURE__ */ new Map();
    filteredHomeRuns.forEach((run) => {
      const entry = map.get(run.rep) ?? { count: 0, value: 0 };
      entry.count += 1;
      entry.value += run.value ?? 0;
      map.set(run.rep, entry);
    });
    return map;
  }, [filteredHomeRuns]);
  const summaries = useMemo(() => {
    return reps.map((repName) => {
      const salesMetrics = salesMetricsByRep.get(repName) ?? {
        revenue: 0,
        profit: 0,
        orders: 0
      };
      const cartMetrics = cartsByRep.get(repName) ?? { actionableValue: 0 };
      const homeRunMetrics = homeRunMetricsByRep.get(repName) ?? { count: 0, value: 0 };
      const totalRevenue2 = salesMetrics.revenue;
      const totalProfit = salesMetrics.profit;
      const totalOrders = salesMetrics.orders;
      const totalHomeRuns2 = homeRunMetrics.count;
      const margin = totalRevenue2 > 0 ? totalProfit / totalRevenue2 : 0;
      const totalOpportunities = totalHomeRuns2 + totalOrders;
      const winRate = totalOpportunities > 0 ? totalHomeRuns2 / totalOpportunities : 0;
      return {
        rep: repName,
        orders: totalOrders,
        revenue: totalRevenue2,
        profit: totalProfit,
        margin,
        winRate,
        actionableCarts: cartMetrics.actionableValue,
        homeRuns: totalHomeRuns2
      };
    }).sort((a, b) => b.revenue - a.revenue);
  }, [cartsByRep, homeRunMetricsByRep, salesMetricsByRep, reps]);
  const totalRevenue = summaries.reduce((sum, item) => sum + item.revenue, 0);
  const topRep = summaries.slice().sort((a, b) => b.revenue - a.revenue)[0];
  const averageWinRate = summaries.length ? summaries.reduce((sum, item) => sum + item.winRate, 0) / summaries.length : 0;
  const totalHomeRuns = summaries.reduce((sum, item) => sum + item.homeRuns, 0);
