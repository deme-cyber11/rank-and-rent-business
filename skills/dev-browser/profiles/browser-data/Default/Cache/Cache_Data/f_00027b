export function GameplanPage() {
  _s();
  const [loading, setLoading] = useState(true);
  const [error, setError] = useState(null);
  const [data, setData] = useState(null);
  const [activeSection, setActiveSection] = useState("overview");
  const [activeTabId, setActiveTabId] = useState(null);
  const [selectedTaskId, setSelectedTaskId] = useState(null);
  const [uploadModalOpen, setUploadModalOpen] = useState(false);
  const fetchGameplanData = async () => {
    setLoading(true);
    setError(null);
    try {
      const res = await fetch("/api/bollards/gameplan");
      const result = await res.json();
      if (!res.ok || !result.success) {
        setError(result.error || "Failed to load gameplan data");
        setData(null);
        setLoading(false);
        return;
      }
      if (!result.data) {
        setError("No data received");
        setData(null);
        setLoading(false);
        return;
      }
      setData(result.data);
      if (!activeTabId && result.data.tabs.length > 0) {
        const firstTab = [...result.data.tabs].sort((a, b) => a.tabOrder - b.tabOrder)[0];
        setActiveTabId(firstTab.id);
      }
      setLoading(false);
    } catch (err) {
      setError(err instanceof Error ? err.message : "Unknown error");
      setData(null);
      setLoading(false);
    }
  };
  useEffect(() => {
    fetchGameplanData();
  }, []);
  const handleToggleComplete = async (taskId, isCompleted) => {
    try {
      const res = await fetch(`/api/bollards/gameplan/tasks/${taskId}/complete`, {
        method: "PATCH",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({ isCompleted })
      });
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to update task");
      }
      await fetchGameplanData();
    } catch (err) {
      console.error("Error toggling task completion:", err);
      alert(err instanceof Error ? err.message : "Failed to update task");
    }
  };
  const handleTaskClick = (taskId) => {
    setSelectedTaskId(taskId);
  };
  const handleUploadSuccess = () => {
    fetchGameplanData();
  };
  const activeTabTasks = data?.tasks.filter((task) => task.tabId === activeTabId) || [];
  if (!loading && !error && (!data || data.tabs.length === 0)) {
