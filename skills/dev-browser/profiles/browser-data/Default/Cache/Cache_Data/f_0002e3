_c3 = TableView;
function ScorecardView({
  opportunities,
  pipelines,
  users
}) {
  _s3();
  const pipelineStats = useMemo(() => {
    return pipelines.map((pipeline) => {
      const pipelineOpps = opportunities.filter((o) => o.pipeline_id === pipeline.id);
      const totalValue = pipelineOpps.reduce((sum, o) => sum + (o.monetary_value || 0), 0);
      const fundedAmount = pipelineOpps.filter((o) => o.status === "won").reduce((sum, o) => sum + (o.funded_amount || 0), 0);
      const openCount = pipelineOpps.filter((o) => o.status === "open").length;
      const wonCount = pipelineOpps.filter((o) => o.status === "won").length;
      return {
        name: pipeline.name,
        totalDeals: pipelineOpps.length,
        totalValue,
        fundedAmount,
        openCount,
        wonCount
      };
    }).filter((p) => p.totalDeals > 0).sort((a, b) => b.fundedAmount - a.fundedAmount);
  }, [opportunities, pipelines]);
  const repStats = useMemo(() => {
    const repMap = /* @__PURE__ */ new Map();
    opportunities.forEach((opp) => {
      const repId = opp.assigned_to || "unassigned";
      const user = users.find((u) => u.id === repId);
      const repName = user?.name || "Unassigned";
      if (!repMap.has(repId)) {
        repMap.set(repId, { name: repName, deals: 0, value: 0, funded: 0, commission: 0 });
      }
      const rep = repMap.get(repId);
      rep.deals++;
      rep.value += opp.monetary_value || 0;
      if (opp.status === "won") {
        rep.funded += opp.funded_amount || 0;
        rep.commission += opp.commission || 0;
      }
    });
    return Array.from(repMap.values()).sort((a, b) => b.funded - a.funded);
  }, [opportunities, users]);
  const monthlyStats = useMemo(() => {
    const monthMap = /* @__PURE__ */ new Map();
    opportunities.filter((o) => o.status === "won" && o.funding_date).forEach((opp) => {
      const date = new Date(opp.funding_date);
      const monthKey = `${date.getFullYear()}-${String(date.getMonth() + 1).padStart(2, "0")}`;
      const monthLabel = date.toLocaleDateString("en-US", { month: "short", year: "numeric" });
      if (!monthMap.has(monthKey)) {
        monthMap.set(monthKey, { month: monthLabel, deals: 0, funded: 0, commission: 0 });
      }
      const month = monthMap.get(monthKey);
      month.deals++;
      month.funded += opp.funded_amount || 0;
      month.commission += opp.commission || 0;
    });
    return Array.from(monthMap.entries()).sort((a, b) => b[0].localeCompare(a[0])).slice(0, 12).map(([, data]) => data);
  }, [opportunities]);
  return /* @__PURE__ */ jsxDEV("div", { className: "grid grid-cols-1 lg:grid-cols-2 gap-6", children: [
    /* @__PURE__ */ jsxDEV("div", { className: "bg-blue-950/30 rounded-lg shadow-xl border border-blue-800/30 p-6", children: [
      /* @__PURE__ */ jsxDEV("h3", { className: "text-xl font-bold text-blue-300 mb-4", children: "By Pipeline" }, void 0, false, {
        fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/pages/Pipeline.tsx",
        lineNumber: 985,
        columnNumber: 9
      }, this),
      /* @__PURE__ */ jsxDEV("div", { className: "space-y-3", children: [
        pipelineStats.map(
          (pipeline) => /* @__PURE__ */ jsxDEV("div", { className: "flex items-center justify-between py-2 border-b border-blue-900/20 last:border-0", children: [
