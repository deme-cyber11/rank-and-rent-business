function PaperTierView({
  opportunities
}) {
  _s6();
  const tierStats = useMemo(() => {
    const tiers = ["A", "B", "C", "D", "Unclassified"];
    return tiers.map((tier) => {
      const tierOpps = opportunities.filter(
        (o) => tier === "Unclassified" ? !o.paper_tier : o.paper_tier === tier
      );
      const totalCount = tierOpps.length;
      const totalValue = tierOpps.reduce((sum, o) => sum + (o.monetary_value || 0), 0);
      const wonOpps = tierOpps.filter((o) => o.status === "won");
      const fundedAmount = wonOpps.reduce((sum, o) => sum + (o.funded_amount || 0), 0);
      const wonCount = wonOpps.length;
      const conversionRate = totalCount > 0 ? wonCount / totalCount * 100 : 0;
      const avgDealSize = totalCount > 0 ? totalValue / totalCount : 0;
      const colorMap = {
        "A": { bg: "bg-green-600", header: "bg-green-900/50", border: "border-green-700/30", text: "text-green-400", light: "text-green-300" },
        "B": { bg: "bg-blue-600", header: "bg-blue-900/50", border: "border-blue-700/30", text: "text-blue-400", light: "text-blue-300" },
        "C": { bg: "bg-yellow-600", header: "bg-yellow-900/50", border: "border-yellow-700/30", text: "text-yellow-400", light: "text-yellow-300" },
        "D": { bg: "bg-red-600", header: "bg-red-900/50", border: "border-red-700/30", text: "text-red-400", light: "text-red-300" },
        "Unclassified": { bg: "bg-gray-600", header: "bg-gray-900/50", border: "border-gray-700/30", text: "text-gray-400", light: "text-gray-300" }
      };
      return {
        tier,
        totalCount,
        totalValue,
        fundedAmount,
        wonCount,
        conversionRate,
        avgDealSize,
        colors: colorMap[tier]
      };
    }).filter((stat) => stat.totalCount > 0);
  }, [opportunities]);
  const funnelStats = useMemo(() => {
    const tiers = ["A", "B", "C", "D", "Unclassified"];
    return tiers.map((tier) => {
      const tierOpps = opportunities.filter(
        (o) => tier === "Unclassified" ? !o.paper_tier : o.paper_tier === tier
      );
      const submitted = tierOpps.length;
      const approved = tierOpps.filter((o) => o.status === "won" || o.status === "open").length;
      const funded = tierOpps.filter((o) => o.status === "won").length;
      const approvalRate = submitted > 0 ? approved / submitted * 100 : 0;
      const fundingRate = submitted > 0 ? funded / submitted * 100 : 0;
      return {
        tier,
        submitted,
        approved,
        funded,
        approvalRate,
        fundingRate
      };
    }).filter((stat) => stat.submitted > 0);
  }, [opportunities]);
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV("div", { className: "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-4", children: tierStats.map(
      (stat) => /* @__PURE__ */ jsxDEV(
        "div",
        {
          className: `bg-blue-950/30 rounded-lg shadow-xl border ${stat.colors.border} p-6 hover:border-opacity-50 transition-all`,
          children: [
            /* @__PURE__ */ jsxDEV("div", { className: "flex items-center justify-between mb-4", children: [
              /* @__PURE__ */ jsxDEV("div", { className: "flex items-center gap-2", children: [
                /* @__PURE__ */ jsxDEV("div", { className: `w-3 h-3 rounded-full ${stat.colors.bg}` }, void 0, false, {
                  fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/pages/Pipeline.tsx",
                  lineNumber: 1524,
                  columnNumber: 17
                }, this),
                /* @__PURE__ */ jsxDEV("h3", { className: `text-2xl font-bold ${stat.colors.light}`, children: [
                  "Tier ",
