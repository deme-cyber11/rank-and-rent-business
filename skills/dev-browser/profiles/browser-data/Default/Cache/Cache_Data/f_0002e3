  useEffect(() => {
    async function fetchData() {
      setLoading(true);
      setError(null);
      try {
        const { data: pipelineData, error: pipelineError } = await supabase.from("ghl_pipelines").select("*").order("name");
        if (pipelineError) throw pipelineError;
        setPipelines(pipelineData || []);
        const { data: stageData, error: stageError } = await supabase.from("ghl_pipeline_stages").select("*").order("position");
        if (stageError) throw stageError;
        setStages(stageData || []);
        const { data: oppData, error: oppError } = await supabase.from("ghl_opportunities").select("*").order("ghl_created_at", { ascending: false });
        if (oppError) throw oppError;
        setOpportunities(oppData || []);
        const { data: userData, error: userError } = await supabase.from("ghl_users").select("*");
        if (userError) throw userError;
        setUsers(userData || []);
        const ninetyDaysAgo = /* @__PURE__ */ new Date();
        ninetyDaysAgo.setDate(ninetyDaysAgo.getDate() - 90);
        const { data: transitionData, error: transitionError } = await supabase.from("ghl_stage_transitions").select("*").gte("transitioned_at", ninetyDaysAgo.toISOString()).order("transitioned_at", { ascending: false });
        if (!transitionError) {
          setStageTransitions(transitionData || []);
        }
      } catch (err) {
        console.error("Error fetching data:", err);
        setError(err instanceof Error ? err.message : "Failed to load data");
      } finally {
        setLoading(false);
      }
