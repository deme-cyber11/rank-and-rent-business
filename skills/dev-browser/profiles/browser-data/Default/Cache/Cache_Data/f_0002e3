function StuckDealsView({
  opportunities,
  stages: _stages,
  stageTransitions,
  getUserName,
  getStageName
}) {
  _s5();
  const avgTimePerStage = useMemo(() => {
    const timings = {};
    stageTransitions.forEach((t) => {
      if (t.to_stage_id && t.duration_in_previous_stage_hours) {
        if (!timings[t.from_stage_id]) {
          timings[t.from_stage_id] = { totalHours: 0, count: 0 };
        }
        timings[t.from_stage_id].totalHours += t.duration_in_previous_stage_hours;
        timings[t.from_stage_id].count++;
      }
    });
    return Object.entries(timings).reduce((acc, [stageId, data]) => {
      acc[stageId] = data.count > 0 ? data.totalHours / data.count : 168;
      return acc;
    }, {});
  }, [stageTransitions]);
  const stuckDeals = useMemo(() => {
    const now = /* @__PURE__ */ new Date();
    return opportunities.filter((opp) => opp.status === "open" && opp.stage_entered_at).map((opp) => {
      const stageEnteredAt = new Date(opp.stage_entered_at);
      const hoursInStage = (now.getTime() - stageEnteredAt.getTime()) / (1e3 * 60 * 60);
      const avgHours = avgTimePerStage[opp.pipeline_stage_id] || 168;
      const stuckRatio = hoursInStage / avgHours;
      return {
        ...opp,
        hoursInStage,
        avgHours,
        stuckRatio,
        isStuck: stuckRatio > 2
      };
    }).filter((opp) => opp.isStuck).sort((a, b) => b.stuckRatio - a.stuckRatio);
  }, [opportunities, avgTimePerStage]);
  const formatDuration = (hours) => {
    if (hours < 24) return `${Math.round(hours)}h`;
    const days = Math.round(hours / 24);
    if (days < 7) return `${days}d`;
    const weeks = Math.round(days / 7);
    return `${weeks}w`;
  };
  const getSeverity = (ratio) => {
    if (ratio > 5) return { color: "red", label: "Critical" };
    if (ratio > 3) return { color: "orange", label: "High" };
    return { color: "yellow", label: "Medium" };
  };
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV("div", { className: "bg-orange-900/20 rounded-lg shadow-xl border border-orange-700/30 p-6", children: /* @__PURE__ */ jsxDEV("div", { className: "flex items-center justify-between", children: [
      /* @__PURE__ */ jsxDEV("div", { className: "flex items-center gap-3", children: [
        /* @__PURE__ */ jsxDEV("div", { className: "p-3 rounded-lg bg-orange-600", children: /* @__PURE__ */ jsxDEV(AlertTriangle, { className: "w-6 h-6 text-white" }, void 0, false, {
          fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/pages/Pipeline.tsx",
          lineNumber: 1332,
          columnNumber: 15
