        }, this),
        /* @__PURE__ */ jsxDEV("div", { children: [
          /* @__PURE__ */ jsxDEV("h3", { className: "text-xl font-bold text-orange-300", children: "Stuck Deals" }, void 0, false, {
            fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/pages/Pipeline.tsx",
