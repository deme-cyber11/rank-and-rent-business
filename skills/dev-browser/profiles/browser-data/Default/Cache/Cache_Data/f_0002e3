_s3(ScorecardView, "v/wKoxZIjKYE3N7F3IUc55aatWw=");
_c4 = ScorecardView;
function FunnelView({
  opportunities,
  stages,
  selectedPipeline,
  stageTransitions
}) {
  _s4();
  const funnelData = useMemo(() => {
    if (selectedPipeline === "all" || stages.length === 0) {
      const total2 = opportunities.length;
      const open = opportunities.filter((o) => o.status === "open").length;
      const won = opportunities.filter((o) => o.status === "won").length;
      const lost = opportunities.filter((o) => o.status === "lost").length;
      return [
        { name: "Total Leads", count: total2, value: opportunities.reduce((s, o) => s + (o.monetary_value || 0), 0), conversion: 100, color: "blue" },
        { name: "Open / Working", count: open, value: opportunities.filter((o) => o.status === "open").reduce((s, o) => s + (o.monetary_value || 0), 0), conversion: total2 > 0 ? open / total2 * 100 : 0, color: "blue" },
        { name: "Won / Funded", count: won, value: opportunities.filter((o) => o.status === "won").reduce((s, o) => s + (o.funded_amount || 0), 0), conversion: total2 > 0 ? won / total2 * 100 : 0, color: "green" },
        { name: "Lost", count: lost, value: opportunities.filter((o) => o.status === "lost").reduce((s, o) => s + (o.monetary_value || 0), 0), conversion: total2 > 0 ? lost / total2 * 100 : 0, color: "red" }
      ];
    }
    const sortedStages = [...stages].sort((a, b) => a.position - b.position);
    const total = opportunities.length;
    return sortedStages.map((stage, idx) => {
      const stageOpps = opportunities.filter((o) => o.pipeline_stage_id === stage.id);
      const oppsBeyondStage = opportunities.filter((o) => {
        const oppStage = stages.find((s) => s.id === o.pipeline_stage_id);
        return oppStage && oppStage.position >= stage.position;
      });
      return {
        name: stage.name,
        count: stageOpps.length,
        value: stageOpps.reduce((s, o) => s + (o.monetary_value || 0), 0),
        conversion: total > 0 ? oppsBeyondStage.length / total * 100 : 0,
        reachedThisStage: oppsBeyondStage.length,
        color: idx === sortedStages.length - 1 ? "green" : "blue"
      };
    });
  }, [opportunities, stages, selectedPipeline]);
  const stageTimings = useMemo(() => {
    const timings = {};
    stageTransitions.forEach((t) => {
      if (t.from_stage_name && t.duration_in_previous_stage_hours) {
        if (!timings[t.from_stage_name]) {
          timings[t.from_stage_name] = { totalHours: 0, count: 0 };
        }
        timings[t.from_stage_name].totalHours += t.duration_in_previous_stage_hours;
        timings[t.from_stage_name].count++;
      }
    });
    return Object.entries(timings).reduce((acc, [stage, data]) => {
      acc[stage] = data.count > 0 ? Math.round(data.totalHours / data.count) : 0;
      return acc;
    }, {});
  }, [stageTransitions]);
  const formatHours = (hours) => {
    if (hours < 24) return `${hours}h`;
    const days = Math.round(hours / 24);
    return `${days}d`;
  };
  const maxCount = Math.max(...funnelData.map((d) => d.count), 1);
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV("div", { className: "bg-blue-950/30 rounded-lg shadow-xl border border-blue-800/30 p-6", children: [
      /* @__PURE__ */ jsxDEV("h3", { className: "text-xl font-bold text-blue-300 mb-6 flex items-center gap-2", children: [
        /* @__PURE__ */ jsxDEV(Filter, { className: "w-5 h-5" }, void 0, false, {
          fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/pages/Pipeline.tsx",
          lineNumber: 1154,
          columnNumber: 11
        }, this),
        "Conversion Funnel"
      ] }, void 0, true, {
        fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/pages/Pipeline.tsx",
        lineNumber: 1153,
        columnNumber: 9
      }, this),
      /* @__PURE__ */ jsxDEV("div", { className: "space-y-3", children: funnelData.map((stage, idx) => {
        const widthPercent = stage.count / maxCount * 100;
        const colorClasses = {
          blue: "bg-blue-600",
          green: "bg-green-600",
          red: "bg-red-600"
        };
        const avgTime = stageTimings[stage.name];
        return /* @__PURE__ */ jsxDEV("div", { className: "relative", children: [
          /* @__PURE__ */ jsxDEV("div", { className: "flex items-center gap-4", children: [
            /* @__PURE__ */ jsxDEV("div", { className: "w-40 flex-shrink-0", children: [
              /* @__PURE__ */ jsxDEV("p", { className: "text-white font-medium text-sm", children: stage.name }, void 0, false, {
                fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/pages/Pipeline.tsx",
