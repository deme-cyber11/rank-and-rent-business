  };
  const getStageName = (stageId) => {
    const stage = stages.find((s) => s.id === stageId);
    return stage?.name || "Unknown";
  };
  const metrics = useMemo(() => {
    const filtered = filteredOpportunities;
    const totalDeals = filtered.length;
    const totalValue = filtered.reduce((sum, o) => sum + (o.monetary_value || 0), 0);
    const fundedDeals = filtered.filter((o) => o.status === "won");
    const totalFunded = fundedDeals.reduce((sum, o) => sum + (o.funded_amount || 0), 0);
    const totalCommission = fundedDeals.reduce((sum, o) => sum + (o.commission || 0), 0);
    const openDeals = filtered.filter((o) => o.status === "open").length;
    const wonDeals = fundedDeals.length;
    const lostDeals = filtered.filter((o) => o.status === "lost").length;
    const winRate = totalDeals > 0 ? wonDeals / (wonDeals + lostDeals) * 100 || 0 : 0;
    return {
      totalDeals,
      totalValue,
      totalFunded,
      totalCommission,
      openDeals,
      wonDeals,
      lostDeals,
      winRate
    };
  }, [filteredOpportunities]);
  const pipelineStages = useMemo(() => {
    if (selectedPipeline === "all") {
      return stages;
