export function calculateDealScore(deal, statements) {
  let score = 100;
  const sortedStatements = [...statements].filter((s) => s.credits !== null).sort((a, b) => b.statement_month.localeCompare(a.statement_month));
  const last3Statements = sortedStatements.slice(0, 3);
  const avgMonthlyRevenue = last3Statements.length > 0 ? last3Statements.reduce((sum, s) => sum + (s.credits || 0), 0) / last3Statements.length : deal.average_monthly_sales || 0;
  const requestedAmount = deal.desired_loan_amount || 0;
  const revenueRatio = requestedAmount / (avgMonthlyRevenue || 1);
  if (revenueRatio > 2) {
    score -= 30;
  } else if (revenueRatio > 1.5) {
    score -= 15;
  } else if (revenueRatio > 1) {
    score -= 5;
  }
  const months = deal.time_in_business_months || 0;
  if (months < 6) {
    score -= 30;
  } else if (months < 12) {
    score -= 15;
  } else if (months < 24) {
    score -= 5;
  }
  const totalNSFs = statements.reduce((sum, s) => sum + (s.nsfs || 0), 0);
  if (totalNSFs > 5) {
    score -= 25;
  } else if (totalNSFs > 2) {
    score -= 15;
  } else if (totalNSFs > 0) {
    score -= 5;
  }
  const totalNegDays = statements.reduce((sum, s) => sum + (s.negative_days || 0), 0);
  if (totalNegDays > 10) {
    score -= 20;
  } else if (totalNegDays > 5) {
    score -= 10;
  } else if (totalNegDays > 0) {
    score -= 5;
  }
  const avgBalance = statements.length > 0 ? statements.reduce((sum, s) => sum + (s.average_daily_balance || 0), 0) / statements.length : null;
  if (avgBalance && requestedAmount > 0) {
    const balanceRatio = avgBalance / requestedAmount;
    if (balanceRatio >= 0.5) {
      score += 5;
    }
  }
  return scoreToGrade(score);
}
function scoreToGrade(score) {
  if (score >= 85) return "A";
  if (score >= 70) return "B";
  if (score >= 55) return "C";
  if (score >= 40) return "D";
  return "F";
}
export function getEffectiveScore(deal) {
  if (deal.score) {
    return deal.score;
  }
  return deal.score_auto;
}
export function isScoreOverridden(deal) {
  return !!(deal.score && deal.score_override_by);
}
export function getScoreColor(score) {
  switch (score) {
    case "A":
      return {
        bg: "bg-green-500/20",
        text: "text-green-400",
        border: "border-green-500/30"
      };
    case "B":
      return {
        bg: "bg-blue-500/20",
        text: "text-blue-400",
        border: "border-blue-500/30"
      };
    case "C":
      return {
        bg: "bg-yellow-500/20",
        text: "text-yellow-400",
        border: "border-yellow-500/30"
      };
    case "D":
      return {
        bg: "bg-orange-500/20",
        text: "text-orange-400",
        border: "border-orange-500/30"
      };
    case "F":
      return {
        bg: "bg-red-500/20",
        text: "text-red-400",
        border: "border-red-500/30"
      };
    default:
      return {
        bg: "bg-gray-500/20",
        text: "text-gray-400",
        border: "border-gray-500/30"
      };
  }
}
export function getScoringFactors(deal, statements) {
  const sortedStatements = [...statements].filter((s) => s.credits !== null).sort((a, b) => b.statement_month.localeCompare(a.statement_month));
  const last3Statements = sortedStatements.slice(0, 3);
  const avgMonthlyRevenue = last3Statements.length > 0 ? last3Statements.reduce((sum, s) => sum + (s.credits || 0), 0) / last3Statements.length : deal.average_monthly_sales || 0;
  const requestedAmount = deal.desired_loan_amount || 0;
  return {
    revenueRatio: requestedAmount / (avgMonthlyRevenue || 1),
    timeInBusiness: deal.time_in_business_months || 0,
    totalNSFs: statements.reduce((sum, s) => sum + (s.nsfs || 0), 0),
    totalNegativeDays: statements.reduce((sum, s) => sum + (s.negative_days || 0), 0),
    avgDailyBalance: statements.length > 0 ? statements.reduce((sum, s) => sum + (s.average_daily_balance || 0), 0) / statements.length : null
  };
}

