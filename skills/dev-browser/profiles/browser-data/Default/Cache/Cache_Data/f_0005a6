export function FAQsTab() {
  _s3();
  const [loading, setLoading] = useState(true);
  const [error, setError] = useState(null);
  const [faqs, setFaqs] = useState([]);
  const [summary, setSummary] = useState(defaultSummary);
  const [categoryFilter, setCategoryFilter] = useState("all");
  const [expandedRows, setExpandedRows] = useState(/* @__PURE__ */ new Set());
  const [faqModalOpen, setFaqModalOpen] = useState(false);
  const [editingFaq, setEditingFaq] = useState(null);
  const [deleteDialogOpen, setDeleteDialogOpen] = useState(false);
  const [faqToDelete, setFaqToDelete] = useState(null);
  const [deleting, setDeleting] = useState(false);
  const [exporting, setExporting] = useState(false);
  const [toast, setToast] = useState({
    show: false,
    type: "success",
    message: ""
  });
  const showToast = (type, message) => {
    setToast({ show: true, type, message });
  };
  const hideToast = () => {
    setToast((prev) => ({ ...prev, show: false }));
  };
  const fetchData = async () => {
    setLoading(true);
    setError(null);
    try {
      const res = await fetch("/api/bollards/gameplan/faqs");
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to load FAQs");
      }
      if (result.data) {
        setFaqs(result.data.faqs || []);
        setSummary(result.data.summary || defaultSummary);
      }
    } catch (err) {
      setError(err instanceof Error ? err.message : "Unknown error");
    } finally {
      setLoading(false);
    }
  };
  useEffect(() => {
    fetchData();
  }, []);
  const filteredFaqs = useMemo(() => {
    if (categoryFilter === "all") return faqs;
    return faqs.filter((faq) => faq.category === categoryFilter);
  }, [faqs, categoryFilter]);
  const filterCounts = useMemo(() => {
    const counts = {
      all: faqs.length,
      pre_purchase: 0,
      installation: 0,
      maintenance: 0,
      warranty: 0
    };
    faqs.forEach((faq) => {
      if (faq.category) {
        counts[faq.category] = (counts[faq.category] || 0) + 1;
      }
    });
    return counts;
  }, [faqs]);
  const toggleRowExpansion = (faqId) => {
    setExpandedRows((prev) => {
      const next = new Set(prev);
      if (next.has(faqId)) {
        next.delete(faqId);
      } else {
        next.add(faqId);
      }
      return next;
    });
  };
  const handleAddFaq = () => {
    setEditingFaq(null);
    setFaqModalOpen(true);
  };
  const handleEditFaq = (faq, e) => {
    e?.stopPropagation();
    setEditingFaq(faq);
    setFaqModalOpen(true);
  };
  const handleDeleteFaqClick = (faq, e) => {
    e.stopPropagation();
    setFaqToDelete(faq);
    setDeleteDialogOpen(true);
  };
  const handleConfirmDeleteFaq = async () => {
    if (!faqToDelete) return;
    setDeleting(true);
    try {
      const res = await fetch(
        `/api/bollards/gameplan/faqs/${faqToDelete.id}`,
        {
          method: "DELETE"
        }
      );
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to delete FAQ");
      }
      showToast("success", "FAQ deleted successfully");
      setDeleteDialogOpen(false);
      setFaqToDelete(null);
      fetchData();
    } catch (err) {
      showToast(
        "error",
        err instanceof Error ? err.message : "Delete failed"
      );
    } finally {
      setDeleting(false);
    }
  };
  const handleExportToShopify = async () => {
    setExporting(true);
    try {
      const res = await fetch("/api/bollards/gameplan/faqs/export");
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to export FAQs");
      }
      showToast("success", "FAQs exported to Shopify successfully");
    } catch (err) {
      showToast(
        "error",
        err instanceof Error ? err.message : "Export failed"
      );
    } finally {
      setExporting(false);
    }
  };
