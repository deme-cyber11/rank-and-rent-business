export default function PaidAdsDashboardClient({
  platformSummary,
  platformComparison
}) {
  _s();
  const [platformFilter, setPlatformFilter] = useState("all");
  const filteredMetrics = useMemo(() => {
    const data = platformSummary.data || [];
    if (platformFilter === "google") {
      const googleData = data.filter((d) => d.platform === "Google");
      return {
        totalAdSpend: googleData.reduce((sum, d) => sum + d.totalAdSpend, 0),
        totalImpressions: googleData.reduce((sum, d) => sum + d.totalImpressions, 0),
        totalClicks: googleData.reduce((sum, d) => sum + d.totalClicks, 0),
        totalConversions: googleData.reduce((sum, d) => sum + d.totalConversions, 0),
        totalRevenue: googleData.reduce((sum, d) => sum + d.totalRevenue, 0),
        uniqueSkus: Math.max(...googleData.map((d) => d.uniqueSkus), 0),
        uniqueVendors: Math.max(...googleData.map((d) => d.uniqueVendors), 0)
      };
    }
    if (platformFilter === "bing") {
      const bingData = data.filter((d) => d.platform === "Bing");
      return {
        totalAdSpend: bingData.reduce((sum, d) => sum + d.totalAdSpend, 0),
        totalImpressions: bingData.reduce((sum, d) => sum + d.totalImpressions, 0),
        totalClicks: bingData.reduce((sum, d) => sum + d.totalClicks, 0),
        totalConversions: bingData.reduce((sum, d) => sum + d.totalConversions, 0),
        totalRevenue: bingData.reduce((sum, d) => sum + d.totalRevenue, 0),
        uniqueSkus: Math.max(...bingData.map((d) => d.uniqueSkus), 0),
        uniqueVendors: Math.max(...bingData.map((d) => d.uniqueVendors), 0)
      };
    }
    return {
      totalAdSpend: data.reduce((sum, d) => sum + d.totalAdSpend, 0),
      totalImpressions: data.reduce((sum, d) => sum + d.totalImpressions, 0),
      totalClicks: data.reduce((sum, d) => sum + d.totalClicks, 0),
      totalConversions: data.reduce((sum, d) => sum + d.totalConversions, 0),
      totalRevenue: data.reduce((sum, d) => sum + d.totalRevenue, 0),
      uniqueSkus: Math.max(...data.map((d) => d.uniqueSkus), 0),
      uniqueVendors: Math.max(...data.map((d) => d.uniqueVendors), 0)
    };
  }, [platformSummary.data, platformFilter]);
  const ctr = filteredMetrics.totalImpressions > 0 ? filteredMetrics.totalClicks / filteredMetrics.totalImpressions : 0;
  const avgCpc = filteredMetrics.totalClicks > 0 ? filteredMetrics.totalAdSpend / filteredMetrics.totalClicks : 0;
  const roas = filteredMetrics.totalAdSpend > 0 ? filteredMetrics.totalRevenue / filteredMetrics.totalAdSpend : 0;
  const chartData = useMemo(() => {
    const comparison = platformComparison.data || [];
    const sorted = [...comparison].sort((a, b) => a.month.localeCompare(b.month));
    return sorted.map((row) => {
      const [year, month] = row.month.split("-");
      const monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
      const monthName = monthNames[parseInt(month, 10) - 1] || month;
      const shortYear = year.slice(-2);
      let adSpend = 0;
      let googleSpend = row.googleAdSpend;
      let bingSpend = row.bingAdSpend;
      if (platformFilter === "all") {
        adSpend = row.googleAdSpend + row.bingAdSpend;
      } else if (platformFilter === "google") {
        adSpend = row.googleAdSpend;
        bingSpend = 0;
      } else {
        adSpend = row.bingAdSpend;
        googleSpend = 0;
      }
      return {
        month: `${monthName} '${shortYear}`,
        rawMonth: row.month,
        adSpend,
        googleSpend,
        bingSpend,
        googleImpressions: row.googleImpressions,
        bingImpressions: row.bingImpressions,
        googleClicks: row.googleClicks,
        bingClicks: row.bingClicks,
        googleRevenue: row.googleRevenue,
        bingRevenue: row.bingRevenue
      };
    });
  }, [platformComparison.data, platformFilter]);
  const displayChartData = chartData.slice(-12);
  const maxValue = useMemo(() => {
    if (!displayChartData.length) return 5e4;
    let max;
    if (platformFilter === "all") {
      max = Math.max(...displayChartData.map((d) => d.googleSpend + d.bingSpend));
    } else if (platformFilter === "google") {
      max = Math.max(...displayChartData.map((d) => d.googleSpend));
    } else {
      max = Math.max(...displayChartData.map((d) => d.bingSpend));
    }
    const roundTo = max < 1e4 ? 1e3 : max < 1e5 ? 1e4 : 1e5;
    return Math.ceil(max * 1.1 / roundTo) * roundTo;
  }, [displayChartData, platformFilter]);
  const yAxisTicks = useMemo(() => {
    const tickCount = 6;
    const step = maxValue / (tickCount - 1);
    return Array.from({ length: tickCount }, (_, i) => Math.round(i * step));
  }, [maxValue]);
  const monthlyTableData = useMemo(() => {
    const comparison = platformComparison.data || [];
    return [...comparison].sort((a, b) => b.month.localeCompare(a.month)).slice(0, 12);
  }, [platformComparison.data]);
