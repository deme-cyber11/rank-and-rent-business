  const [selectedMonth, setSelectedMonth] = useState(reviewData.data.current.month);
  const [compareMonth, setCompareMonth] = useState(null);
  const { current, averages, percentChanges } = reviewData.data;
  const handleMonthChange = (month) => {
    setSelectedMonth(month);
    startTransition(() => {
      navigate(`/sales/monthly-review?month=${encodeURIComponent(month)}`);
    });
  };
  const handleCompareChange = (month) => {
    setCompareMonth(month);
  };
  const vendorChartData = current.topVendors.slice(0, 5).map((v) => ({
