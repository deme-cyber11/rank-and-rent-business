import { saveMultipleToLibrary, setLikedStatus, setDislikedStatus, saveDraft } from "/src/services/contentLibraryService.ts";
const PERSONA_PLATFORMS = {
  "Zac": ["LinkedIn", "Instagram", "Facebook", "Twitter"],
  "Luke": ["LinkedIn", "Instagram", "Facebook"],
  "Huge Capital": ["Facebook", "Blog", "Newsletter"]
};
export const BatchContentGenerator = ({
  selectedPerson,
  onContentGenerated,
  onAddToCalendar,
  persistedResult,
  onResultChange,
  persistedPostIndex,
  onPostIndexChange,
  persistedScheduledPosts,
  onScheduledPostsChange
}) => {
  _s();
  const [isGenerating, setIsGenerating] = useState(false);
  const [progress, setProgress] = useState({ completed: 0, total: 0 });
  const [localResult, setLocalResult] = useState(null);
  const result = persistedResult !== void 0 ? persistedResult : localResult;
  const setResult = (newResult) => {
    if (onResultChange) {
      onResultChange(newResult);
    } else {
      setLocalResult(newResult);
    }
  };
  const [localPostIndex, setLocalPostIndex] = useState(0);
  const currentPostIndex = persistedPostIndex !== void 0 ? persistedPostIndex : localPostIndex;
  const setCurrentPostIndex = (index) => {
    if (onPostIndexChange) {
      onPostIndexChange(index);
    } else {
      setLocalPostIndex(index);
    }
  };
  const [localScheduledPosts, setLocalScheduledPosts] = useState(/* @__PURE__ */ new Set());
  const scheduledPosts = persistedScheduledPosts !== void 0 ? persistedScheduledPosts : localScheduledPosts;
  const setScheduledPosts = (postsOrUpdater) => {
    if (typeof postsOrUpdater === "function") {
      const newPosts = postsOrUpdater(scheduledPosts);
      if (onScheduledPostsChange) {
        onScheduledPostsChange(newPosts);
      } else {
        setLocalScheduledPosts(newPosts);
      }
    } else {
      if (onScheduledPostsChange) {
        onScheduledPostsChange(postsOrUpdater);
      } else {
        setLocalScheduledPosts(postsOrUpdater);
      }
    }
  };
  const [copiedIndex, setCopiedIndex] = useState(null);
  const [likedPosts, setLikedPosts] = useState(/* @__PURE__ */ new Set());
  const [dislikedPosts, setDislikedPosts] = useState(/* @__PURE__ */ new Set());
  const [draftedPosts, setDraftedPosts] = useState(/* @__PURE__ */ new Set());
  const [regeneratingIndex, setRegeneratingIndex] = useState(null);
  const [libraryIds, setLibraryIds] = useState([]);
  const [isEditing, setIsEditing] = useState(false);
  const [editedContent, setEditedContent] = useState("");
  const [selectedPlatforms, setSelectedPlatforms] = useState([]);
  const [postsPerPlatform, setPostsPerPlatform] = useState(5);
  const [selectedPillars, setSelectedPillars] = useState([]);
  const [showSchedulingModal, setShowSchedulingModal] = useState(false);
  const [contentToSchedule, setContentToSchedule] = useState(null);
  const availablePlatforms = selectedPerson ? PERSONA_PLATFORMS[selectedPerson] : [];
  const personaConfig = selectedPerson ? SkillsRunner.personaConfig[selectedPerson] : { color: "#6366F1", voiceMarkers: [], style: {} };
  useEffect(() => {
    setSelectedPlatforms([]);
  }, [selectedPerson]);
  const startEditing = () => {
    if (result && result.contents[currentPostIndex]) {
      setEditedContent(result.contents[currentPostIndex].content);
      setIsEditing(true);
    }
  };
  const saveEdits = () => {
    if (result && editedContent) {
      const newContents = [...result.contents];
      newContents[currentPostIndex] = {
        ...newContents[currentPostIndex],
        content: editedContent
      };
      setResult({ ...result, contents: newContents });
      setIsEditing(false);
    }
  };
  const cancelEditing = () => {
    setIsEditing(false);
    setEditedContent("");
  };
  const handleSaveDraft = () => {
    if (result && result.contents[currentPostIndex]) {
      const currentContent = result.contents[currentPostIndex];
      const contentText = isEditing ? editedContent : currentContent.content;
      saveDraft(
        contentText,
        currentContent.metadata.persona,
        currentContent.metadata.platform
      );
      setDraftedPosts((prev) => new Set(prev).add(currentPostIndex));
      console.log("[BatchGenerator] Saved draft for post", currentPostIndex + 1);
    }
  };
  const togglePlatform = (platform) => {
    setSelectedPlatforms(
      (prev) => prev.includes(platform) ? prev.filter((p) => p !== platform) : [...prev, platform]
    );
  };
  const selectAllPlatforms = () => {
    setSelectedPlatforms(availablePlatforms);
  };
  const clearAllPlatforms = () => {
    setSelectedPlatforms([]);
  };
  const togglePillar = (pillarName) => {
    setSelectedPillars((prev) => {
      if (prev.includes(pillarName)) {
        return prev.filter((p) => p !== pillarName);
      } else if (prev.length < 3) {
        return [...prev, pillarName];
      }
      return prev;
    });
  };
  const availablePillars = selectedPerson ? CONTENT_PILLARS[selectedPerson] || [] : [];
  const handleLike = (index) => {
    const libraryId = libraryIds[index];
    const isCurrentlyLiked = likedPosts.has(index);
    const newLikedStatus = !isCurrentlyLiked;
    setLikedPosts((prev) => {
      const newSet = new Set(prev);
      if (isCurrentlyLiked) {
        newSet.delete(index);
      } else {
        newSet.add(index);
        setDislikedPosts((d) => {
          const newD = new Set(d);
          newD.delete(index);
          return newD;
        });
      }
      return newSet;
    });
    if (libraryId) {
      setLikedStatus(libraryId, newLikedStatus);
      console.log(`[BatchGenerator] Set liked status for ${libraryId}: ${newLikedStatus}`);
    }
  };
  const handleDislike = (index) => {
    const libraryId = libraryIds[index];
    const isCurrentlyDisliked = dislikedPosts.has(index);
    const newDislikedStatus = !isCurrentlyDisliked;
    setDislikedPosts((prev) => {
      const newSet = new Set(prev);
      if (isCurrentlyDisliked) {
        newSet.delete(index);
      } else {
        newSet.add(index);
        setLikedPosts((l) => {
          const newL = new Set(l);
          newL.delete(index);
          return newL;
        });
      }
      return newSet;
    });
    if (libraryId) {
      setDislikedStatus(libraryId, newDislikedStatus);
      console.log(`[BatchGenerator] Set disliked status for ${libraryId}: ${newDislikedStatus}`);
    }
  };
  const handleRetrySimilar = async (index, originalContent) => {
    if (!result) return;
    setRegeneratingIndex(index);
    try {
      const request = {
        persona: originalContent.metadata.persona,
        platform: originalContent.metadata.platform,
        topic: selectedPillars.length > 0 ? selectedPillars.join(", ") : void 0,
        inspiration: `Create a similar style post but with fresh content. Original was about: ${originalContent.content.substring(0, 100)}`
      };
      const newContent = await SkillsRunner.generateSinglePost(request);
      if (newContent) {
        const newContents = [...result.contents];
        newContents[index] = newContent;
        setResult({
          ...result,
          contents: newContents
        });
        setLikedPosts((prev) => {
          const newSet = new Set(prev);
          newSet.delete(index);
          return newSet;
        });
        setDislikedPosts((prev) => {
          const newSet = new Set(prev);
          newSet.delete(index);
          return newSet;
        });
      }
    } catch (error) {
      console.error("Failed to regenerate content:", error);
    } finally {
      setRegeneratingIndex(null);
    }
  };
  const calculateTotalPosts = () => {
    return selectedPlatforms.length * postsPerPlatform;
  };
  const handleGenerate = async () => {
    if (selectedPlatforms.length === 0 || !selectedPerson) return;
    setIsGenerating(true);
    setResult(null);
    setProgress({ completed: 0, total: calculateTotalPosts() });
    const requests = [];
    for (const platform of selectedPlatforms) {
      for (let i = 0; i < postsPerPlatform; i++) {
        requests.push({
          persona: selectedPerson,
          platform,
          topic: selectedPillars.length > 0 ? selectedPillars.join(", ") : void 0
        });
      }
    }
    try {
      const batchResult = await SkillsRunner.orchestrateBatchGeneration(
        requests,
        (completed, total) => setProgress({ completed, total })
      );
      setResult(batchResult);
      setCurrentPostIndex(0);
      setScheduledPosts(/* @__PURE__ */ new Set());
      setDraftedPosts(/* @__PURE__ */ new Set());
      if (batchResult.contents.length > 0) {
        const savedItems = saveMultipleToLibrary(batchResult.contents, selectedPillars.length > 0 ? selectedPillars.join(", ") : void 0);
        setLibraryIds(savedItems.map((item) => item.id));
        console.log(`[BatchGenerator] Saved ${batchResult.contents.length} items to library with IDs:`, savedItems.map((i) => i.id));
      }
      if (onContentGenerated) {
        onContentGenerated(batchResult.contents);
      }
    } catch (error) {
      console.error("Batch generation failed:", error);
    } finally {
      setIsGenerating(false);
    }
  };
  const handleCopy = (content, index) => {
    navigator.clipboard.writeText(content);
    setCopiedIndex(index);
    setTimeout(() => setCopiedIndex(null), 2e3);
  };
  const handleExportAll = () => {
    if (!result) return;
    const htmlContent = `
<!DOCTYPE html>
<html>
<head>
  <title>${selectedPerson || "Content"} Export - ${(/* @__PURE__ */ new Date()).toLocaleDateString()}</title>
  <style>
    body { font-family: Arial, sans-serif; max-width: 800px; margin: 0 auto; padding: 40px 20px; color: #333; }
    h1 { color: #1a1a1a; border-bottom: 2px solid #333; padding-bottom: 10px; }
    .post { margin-bottom: 40px; page-break-inside: avoid; border: 1px solid #ddd; padding: 20px; border-radius: 8px; }
    .post-header { display: flex; justify-content: space-between; margin-bottom: 15px; padding-bottom: 10px; border-bottom: 1px solid #eee; }
    .post-number { font-weight: bold; font-size: 18px; }
    .platform { background: #f0f0f0; padding: 4px 12px; border-radius: 4px; font-size: 14px; }
    .content { white-space: pre-wrap; background: #f9f9f9; padding: 15px; border-radius: 4px; margin: 10px 0; line-height: 1.6; }
    .image-prompt { margin-top: 15px; padding-top: 15px; border-top: 1px solid #eee; }
    .image-prompt h4 { margin: 0 0 10px 0; color: #666; font-size: 14px; }
    .prompt-text { font-family: monospace; font-size: 12px; background: #f0f0f0; padding: 10px; border-radius: 4px; }
    .scores { font-size: 12px; color: #666; }
    @media print { .post { break-inside: avoid; } }
  </style>
</head>
<body>
  <h1>${selectedPerson || "Content"} Export</h1>
  <p style="color: #666; margin-bottom: 30px;">Generated: ${(/* @__PURE__ */ new Date()).toLocaleString()} | ${result.contents.length} posts</p>
  ${result.contents.map((content, index) => `
    <div class="post">
      <div class="post-header">
        <span class="post-number">Post ${index + 1}</span>
        <span class="platform">${content.metadata.platform}</span>
      </div>
      <div class="content">${content.content.replace(/</g, "&lt;").replace(/>/g, "&gt;")}</div>
      <div class="image-prompt">
        <h4>Image Prompt (Midjourney)</h4>
        <p class="prompt-text">${content.imagePrompt.plugAndPlayPrompt.replace(/</g, "&lt;").replace(/>/g, "&gt;")}</p>
      </div>
      <div class="scores">Voice: ${content.metadata.voiceScore ?? 0}% | Compliance: ${content.metadata.complianceScore ?? 0}%</div>
    </div>
  `).join("")}
</body>
</html>`;
    const printWindow = window.open("", "_blank");
    if (printWindow) {
      printWindow.document.write(htmlContent);
      printWindow.document.close();
      setTimeout(() => printWindow.print(), 500);
    }
  };
