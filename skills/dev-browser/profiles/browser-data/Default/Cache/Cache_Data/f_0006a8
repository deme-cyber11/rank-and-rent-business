function getStatusBadgeVariant(status) {
  const statusLower = status.toLowerCase();
  if (statusLower === "fulfilled" || statusLower === "completed" || statusLower === "paid") {
    return "success";
  }
  if (statusLower === "pending" || statusLower === "processing" || statusLower === "open") {
    return "warning";
  }
  if (statusLower === "cancelled" || statusLower === "refunded" || statusLower === "failed") {
    return "danger";
  }
  if (statusLower === "unfulfilled" || statusLower === "partially_fulfilled") {
    return "secondary";
  }
  return "default";
}
function formatDate(dateString) {
  const date = new Date(dateString);
  return date.toLocaleDateString("en-US", {
    month: "short",
    day: "numeric",
    year: "numeric"
  });
}
function formatStatusDisplay(status) {
  return status.replace(/_/g, " ").replace(/\b\w/g, (char) => char.toUpperCase());
}
