      TableHeadCell,
      {
        className: cn(
          "cursor-pointer hover:bg-muted/50 select-none transition-colors",
          alignClass
        ),
        onClick: () => handleSort(column),
        children: /* @__PURE__ */ jsxDEV(
          "div",
          {
            className: cn(
