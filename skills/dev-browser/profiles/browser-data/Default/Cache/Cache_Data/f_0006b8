const formatCurrency = (value) => {
  return new Intl.NumberFormat("en-US", {
    style: "currency",
    currency: "USD",
    minimumFractionDigits: 2,
    maximumFractionDigits: 2
  }).format(value);
};
const formatPercent = (value) => {
  return `${value >= 0 ? "+" : ""}${value.toFixed(1)}%`;
};
const formatDateTime = (dateString) => {
  const date = new Date(dateString);
  return date.toLocaleDateString("en-US", {
    month: "short",
    day: "numeric",
    year: "numeric"
  }) + " at " + date.toLocaleTimeString("en-US", {
    hour: "numeric",
    minute: "2-digit",
    hour12: true
  });
};
const getMethodBadgeVariant = (method) => {
  switch (method) {
    case "manual":
      return "default";
    case "bulk_upload":
      return "success";
    case "scheduled":
      return "warning";
    case "api":
      return "secondary";
    default:
      return "default";
  }
};
const getMethodLabel = (method) => {
  switch (method) {
    case "manual":
      return "Manual";
    case "bulk_upload":
      return "Bulk Upload";
    case "scheduled":
      return "Scheduled";
    case "api":
      return "API";
    default:
      return method;
  }
};
const isRecentChange = (changedAt) => {
  const changeDate = new Date(changedAt);
  const thirtyDaysAgo = /* @__PURE__ */ new Date();
  thirtyDaysAgo.setDate(thirtyDaysAgo.getDate() - 30);
  return changeDate >= thirtyDaysAgo;
};
