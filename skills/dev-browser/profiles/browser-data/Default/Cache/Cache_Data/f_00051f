import { SPECIAL_SERVICES, SPEED_OPTIONS } from "/src/pages/sales/freight-calculator/tabs/types.ts";
export function QuickQuoteTab({ onQuoteSaved }) {
  _s();
  const [formData, setFormData] = useState({
    originWarehouse: "main",
    originZip: "",
    destinationAddress: "",
    destinationZip: "",
    isResidential: false,
    products: [{ id: crypto.randomUUID(), sku: "", name: "", quantity: 1, weight: 0 }],
    totalWeight: 0,
    dimensions: { length: 0, width: 0, height: 0 },
    packageCount: 1,
    speed: "ground",
    specialServices: []
  });
  const [carrierQuotes, setCarrierQuotes] = useState([]);
  const [selectedCarrier, setSelectedCarrier] = useState(null);
  const [loading, setLoading] = useState(false);
  const [errors, setErrors] = useState({});
  const [quoteGenerated, setQuoteGenerated] = useState(false);
  const [copied, setCopied] = useState(false);
  const updateField = (field, value) => {
    setFormData((prev) => ({ ...prev, [field]: value }));
    if (errors[field]) {
      setErrors((prev) => ({ ...prev, [field]: void 0 }));
    }
  };
  const updateProduct = (id, field, value) => {
    setFormData((prev) => ({
      ...prev,
      products: prev.products.map((p) => p.id === id ? { ...p, [field]: value } : p)
    }));
  };
  const addProduct = () => {
    setFormData((prev) => ({
      ...prev,
      products: [...prev.products, { id: crypto.randomUUID(), sku: "", name: "", quantity: 1, weight: 0 }]
    }));
  };
  const removeProduct = (id) => {
    if (formData.products.length === 1) return;
    setFormData((prev) => ({
      ...prev,
      products: prev.products.filter((p) => p.id !== id)
    }));
  };
  const calculateTotalWeight = () => {
    return formData.products.reduce((sum, p) => sum + p.weight * p.quantity, 0);
  };
  const toggleService = (value) => {
    setFormData((prev) => ({
      ...prev,
      specialServices: prev.specialServices.includes(value) ? prev.specialServices.filter((s) => s !== value) : [...prev.specialServices, value]
    }));
  };
  const validateForm = () => {
    const newErrors = {};
    if (!formData.originZip || formData.originZip.length !== 5) {
      newErrors.originZip = "Valid 5-digit ZIP code required";
    }
    if (!formData.destinationZip || formData.destinationZip.length !== 5) {
      newErrors.destinationZip = "Valid 5-digit ZIP code required";
    }
    if (formData.products.length === 0 || formData.products.every((p) => !p.sku)) {
      newErrors.products = "At least one product is required";
    }
    const totalWeight2 = calculateTotalWeight();
    if (totalWeight2 <= 0) {
      newErrors.weight = "Total weight must be greater than 0";
    }
    if (formData.dimensions.length <= 0 || formData.dimensions.width <= 0 || formData.dimensions.height <= 0) {
      newErrors.dimensions = "Valid dimensions required";
    }
    setErrors(newErrors);
    return Object.keys(newErrors).length === 0;
  };
  const calculateQuote = async () => {
    if (!validateForm()) return;
    setLoading(true);
    setQuoteGenerated(false);
    await new Promise((resolve) => setTimeout(resolve, 1500));
    const mockQuotes = [
      { carrier: "Priority 1", rate: 245, transitDays: 5, serviceLevel: "Standard Ground" },
      { carrier: "FedEx Freight", rate: 289, transitDays: 4, serviceLevel: "Economy" },
      { carrier: "XPO Logistics", rate: 312, transitDays: 3, serviceLevel: "Standard LTL" },
      { carrier: "YRC Freight", rate: 268, transitDays: 5, serviceLevel: "Standard" },
      { carrier: "Old Dominion", rate: 295, transitDays: 4, serviceLevel: "Standard Service" }
    ];
    setCarrierQuotes(mockQuotes);
    setSelectedCarrier(null);
    setLoading(false);
    setQuoteGenerated(true);
  };
  const selectCarrier = (carrier) => {
    setSelectedCarrier(carrier);
  };
  const saveQuote = () => {
    if (!selectedCarrier) {
      alert("Please select a carrier first");
      return;
    }
    const quote = {
      quoteNumber: `FQ-${Date.now()}`,
      timestamp: (/* @__PURE__ */ new Date()).toISOString(),
      formData,
      carrierQuotes,
      selectedCarrier,
      selectedRate: carrierQuotes.find((q) => q.carrier === selectedCarrier)?.rate
    };
    console.log("Saving quote:", quote);
    onQuoteSaved?.(quote);
    alert("Quote saved successfully!");
  };
  const copyQuote = () => {
    if (!selectedCarrier) {
      alert("Please select a carrier first");
      return;
    }
    const selectedQuote = carrierQuotes.find((q) => q.carrier === selectedCarrier);
    if (!selectedQuote) return;
    let text = "FREIGHT QUOTE\n";
    text += "=".repeat(60) + "\n\n";
    text += `Quote Date: ${(/* @__PURE__ */ new Date()).toLocaleDateString()}
`;
    text += `Origin: ${formData.originZip}
`;
    text += `Destination: ${formData.destinationZip}
`;
    text += `Weight: ${calculateTotalWeight()} lbs
`;
    text += `Dimensions: ${formData.dimensions.length}" × ${formData.dimensions.width}" × ${formData.dimensions.height}"
`;
    text += `Packages: ${formData.packageCount}

`;
    text += "PRODUCTS:\n";
    text += "-".repeat(60) + "\n";
    formData.products.forEach((p) => {
      text += `${p.name || p.sku} - Qty: ${p.quantity}, Weight: ${p.weight} lbs
`;
    });
    text += "\n" + "-".repeat(60) + "\n";
    text += `Selected Carrier: ${selectedQuote.carrier}
`;
    text += `Service Level: ${selectedQuote.serviceLevel}
`;
    text += `Transit Time: ${selectedQuote.transitDays} business days
`;
    text += `Rate: ${formatCurrency(selectedQuote.rate)}
`;
    navigator.clipboard.writeText(text);
    setCopied(true);
    setTimeout(() => setCopied(false), 2e3);
  };
  const emailQuote = () => {
    alert("Email functionality coming soon!");
  };
  const totalWeight = calculateTotalWeight();
