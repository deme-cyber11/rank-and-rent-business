export function TaskDetailPanel({ taskId, onClose }) {
  _s();
  const [loading, setLoading] = useState(false);
  const [error, setError] = useState(null);
  const [data, setData] = useState(null);
  const [selectedStatus, setSelectedStatus] = useState("");
  const [updatingStatus, setUpdatingStatus] = useState(false);
  useEffect(() => {
    if (!taskId) {
      setData(null);
      setError(null);
      return;
    }
    let cancelled = false;
    setLoading(true);
    setError(null);
    const fetchTaskDetails = async () => {
      try {
        const res = await fetch(`/api/bollards/gameplan/tasks/${taskId}`);
        const jsonBody = await res.json();
        if (cancelled) return;
        if (!res.ok || !jsonBody.success || !jsonBody.data) {
          setError(jsonBody.error || `Failed to load task details (${res.status})`);
          setData(null);
          setLoading(false);
          return;
        }
        setData(jsonBody.data);
        setSelectedStatus(jsonBody.data.task.status);
        setLoading(false);
      } catch (err) {
        if (cancelled) return;
        setError(err instanceof Error ? err.message : "Unknown error");
        setData(null);
        setLoading(false);
      }
    };
    fetchTaskDetails();
    return () => {
      cancelled = true;
    };
  }, [taskId]);
  const handleAddNote = async (noteText) => {
    if (!taskId) return;
    const res = await fetch(`/api/bollards/gameplan/tasks/${taskId}/notes`, {
      method: "POST",
      headers: { "Content-Type": "application/json" },
      body: JSON.stringify({ noteText })
    });
    const result = await res.json();
    if (!res.ok || !result.success) {
      throw new Error(result.error || "Failed to add note");
    }
    const refreshRes = await fetch(`/api/bollards/gameplan/tasks/${taskId}`);
    const refreshBody = await refreshRes.json();
    if (refreshRes.ok && refreshBody.success && refreshBody.data) {
      setData(refreshBody.data);
    }
  };
  const handleUpdateStatus = async () => {
    if (!selectedStatus || !taskId || selectedStatus === data?.task.status) return;
    setUpdatingStatus(true);
    try {
      const res = await fetch(`/api/bollards/gameplan/tasks/${taskId}/status`, {
        method: "PATCH",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({ status: selectedStatus })
      });
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to update status");
      }
      const refreshRes = await fetch(`/api/bollards/gameplan/tasks/${taskId}`);
      const refreshBody = await refreshRes.json();
      if (refreshRes.ok && refreshBody.success && refreshBody.data) {
        setData(refreshBody.data);
      }
    } catch (err) {
      console.error("Error updating status:", err);
      alert(err instanceof Error ? err.message : "Failed to update status");
    } finally {
      setUpdatingStatus(false);
    }
  };
