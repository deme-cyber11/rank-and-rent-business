function generateSummaryCards(data) {
  const { fundedAmount, factorRate, interestRate, paymentAmount, paymentFrequency, schedule } = data;
  const frequencyLabel = getFrequencyLabel(paymentFrequency);
  const fundingCards = [];
  fundingCards.push(`
    <div class="summary-card">
      <div class="card-label">Funded Amount</div>
      <div class="card-value">${formatCurrency(fundedAmount)}</div>
    </div>
  `);
  if (factorRate !== void 0) {
    fundingCards.push(`
      <div class="summary-card">
        <div class="card-label">Factor Rate</div>
        <div class="card-value">${factorRate.toFixed(2)}</div>
      </div>
    `);
  }
  if (interestRate !== void 0) {
    fundingCards.push(`
      <div class="summary-card">
        <div class="card-label">Interest Rate</div>
        <div class="card-value">${(interestRate * 100).toFixed(2)}%</div>
      </div>
    `);
  }
  fundingCards.push(`
    <div class="summary-card">
      <div class="card-label">Payment Amount</div>
      <div class="card-value">${formatCurrency(paymentAmount)}/${frequencyLabel.toLowerCase().slice(0, -2)}</div>
    </div>
  `);
  fundingCards.push(`
    <div class="summary-card">
      <div class="card-label">Frequency</div>
      <div class="card-value">${frequencyLabel}</div>
    </div>
  `);
  const payoffCards = `
    <div class="summary-card highlight">
      <div class="card-label">Total Payback</div>
      <div class="card-value">${formatCurrency(schedule.totalPayback)}</div>
    </div>
    <div class="summary-card">
      <div class="card-label">Total ${factorRate ? "Fees" : "Interest"}</div>
      <div class="card-value">${formatCurrency(schedule.totalInterest)}</div>
    </div>
    <div class="summary-card">
      <div class="card-label">Payoff Date</div>
      <div class="card-value">${formatDateForPDF(schedule.payoffDate)}</div>
    </div>
    <div class="summary-card">
      <div class="card-label">Total Payments</div>
      <div class="card-value">${schedule.payments.length}</div>
    </div>
    <div class="summary-card">
      <div class="card-label">Term Length</div>
      <div class="card-value">${schedule.termLength} days</div>
    </div>
  `;
  return `
    <div class="summary-section">
      <div class="section-title">Funding Summary</div>
      <div class="summary-grid">
        ${fundingCards.join("")}
      </div>
    </div>
    <div class="summary-section">
      <div class="section-title">Payoff Metrics</div>
      <div class="summary-grid">
        ${payoffCards}
      </div>
    </div>
  `;
}
function generateScheduleTable(payments, hasFactor) {
  const interestLabel = hasFactor ? "Fees" : "Interest";
  const rows = payments.map((payment, index) => {
    const isLast = index === payments.length - 1;
    const rowClass = isLast ? "final-row" : "";
    return `
      <tr class="${rowClass}">
        <td class="payment-num">${payment.paymentNumber}</td>
        <td>${formatShortDate(payment.date)}</td>
        <td>${formatCurrency(payment.payment)}</td>
        <td>${formatCurrency(payment.principal)}</td>
        <td>${formatCurrency(payment.interest)}</td>
        <td>${formatCurrency(payment.balance)}</td>
      </tr>
    `;
  }).join("");
  return `
    <div class="schedule-section">
      <div class="section-title">Payment Schedule</div>
      <table class="schedule-table">
        <thead>
          <tr>
            <th>#</th>
            <th>Date</th>
            <th>Payment</th>
            <th>Principal</th>
            <th>${interestLabel}</th>
            <th>Balance</th>
          </tr>
        </thead>
        <tbody>
          ${rows}
        </tbody>
      </table>
    </div>
  `;
}
function generateFooter(lenderName) {
  const lenderInfo = lenderName ? `<div class="lender-info">Funding provided by: <strong>${lenderName}</strong></div>` : "";
  return `
    ${lenderInfo}
    <div class="contact-section">
      <div class="section-title">Contact Information</div>
      <div class="contact-grid">
        <div class="contact-item">
          <div class="label">Company</div>
          <div class="value">${BRAND.companyName}</div>
        </div>
        <div class="contact-item">
          <div class="label">Email</div>
          <div class="value"><a href="mailto:${BRAND.email}">${BRAND.email}</a></div>
        </div>
        <div class="contact-item">
          <div class="label">Phone</div>
          <div class="value">${BRAND.phone}</div>
        </div>
        <div class="contact-item">
          <div class="label">Website</div>
          <div class="value"><a href="https://${BRAND.website}" target="_blank">${BRAND.website}</a></div>
        </div>
      </div>
    </div>
    <div class="disclaimer-section">
      <div class="disclaimer-title">Disclaimer</div>
      <div class="disclaimer-text">${BRAND.disclaimer}</div>
    </div>
  `;
}
export function generateScheduleHTML(data) {
  const generatedDate = data.generatedDate || /* @__PURE__ */ new Date();
  const hasFactor = data.factorRate !== void 0;
  const dealIdSection = data.dealId ? `<div class="deal-id">Deal ID: ${data.dealId}</div>` : "";
  const html = `
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Debt Schedule - ${data.businessName} | ${BRAND.companyName}</title>
  <style>${generateStyles()}</style>
</head>
<body>
  <div class="print-controls no-print">
    <button class="print-btn" onclick="window.print()">
      <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
        <polyline points="6 9 6 2 18 2 18 9"></polyline>
        <path d="M6 18H4a2 2 0 0 1-2-2v-5a2 2 0 0 1 2-2h16a2 2 0 0 1 2 2v5a2 2 0 0 1-2 2h-2"></path>
        <rect x="6" y="14" width="12" height="8"></rect>
      </svg>
      Print / Save as PDF
    </button>
  </div>

  <div class="container">
    <!-- Header -->
    <div class="header">
      <div class="logo">${BRAND.companyName}</div>
      <div class="header-date">
        <strong>Document Generated</strong>
        ${formatDateForPDF(generatedDate)}
      </div>
    </div>

    <!-- Title Section -->
    <div class="title-section">
      <div class="document-title">Debt Schedule</div>
      <div class="business-name">${data.businessName}</div>
      ${dealIdSection}
    </div>

    <!-- Summary Cards -->
    ${generateSummaryCards(data)}

    <!-- Payment Schedule Table -->
    ${generateScheduleTable(data.schedule.payments, hasFactor)}

    <!-- Footer -->
    ${generateFooter(data.lenderName)}
  </div>

  <script>
    // Auto-trigger print dialog when opened with intent to print
    // This is controlled by the opener
    if (window.opener && window.opener.hugePDFAutoPrint) {
      setTimeout(() => window.print(), 500);
    }
  <\/script>
</body>
</html>
  `.trim();
  return html;
}
export function generateDebtSchedulePDF(data, options) {
  const { autoPrint = true, windowName = "DebtSchedulePDF" } = options || {};
  const html = generateScheduleHTML(data);
  if (autoPrint) {
    window.hugePDFAutoPrint = true;
  }
  const width = Math.min(900, window.screen.width - 100);
  const height = Math.min(1100, window.screen.height - 100);
  const left = Math.max(0, (window.screen.width - width) / 2);
  const top = Math.max(0, (window.screen.height - height) / 2);
  const printWindow = window.open(
    "",
    windowName,
    `width=${width},height=${height},left=${left},top=${top},scrollbars=yes,resizable=yes`
  );
  if (!printWindow) {
    console.error("Failed to open print window. Please check your popup blocker settings.");
    if (autoPrint) {
      window.hugePDFAutoPrint = false;
    }
    return null;
  }
  printWindow.document.write(html);
  printWindow.document.close();
  printWindow.onload = () => {
    if (autoPrint) {
      window.hugePDFAutoPrint = false;
    }
  };
  return printWindow;
}
export function downloadScheduleAsHTML(data, filename) {
  const html = generateScheduleHTML(data);
  const blob = new Blob([html], { type: "text/html" });
  const url = URL.createObjectURL(blob);
  const sanitizedName = data.businessName.replace(/[^a-zA-Z0-9]/g, "-").toLowerCase();
  const defaultFilename = `debt-schedule-${sanitizedName}.html`;
  const link = document.createElement("a");
  link.href = url;
  link.download = filename || defaultFilename;
  document.body.appendChild(link);
  link.click();
  document.body.removeChild(link);
  setTimeout(() => URL.revokeObjectURL(url), 100);
}
export function renderScheduleToIframe(data, iframe) {
  const html = generateScheduleHTML(data);
  const doc = iframe.contentDocument || iframe.contentWindow?.document;
  if (!doc) {
    console.error("Could not access iframe document");
    return;
  }
  doc.open();
  doc.write(html);
  doc.close();
}
export function printFromIframe(iframe) {
  const win = iframe.contentWindow;
  if (win) {
    win.focus();
    win.print();
  }
}

