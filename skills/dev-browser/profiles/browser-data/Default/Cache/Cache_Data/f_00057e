_c3 = YearlyComparison;
export function HomeRunsClient({ records, categories, refreshedAt }) {
  _s();
  const [dateRangeOption, setDateRangeOption] = useState("all-time");
  const [selectedRep, setSelectedRep] = useState("all");
  const [selectedVendor, setSelectedVendor] = useState("all");
  const { reps, vendors } = useMemo(() => {
    const repSet = /* @__PURE__ */ new Set();
    const vendorSet = /* @__PURE__ */ new Set();
    records.forEach((record) => {
      if (record.rep) repSet.add(record.rep);
      if (record.vendors && Array.isArray(record.vendors)) {
        record.vendors.forEach((v) => v && vendorSet.add(v));
      }
    });
    return {
      reps: Array.from(repSet).sort(),
      vendors: Array.from(vendorSet).sort()
    };
  }, [records]);
  const filteredRecords = useMemo(() => {
    const range = getDateRange(dateRangeOption);
    return records.filter((record) => {
      const date = parseDate(record.closedAt);
      if (!date || date < range.start || date > range.end) return false;
      if (selectedRep !== "all" && record.rep !== selectedRep) return false;
      if (selectedVendor !== "all") {
        const hasMatchingVendor = record.vendors?.some((v) => v === selectedVendor);
        if (!hasMatchingVendor) return false;
      }
      return true;
    });
  }, [records, dateRangeOption, selectedRep, selectedVendor]);
  const rankedRecords = useMemo(() => {
    return filteredRecords.slice().sort((a, b) => (b.value ?? 0) - (a.value ?? 0)).slice(0, 100);
  }, [filteredRecords]);
  const totalHomeRuns = filteredRecords.length;
  const totalRevenue = filteredRecords.reduce((sum, record) => sum + (record.value ?? 0), 0);
  const totalProfit = filteredRecords.reduce((sum, record) => sum + (record.profit ?? 0), 0);
  const averageOrder = totalHomeRuns > 0 ? totalRevenue / totalHomeRuns : 0;
  const topOrder = rankedRecords[0];
  const { topReps, topVendors } = useMemo(() => {
    const repMap = /* @__PURE__ */ new Map();
    filteredRecords.forEach((record) => {
      const rep = record.rep || "Unknown";
      const existing = repMap.get(rep) || { value: 0, count: 0, profit: 0 };
      repMap.set(rep, {
        value: existing.value + (record.value ?? 0),
        count: existing.count + 1,
        profit: existing.profit + (record.profit ?? 0)
      });
    });
    const topReps2 = Array.from(repMap.entries()).map(([name, data]) => ({ name, ...data })).sort((a, b) => b.value - a.value);
    const vendorMap = /* @__PURE__ */ new Map();
    filteredRecords.forEach((record) => {
      const vendors2 = record.vendors || ["Unknown"];
      const perVendorValue = (record.value ?? 0) / vendors2.length;
      const perVendorProfit = (record.profit ?? 0) / vendors2.length;
      vendors2.forEach((vendor) => {
        const existing = vendorMap.get(vendor) || { value: 0, count: 0, profit: 0 };
        vendorMap.set(vendor, {
          value: existing.value + perVendorValue,
          count: existing.count + 1,
          profit: existing.profit + perVendorProfit
        });
      });
    });
    const topVendors2 = Array.from(vendorMap.entries()).map(([name, data]) => ({ name, ...data })).sort((a, b) => b.value - a.value);
    return { topReps: topReps2, topVendors: topVendors2 };
  }, [filteredRecords]);
  const yearlyData = useMemo(() => {
    const yearMap = /* @__PURE__ */ new Map();
    records.forEach((record) => {
      const date = parseDate(record.closedAt);
      if (!date) return;
      const year = date.getFullYear();
      const value = record.value ?? 0;
      const profit = record.profit ?? 0;
      const tier = value >= 5e4 ? "top" : value >= 25e3 ? "mid" : "base";
      const existing = yearMap.get(year) || {
        year,
        total: 0,
        revenue: 0,
        profit: 0,
        tiers: { base: 0, mid: 0, top: 0 },
        tierRevenue: { base: 0, mid: 0, top: 0 }
      };
      existing.total += 1;
      existing.revenue += value;
      existing.profit += profit;
      existing.tiers[tier] += 1;
      existing.tierRevenue[tier] += value;
      yearMap.set(year, existing);
    });
    return Array.from(yearMap.values()).sort((a, b) => b.year - a.year);
  }, [records]);
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV("div", { className: "flex flex-col gap-4 sm:flex-row sm:items-center sm:justify-between", children: [
      /* @__PURE__ */ jsxDEV(
        SectionHeader,
        {
          title: "Home Runs",
          description: "Track our biggest orders ($10K+) and celebrate sales wins",
          badge: "Sales"
        },
        void 0,
        false,
        {
          fileName: "C:/Projects/Source 4 Industries/src/pages/dashboards/home-runs/home-runs-client.tsx",
          lineNumber: 590,
          columnNumber: 9
        },
        this
      ),
      /* @__PURE__ */ jsxDEV("div", { className: "flex items-center gap-3", children: [
        /* @__PURE__ */ jsxDEV(
          "select",
          {
            value: dateRangeOption,
            onChange: (e) => setDateRangeOption(e.target.value),
            className: "h-9 w-28 rounded-md border border-border bg-card px-3 text-sm shadow-sm focus-visible:outline-none focus-visible:ring-2 focus-visible:ring-primary",
            children: DATE_RANGE_OPTIONS.map(
              (option) => /* @__PURE__ */ jsxDEV("option", { value: option.value, children: option.label }, option.value, false, {
