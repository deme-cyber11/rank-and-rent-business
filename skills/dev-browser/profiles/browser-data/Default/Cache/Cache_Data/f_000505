export function ScheduledUpdatesTable({
  updates,
  isLoading = false,
  onCancel,
  onReschedule
}) {
  _s2();
  const [sortConfig, setSortConfig] = useState({ column: "effectiveDate", direction: "asc" });
  const [cancelDialog, setCancelDialog] = useState({
    open: false,
    update: null
  });
  const [rescheduleDialog, setRescheduleDialog] = useState({
    open: false,
    update: null
  });
  const [actionLoading, setActionLoading] = useState(null);
  const sortedUpdates = useMemo(() => {
    if (!sortConfig.column || !sortConfig.direction) {
      return updates;
    }
    return [...updates].sort((a, b) => {
      const column = sortConfig.column;
      let aVal;
      let bVal;
      switch (column) {
        case "sku":
          aVal = a.sku;
          bVal = b.sku;
          break;
        case "productTitle":
          aVal = a.productTitle;
          bVal = b.productTitle;
          break;
        case "currentPrice":
          aVal = a.currentPrice;
          bVal = b.currentPrice;
          break;
        case "newPrice":
          aVal = a.newPrice;
          bVal = b.newPrice;
          break;
        case "priceChange":
          aVal = a.priceChangeDollar;
          bVal = b.priceChangeDollar;
          break;
        case "effectiveDate":
          aVal = a.effectiveDate;
          bVal = b.effectiveDate;
          break;
        default:
          return 0;
      }
      if (aVal == null) return sortConfig.direction === "asc" ? 1 : -1;
      if (bVal == null) return sortConfig.direction === "asc" ? -1 : 1;
      if (typeof aVal === "number" && typeof bVal === "number") {
        return sortConfig.direction === "asc" ? aVal - bVal : bVal - aVal;
      }
      const aStr = String(aVal).toLowerCase();
      const bStr = String(bVal).toLowerCase();
      return sortConfig.direction === "asc" ? aStr.localeCompare(bStr) : bStr.localeCompare(aStr);
    });
  }, [updates, sortConfig]);
  const groupedUpdates = useMemo(() => groupByDate(sortedUpdates), [sortedUpdates]);
  const handleSort = (column) => {
    setSortConfig((prev) => {
      if (prev.column === column) {
        if (prev.direction === "asc") {
          return { column, direction: "desc" };
        } else if (prev.direction === "desc") {
          return { column: null, direction: null };
        }
      }
      return { column, direction: "asc" };
    });
  };
