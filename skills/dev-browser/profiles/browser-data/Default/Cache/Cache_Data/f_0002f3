import { TwitterThreadBuilder } from "/src/components/ContentPlanner/TwitterThreadBuilder.tsx";
import { AIContentSuggestions } from "/src/components/ContentPlanner/AIContentSuggestions.tsx";
import { generateAIContent } from "/src/services/aiContentGenerator.ts";
import { saveDraft } from "/src/services/contentLibraryService.ts";
import { PLATFORM_LIMITS, PLATFORM_COLORS } from "/src/types/content.ts";
const PERSONA_PLATFORMS = {
  "Zac": ["LinkedIn", "Instagram", "Facebook", "Twitter"],
  "Luke": ["LinkedIn", "Instagram", "Facebook"],
  "Huge Capital": ["Facebook", "Blog", "Newsletter"]
};
export const ContentEditor = ({
  post,
  platform,
  person,
  profile,
  stories = [],
  onSave,
  onSchedule,
  onApprove,
  onPlatformChange,
  pillars = []
}) => {
  _s();
  const availablePlatforms = PERSONA_PLATFORMS[person] || [];
  const [content, setContent] = useState(post.content || "");
  const [threadHook, setThreadHook] = useState(post.threadHook || "");
  const [threadParts, setThreadParts] = useState(post.threadContent || []);
  const [isThread, setIsThread] = useState(post.isThread || false);
  const [selectedPillar, setSelectedPillar] = useState(post.contentPillar || "");
  const [sources, setSources] = useState(post.sources || []);
  const [scheduledDate, setScheduledDate] = useState(post.scheduledFor || "");
  const [isSaving, setIsSaving] = useState(false);
  const [copied, setCopied] = useState(false);
  const [lastSaved, setLastSaved] = useState(null);
  const [hasUnsavedChanges, setHasUnsavedChanges] = useState(false);
  const [showAISuggestions, setShowAISuggestions] = useState(false);
  const [aiSuggestions, setAiSuggestions] = useState([]);
  const [isGeneratingAI, setIsGeneratingAI] = useState(false);
  const [showAIPromptModal, setShowAIPromptModal] = useState(false);
  const [aiTopic, setAiTopic] = useState("");
  const [selectedStoryIds, setSelectedStoryIds] = useState([]);
  const [showApprovedMessage, setShowApprovedMessage] = useState(false);
  useEffect(() => {
    if (!hasUnsavedChanges || !content) return;
    const autoSaveTimer = setTimeout(async () => {
      await handleSave("draft");
      setHasUnsavedChanges(false);
      setLastSaved(/* @__PURE__ */ new Date());
    }, 3e4);
    return () => clearTimeout(autoSaveTimer);
  }, [content, threadHook, threadParts, selectedPillar, sources, hasUnsavedChanges]);
  useEffect(() => {
    setHasUnsavedChanges(true);
  }, [content, threadHook, threadParts, selectedPillar, sources]);
  const characterLimit = PLATFORM_LIMITS[platform];
  const characterCount = isThread ? threadHook.length : content.length;
  const isOverLimit = characterCount > characterLimit;
  const supportsThreads = platform === "Twitter";
  const supportsImages = ["Twitter", "Facebook", "Instagram", "LinkedIn"].includes(platform);
  const supportsSources = ["Blog", "Newsletter"].includes(platform);
  const handleSave = async (status = "draft") => {
    setIsSaving(true);
    try {
      const postContent = isThread ? threadHook : content;
      const postData = {
        ...post,
        content: postContent,
        threadContent: isThread ? threadParts : void 0,
        isThread,
        threadHook: isThread ? threadHook : void 0,
        contentPillar: selectedPillar,
        sources: supportsSources ? sources : void 0,
        scheduledFor: status === "scheduled" ? scheduledDate : void 0,
        status,
        platform
      };
      if (status === "scheduled") {
        await onSchedule(postData);
      } else {
        await onSave(postData);
        if (postContent && postContent.trim().length > 0) {
          saveDraft(postContent, person, platform);
          console.log("[ContentEditor] Saved draft to Content Library");
        }
      }
      setLastSaved(/* @__PURE__ */ new Date());
      setHasUnsavedChanges(false);
    } catch (error) {
      console.error("Error saving post:", error);
    } finally {
      setIsSaving(false);
    }
  };
  const handleCopy = () => {
    const textToCopy = isThread ? `${threadHook}

${threadParts.map((t) => t.content).join("\n\n")}` : content;
    navigator.clipboard.writeText(textToCopy);
    setCopied(true);
    setTimeout(() => setCopied(false), 2e3);
  };
  const handleApprove = async () => {
    await handleSave("scheduled");
    await onApprove({
      ...post,
      content: isThread ? threadHook : content,
      status: "scheduled"
    });
    setShowApprovedMessage(true);
    setTimeout(() => setShowApprovedMessage(false), 4e3);
    console.log(`[ContentEditor] Content approved and scheduled for ${platform}`);
  };
  const addSource = () => {
    setSources([...sources, { type: "website", title: "", url: "" }]);
  };
  const updateSource = (index, field, value) => {
    const updated = [...sources];
    updated[index] = { ...updated[index], [field]: value };
    setSources(updated);
  };
  const removeSource = (index) => {
    setSources(sources.filter((_, i) => i !== index));
  };
  const handleGenerateAI = async () => {
    if (!profile) return;
    setIsGeneratingAI(true);
    setShowAISuggestions(true);
    setShowAIPromptModal(false);
    try {
      let selectedStories = [];
      if (selectedStoryIds.length > 0) {
        selectedStories = stories.filter((s) => selectedStoryIds.includes(s.id));
      } else {
        selectedStories = stories.filter((s) => s.personName === person && s.isApproved).slice(0, 5);
      }
      const suggestions = await generateAIContent({
        person,
        platform,
        profile,
        contentPillar: selectedPillar,
        topic: aiTopic || void 0,
        stories: selectedStories.length > 0 ? selectedStories : void 0
      });
      setAiSuggestions(suggestions);
    } catch (error) {
      console.error("AI generation failed:", error);
    } finally {
      setIsGeneratingAI(false);
    }
  };
  const openAIPromptModal = () => {
    setShowAIPromptModal(true);
  };
  const handleSelectAISuggestion = (suggestion) => {
    if (isThread && platform === "Twitter") {
      setThreadHook(suggestion);
    } else {
      setContent(suggestion);
    }
    setShowAISuggestions(false);
    setHasUnsavedChanges(true);
  };
  return /* @__PURE__ */ jsxDEV("div", { className: "space-y-6", children: [
    /* @__PURE__ */ jsxDEV(
