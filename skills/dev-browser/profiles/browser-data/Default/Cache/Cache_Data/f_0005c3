export function ProductOptimizationClient({ initialStats, initialAudits }) {
  _s();
  const navigate = useNavigate();
  const [scoreRange, setScoreRange] = useState([0, 100]);
  const [vendorFilter, setVendorFilter] = useState("all");
  const [categoryFilter, setCategoryFilter] = useState("all");
  const [minPriority, setMinPriority] = useState("");
  const [searchSku, setSearchSku] = useState("");
  const [debouncedSearch, setDebouncedSearch] = useState("");
  const [stats, setStats] = useState(initialStats);
  const [audits, setAudits] = useState(initialAudits);
  const [loading, setLoading] = useState(false);
  const [syncing, setSyncing] = useState(false);
  const [auditing, setAuditing] = useState(false);
  const [currentPage, setCurrentPage] = useState(1);
  const [sortBy, setSortBy] = useState("priority");
  const [sortOrder, setSortOrder] = useState("desc");
  const itemsPerPage = 50;
  useEffect(() => {
    const timer = setTimeout(() => {
      setDebouncedSearch(searchSku);
    }, 300);
    return () => clearTimeout(timer);
  }, [searchSku]);
  const vendors = useMemo(() => {
    return Array.from(new Set(audits.map((a) => a.vendor).filter(Boolean))).sort();
  }, [audits]);
  const categories = useMemo(() => {
    const cats = /* @__PURE__ */ new Set();
    audits.forEach((a) => {
    });
    return Array.from(cats).sort();
  }, [audits]);
  const filteredAudits = useMemo(() => {
    const filtered = audits.filter((audit) => {
      const matchesScore = audit.overall_score >= scoreRange[0] && audit.overall_score <= scoreRange[1];
      const matchesVendor = vendorFilter === "all" || audit.vendor === vendorFilter;
      const matchesPriority = !minPriority || audit.priority_score >= parseFloat(minPriority);
      const matchesSearch = !debouncedSearch || audit.sku.toLowerCase().includes(debouncedSearch.toLowerCase()) || audit.title.toLowerCase().includes(debouncedSearch.toLowerCase());
      return matchesScore && matchesVendor && matchesPriority && matchesSearch;
    });
    filtered.sort((a, b) => {
      let comparison = 0;
      switch (sortBy) {
        case "priority":
          comparison = a.priority_score - b.priority_score;
          break;
        case "score":
          comparison = a.overall_score - b.overall_score;
          break;
        case "revenue":
          comparison = a.total_revenue - b.total_revenue;
          break;
      }
      return sortOrder === "asc" ? comparison : -comparison;
    });
    return filtered;
  }, [audits, scoreRange, vendorFilter, minPriority, debouncedSearch, sortBy, sortOrder]);
  const totalPages = Math.ceil(filteredAudits.length / itemsPerPage);
  const paginatedAudits = useMemo(() => {
    const start = (currentPage - 1) * itemsPerPage;
    return filteredAudits.slice(start, start + itemsPerPage);
  }, [filteredAudits, currentPage]);
  useEffect(() => {
    setCurrentPage(1);
  }, [scoreRange, vendorFilter, categoryFilter, minPriority, debouncedSearch]);
