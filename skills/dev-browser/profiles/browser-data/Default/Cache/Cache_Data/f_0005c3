    setAuditing(true);
    try {
      const skus = filteredAudits.map((a) => a.sku);
      await fetch("/api/products/run-audit", {
        method: "POST",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify({ skus })
      });
      setTimeout(() => {
        window.location.reload();
      }, 2e3);
