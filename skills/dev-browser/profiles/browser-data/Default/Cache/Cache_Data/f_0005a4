        lineNumber: 710,
        columnNumber: 11
      }, this),
      /* @__PURE__ */ jsxDEV(DialogDescription, { children: product ? /* @__PURE__ */ jsxDEV(Fragment, { children: [
        "Configure packaging details for ",
        /* @__PURE__ */ jsxDEV("span", { className: "font-medium text-foreground", children: product.productName }, void 0, false, {
