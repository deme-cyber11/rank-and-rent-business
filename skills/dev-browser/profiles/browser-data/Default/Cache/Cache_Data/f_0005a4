_s2(SupplierFormModal, "UIFGkq52gee4PL3ZnA6IEihDXJw=");
_c8 = SupplierFormModal;
const defaultPrintedMaterials = {
  installGuide: false,
  warrantyCard: false,
  safetySheet: false,
  qrCode: false,
  productRegistration: false
};
const defaultPackagingFormData = {
  boxDimensions: null,
  boxWeight: null,
  shippingWeight: null,
  freightClass: null,
  logoRequired: false,
  logoFileUrl: null,
  printedMaterials: defaultPrintedMaterials,
  notes: null
};
function packagingToFormData(packaging) {
  return {
    boxDimensions: packaging.boxDimensions,
    boxWeight: packaging.boxWeight,
    shippingWeight: packaging.shippingWeight,
    freightClass: packaging.freightClass,
    logoRequired: packaging.logoRequired,
    logoFileUrl: packaging.logoFileUrl,
    printedMaterials: packaging.printedMaterials || defaultPrintedMaterials,
    notes: packaging.notes
  };
}
function PackagingEditModal({
  open,
  onOpenChange,
  packaging,
  product,
  onSuccess,
  showToast
}) {
  _s3();
  const [formData, setFormData] = useState(defaultPackagingFormData);
  const [saving, setSaving] = useState(false);
  const [error, setError] = useState(null);
  useEffect(() => {
    if (open) {
      if (packaging) {
        setFormData(packagingToFormData(packaging));
      } else {
        setFormData(defaultPackagingFormData);
      }
      setError(null);
    }
  }, [open, packaging]);
  const handleChange = (field, value) => {
    setFormData((prev) => ({ ...prev, [field]: value }));
  };
  const handleNumberChange = (field, value) => {
    if (value === "") {
      handleChange(field, null);
    } else {
      const num = parseFloat(value);
      if (!isNaN(num)) {
        handleChange(field, num);
      }
    }
  };
  const handlePrintedMaterialChange = (key, checked) => {
    setFormData((prev) => ({
      ...prev,
      printedMaterials: {
        ...prev.printedMaterials || defaultPrintedMaterials,
        [key]: checked
      }
    }));
  };
  const handleSubmit = async (e) => {
    e.preventDefault();
    if (!product) {
      setError("No product selected");
      return;
    }
    setSaving(true);
    setError(null);
    try {
      const url = packaging ? `/api/bollards/gameplan/packaging/${packaging.id}` : "/api/bollards/gameplan/packaging";
      const method = packaging ? "PUT" : "POST";
      const body = packaging ? formData : { ...formData, productId: product.id };
      const res = await fetch(url, {
        method,
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify(body)
      });
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to save packaging configuration");
      }
      showToast("success", "Packaging configuration saved");
      onSuccess();
      onOpenChange(false);
    } catch (err) {
      setError(err instanceof Error ? err.message : "An error occurred");
    } finally {
      setSaving(false);
    }
  };
  const printedMaterials = formData.printedMaterials || defaultPrintedMaterials;
  return /* @__PURE__ */ jsxDEV(Dialog, { open, onOpenChange, children: /* @__PURE__ */ jsxDEV(DialogContent, { className: "max-w-lg max-h-[90vh] overflow-y-auto", children: [
