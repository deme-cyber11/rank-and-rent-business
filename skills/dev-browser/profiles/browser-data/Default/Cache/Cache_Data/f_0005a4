const defaultPackagingSummary = {
  total: 0,
  withPackaging: 0,
  withoutPackaging: 0,
  logosPending: 0
};
const defaultSupplierSummary = {
  total: 0,
  byType: { packaging: 0, logistics: 0, manufacturer: 0, other: 0 },
  byStatus: { active: 0, reached_out: 0, negotiating: 0, inactive: 0 }
};
export function PackagingTab() {
  _s4();
  const [loading, setLoading] = useState(true);
  const [error, setError] = useState(null);
  const [products, setProducts] = useState([]);
  const [packagingSummary, setPackagingSummary] = useState(defaultPackagingSummary);
  const [suppliers, setSuppliers] = useState([]);
  const [supplierSummary, setSupplierSummary] = useState(defaultSupplierSummary);
  const [supplierFilter, setSupplierFilter] = useState("all");
  const [expandedRows, setExpandedRows] = useState(/* @__PURE__ */ new Set());
  const [supplierModalOpen, setSupplierModalOpen] = useState(false);
  const [editingSupplier, setEditingSupplier] = useState(null);
  const [packagingModalOpen, setPackagingModalOpen] = useState(false);
  const [editingPackaging, setEditingPackaging] = useState(null);
  const [selectedProduct, setSelectedProduct] = useState(null);
  const [deleteDialogOpen, setDeleteDialogOpen] = useState(false);
  const [supplierToDelete, setSupplierToDelete] = useState(null);
  const [deleting, setDeleting] = useState(false);
  const [toast, setToast] = useState({
    show: false,
    type: "success",
    message: ""
  });
  const showToast = (type, message) => {
    setToast({ show: true, type, message });
  };
  const hideToast = () => {
    setToast((prev) => ({ ...prev, show: false }));
  };
  const fetchPackaging = async () => {
    try {
      const res = await fetch("/api/bollards/gameplan/packaging");
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to load packaging data");
      }
      if (result.data) {
        if (result.data.products) {
          setProducts(result.data.products);
        }
        if (result.data.summary) {
          setPackagingSummary(result.data.summary);
        }
      }
    } catch (err) {
      throw err;
    }
  };
  const fetchSuppliers = async () => {
    try {
      const res = await fetch("/api/bollards/gameplan/suppliers");
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to load suppliers");
      }
      if (result.data) {
        setSuppliers(result.data.suppliers);
        setSupplierSummary(result.data.summary);
      }
    } catch (err) {
      throw err;
    }
  };
  const fetchAllData = async () => {
    setLoading(true);
    setError(null);
    try {
      await Promise.all([fetchPackaging(), fetchSuppliers()]);
    } catch (err) {
      setError(err instanceof Error ? err.message : "Unknown error");
    } finally {
      setLoading(false);
    }
  };
  useEffect(() => {
    fetchAllData();
  }, []);
  const filteredSuppliers = useMemo(() => {
    if (supplierFilter === "all") return suppliers;
    return suppliers.filter((s) => s.supplierType === supplierFilter);
  }, [suppliers, supplierFilter]);
  const supplierTypeCounts = useMemo(() => {
    const counts = {
      all: suppliers.length,
      packaging: 0,
      logistics: 0,
      manufacturer: 0,
      other: 0
    };
    suppliers.forEach((s) => {
      counts[s.supplierType] = (counts[s.supplierType] || 0) + 1;
    });
    return counts;
  }, [suppliers]);
  const toggleRowExpansion = (productId) => {
    setExpandedRows((prev) => {
      const next = new Set(prev);
      if (next.has(productId)) {
        next.delete(productId);
      } else {
        next.add(productId);
      }
      return next;
    });
  };
  const handleAddSupplier = () => {
    setEditingSupplier(null);
    setSupplierModalOpen(true);
  };
  const handleEditSupplier = (supplier) => {
    setEditingSupplier(supplier);
    setSupplierModalOpen(true);
  };
  const handleDeleteSupplierClick = (supplier, e) => {
    e.stopPropagation();
    setSupplierToDelete(supplier);
    setDeleteDialogOpen(true);
  };
  const handleConfirmDeleteSupplier = async () => {
    if (!supplierToDelete) return;
    setDeleting(true);
    try {
      const res = await fetch(
        `/api/bollards/gameplan/suppliers/${supplierToDelete.id}`,
        {
          method: "DELETE"
        }
      );
      const result = await res.json();
      if (!res.ok || !result.success) {
        throw new Error(result.error || "Failed to delete supplier");
      }
      showToast("success", `${supplierToDelete.supplierName} deleted`);
      setDeleteDialogOpen(false);
      setSupplierToDelete(null);
      fetchSuppliers();
    } catch (err) {
      showToast(
        "error",
        err instanceof Error ? err.message : "Delete failed"
      );
    } finally {
      setDeleting(false);
    }
  };
  const handleEditPackaging = (product) => {
    setSelectedProduct(product);
    setEditingPackaging(product.packaging || null);
    setPackagingModalOpen(true);
  };
  const handleSupplierFormSuccess = () => {
    fetchSuppliers();
  };
  const handlePackagingFormSuccess = () => {
    fetchPackaging();
  };
  const getPackagingStatus = (product) => {
    if (!product.packaging) {
      return { status: "pending", label: "Not Configured" };
    }
    if (product.packaging.boxDimensions && product.packaging.freightClass) {
      return { status: "complete", label: "Configured" };
    }
    return { status: "partial", label: "Partial" };
  };
  if (loading) {
    return /* @__PURE__ */ jsxDEV(Card, { children: /* @__PURE__ */ jsxDEV("div", { className: "flex items-center justify-center py-12", children: /* @__PURE__ */ jsxDEV(Loader2, { className: "h-8 w-8 animate-spin text-muted-foreground" }, void 0, false, {
      fileName: "C:/Projects/Source 4 Industries/src/components/bollards/gameplan/PackagingTab.tsx",
      lineNumber: 1240,
      columnNumber: 11
    }, this) }, void 0, false, {
