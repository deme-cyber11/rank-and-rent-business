const convertToMonthly = (amount, frequency) => {
  switch (frequency) {
    case "daily":
      return amount * 22;
    // ~22 business days per month
    case "weekly":
      return amount * 4;
    case "bi-weekly":
      return amount * 2;
    case "monthly":
    default:
      return amount;
  }
};
const formatFrequency = (frequency) => {
  switch (frequency) {
    case "daily":
      return "Daily";
    case "weekly":
      return "Weekly";
    case "bi-weekly":
      return "Bi-Weekly";
    case "monthly":
      return "Monthly";
    default:
      return frequency;
  }
};
export default function ActualPositionsSection({
  dealId,
  detectedPositions = [],
  onRefresh
}) {
  _s();
  const [positions, setPositions] = useState([]);
  const [loading, setLoading] = useState(true);
  const [deletingId, setDeletingId] = useState(null);
  const [showAddModal, setShowAddModal] = useState(false);
  const [verifyingPosition, setVerifyingPosition] = useState(null);
  useEffect(() => {
    fetchPositions();
  }, [dealId]);
  const fetchPositions = async () => {
    try {
      setLoading(true);
      const { data, error } = await supabase.from("deal_actual_positions").select("*").eq("deal_id", dealId).order("position_number", { ascending: true });
      if (error) throw error;
      setPositions(data || []);
    } catch (err) {
      console.error("Error fetching actual positions:", err);
    } finally {
      setLoading(false);
    }
  };
  const handleDelete = async (positionId, lenderName) => {
    if (!window.confirm(`Delete verified position for "${lenderName}"? This cannot be undone.`)) {
      return;
    }
    try {
      setDeletingId(positionId);
      const { error } = await supabase.from("deal_actual_positions").delete().eq("id", positionId);
      if (error) throw error;
      setPositions(positions.filter((p) => p.id !== positionId));
      onRefresh?.();
    } catch (err) {
      console.error("Error deleting position:", err);
      alert(err.message || "Failed to delete position");
    } finally {
      setDeletingId(null);
    }
  };
