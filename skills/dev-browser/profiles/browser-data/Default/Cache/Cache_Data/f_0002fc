function parseIndustries(industriesStr) {
  if (!industriesStr) return [];
  const items = industriesStr.split(/[,;\n]+/).map((item) => item.trim()).filter((item) => item.length > 0 && item !== "etc." && item !== "etc");
  return items;
}
