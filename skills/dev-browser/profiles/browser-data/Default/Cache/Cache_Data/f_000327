  const [notes, setNotes] = useState(initialNotes || "");
  const [originalNotes, setOriginalNotes] = useState(initialNotes || "");
  const [saving, setSaving] = useState(false);
  const [syncing, setSyncing] = useState(false);
  const [lastSaved, setLastSaved] = useState(lastUpdated);
  const [hasUnsavedChanges, setHasUnsavedChanges] = useState(false);
  useEffect(() => {
    setHasUnsavedChanges(notes !== originalNotes);
  }, [notes, originalNotes]);
  useEffect(() => {
    setNotes(initialNotes || "");
    setOriginalNotes(initialNotes || "");
    setLastSaved(lastUpdated);
  }, [initialNotes, lastUpdated]);
  const handleSave = async () => {
    if (!hasUnsavedChanges) return;
    try {
      setSaving(true);
      const now = (/* @__PURE__ */ new Date()).toISOString();
      const { error } = await supabase.from("deals").update({
        broker_notes: notes || null,
        broker_notes_updated_at: now
      }).eq("id", dealId);
      if (error) throw error;
      setOriginalNotes(notes);
      setLastSaved(now);
      setHasUnsavedChanges(false);
      onNotesUpdated?.();
    } catch (err) {
      console.error("Error saving broker notes:", err);
      alert("Failed to save notes. Please try again.");
    } finally {
      setSaving(false);
    }
  };
  const handleSyncToGHL = async () => {
    if (!notes || !dealId) return;
    try {
      setSyncing(true);
      const response = await supabase.functions.invoke("sync-deal-to-ghl", {
        body: {
          deal_id: dealId,
          action: "notes_sync"
        }
      });
      if (response.error) {
        console.error("GHL sync error:", response.error);
        alert("Failed to sync notes to GHL. Please try again.");
        return;
      }
      const result = response.data;
      if (result?.success) {
        alert("Notes synced to GHL successfully!");
      } else if (result?.error?.includes("No GHL opportunity found")) {
        alert("No matching GHL opportunity found for this deal. Please link the deal to a GHL contact first.");
      } else {
        console.warn("GHL sync returned unsuccessful:", result?.error || "Unknown error");
        alert(`GHL sync failed: ${result?.error || "Unknown error"}`);
      }
    } catch (err) {
      console.error("GHL sync exception:", err);
      alert("Failed to sync notes to GHL. Please try again.");
    } finally {
      setSyncing(false);
    }
  };
  const isGhlLinked = !!(ghlContactId || ghlOpportunityId);
  const formatTimestamp = (timestamp) => {
    if (!timestamp) return "Never saved";
    const date = new Date(timestamp);
    return date.toLocaleString("en-US", {
      month: "short",
      day: "numeric",
      year: "numeric",
      hour: "numeric",
      minute: "2-digit",
      hour12: true
    });
