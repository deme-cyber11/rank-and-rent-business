_s2(BankStatementsSection, "PW/qK9L0MMOScr63Ycl53DHXX8o=");
_c2 = BankStatementsSection;
function DetectedPositionsSection({ deal, onOpenFundingModal }) {
  const hasPositions = deal.statements?.some((s) => s.deal_funding_positions?.length > 0);
  const normalizeLenderName = (name) => {
    let normalized = name.toLowerCase().trim();
    const suffixes = ["payments", "payment", "funding", "capital", "partners", "partner", "inc", "llc", "corp", "corporation", "select", "group", "financial", "services"];
    let changed = true;
    while (changed) {
      changed = false;
      for (const suffix of suffixes) {
        const pattern = new RegExp(`\\s+${suffix}$`, "i");
        if (pattern.test(normalized)) {
          normalized = normalized.replace(pattern, "").trim();
          changed = true;
        }
      }
    }
    return normalized;
  };
  const getConsolidatedPositions = () => {
    const consolidatedPositions = /* @__PURE__ */ new Map();
    deal.statements?.forEach((statement) => {
      statement.deal_funding_positions?.forEach((position) => {
        const normalizedName = normalizeLenderName(position.lender_name);
        const key = `${normalizedName}|${position.amount}`;
        if (consolidatedPositions.has(key)) {
          const existing = consolidatedPositions.get(key);
          existing.dates.push(...position.detected_dates);
        } else {
          consolidatedPositions.set(key, {
            lender_name: position.lender_name,
            amount: position.amount,
            frequency: position.frequency,
            dates: [...position.detected_dates],
            // Use stored status, capitalize first letter for display
            status: position.status ? position.status.charAt(0).toUpperCase() + position.status.slice(1) : "Closed"
          });
        }
      });
    });
    const statusOrder = { "Active": 0, "Unclear": 1, "Closed": 2 };
    return Array.from(consolidatedPositions.values()).sort((a, b) => {
      const orderA = statusOrder[a.status] ?? 2;
      const orderB = statusOrder[b.status] ?? 2;
      if (orderA !== orderB) {
        return orderA - orderB;
      }
      return b.amount - a.amount;
    });
  };
  const formatDate = (dateStr) => {
    const date = new Date(dateStr);
    return `${date.getMonth() + 1}/${date.getDate()}`;
  };
  const statusColors = {
    "Active": "bg-green-500/20 text-green-300 border-green-500/30",
    "Closed": "bg-blue-500/20 text-blue-300 border-blue-500/30",
    "Unclear": "bg-yellow-500/20 text-yellow-300 border-yellow-500/30"
  };
  return /* @__PURE__ */ jsxDEV("section", { className: "bg-gray-800/30 border border-gray-700/30 rounded-xl overflow-hidden flex flex-col max-h-[400px]", children: [
    /* @__PURE__ */ jsxDEV("div", { className: "px-6 py-3 border-b border-gray-700/30 bg-gray-900/20 flex-shrink-0", children: /* @__PURE__ */ jsxDEV(
      "div",
      {
        className: `flex items-center gap-3 ${hasPositions ? "cursor-pointer hover:text-indigo-400" : ""} transition-colors`,
        onClick: () => hasPositions && onOpenFundingModal(),
        children: [
          /* @__PURE__ */ jsxDEV(ExternalLink, { className: "w-5 h-5 text-indigo-300" }, void 0, false, {
            fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/components/Deals/DealDetailsTabs.tsx",
            lineNumber: 794,
            columnNumber: 11
          }, this),
          /* @__PURE__ */ jsxDEV("h3", { className: "text-lg font-semibold text-white", children: "Detected Funding Positions" }, void 0, false, {
            fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/components/Deals/DealDetailsTabs.tsx",
            lineNumber: 795,
            columnNumber: 11
          }, this)
        ]
      },
      void 0,
      true,
      {
        fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/components/Deals/DealDetailsTabs.tsx",
        lineNumber: 790,
        columnNumber: 9
      },
      this
    ) }, void 0, false, {
      fileName: "C:/Projects/Huge Capital/huge-capital-mgmt/src/components/Deals/DealDetailsTabs.tsx",
      lineNumber: 789,
      columnNumber: 7
    }, this),
    hasPositions ? /* @__PURE__ */ jsxDEV("div", { className: "px-6 py-3 space-y-3 overflow-y-auto flex-1", children: (() => {
      const positionsWithStatus = getConsolidatedPositions().map((position) => {
        const uniqueDates = [...new Set(position.dates)].sort();
        return { ...position, uniqueDates };
      });
      return /* @__PURE__ */ jsxDEV("div", { className: "space-y-3", children: positionsWithStatus.map((position, idx) => {
        const formattedDates = position.uniqueDates.map(formatDate).join(", ");
        const status = position.status;
        return /* @__PURE__ */ jsxDEV("div", { className: "group/position bg-indigo-500/5 border border-indigo-500/20 rounded-lg p-2.5 relative isolate", children: [
          /* @__PURE__ */ jsxDEV("div", { className: "lg:hidden", children: [
            /* @__PURE__ */ jsxDEV("div", { className: "flex items-center justify-between gap-2 mb-1.5", children: [
