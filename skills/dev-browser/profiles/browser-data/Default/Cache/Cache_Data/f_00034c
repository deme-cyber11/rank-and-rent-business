      dealId: selectedDeal.id,
      fundedAmount: selectedSchedule.funded_amount,
      factorRate: selectedSchedule.factor_rate || void 0,
      interestRate: selectedSchedule.interest_rate ? selectedSchedule.interest_rate / 100 : void 0,
      paymentAmount: selectedSchedule.payment_amount,
      paymentFrequency: selectedSchedule.payment_frequency,
      schedule: computedSchedule,
