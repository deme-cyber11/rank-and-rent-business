export default function DebtScheduler() {
  _s();
  const [searchParams] = useSearchParams();
  const dealIdFromUrl = searchParams.get("dealId");
  const [selectedDealId, setSelectedDealId] = useState("");
  const [selectedScheduleId, setSelectedScheduleId] = useState(null);
  const [showForm, setShowForm] = useState(false);
  const [payoffType, setPayoffType] = useState("prorated");
  const [highlightPayment, setHighlightPayment] = useState(0);
  const [fundedDeals, setFundedDeals] = useState([]);
  const [dealsLoading, setDealsLoading] = useState(true);
  const [dealsError, setDealsError] = useState(null);
  const [showScenarioComparison, setShowScenarioComparison] = useState(false);
  const [scenarios, setScenarios] = useState([]);
  const { schedules, isLoading: schedulesLoading, error: schedulesError, refetch: refetchSchedules } = useDebtSchedules(selectedDealId || void 0);
  const { create, remove, isCreating, isDeleting } = useDebtSchedulesMutations();
  const { lenders } = useLenders({ status: "active" });
  const fetchFundedDeals = useCallback(async () => {
    try {
      setDealsLoading(true);
      setDealsError(null);
      const { data, error } = await supabase.from("deals").select("id, legal_business_name, dba_name, desired_loan_amount, status").eq("status", "Funded").order("legal_business_name", { ascending: true });
      if (error) throw error;
      setFundedDeals(data || []);
    } catch (err) {
      const message = err instanceof Error ? err.message : "Failed to fetch funded deals";
      setDealsError(message);
      console.error("Error fetching funded deals:", err);
    } finally {
      setDealsLoading(false);
    }
  }, []);
  useEffect(() => {
    fetchFundedDeals();
  }, [fetchFundedDeals]);
  useEffect(() => {
    if (dealIdFromUrl && fundedDeals.length > 0 && !selectedDealId) {
      const dealExists = fundedDeals.some((d) => d.id === dealIdFromUrl);
      if (dealExists) {
        setSelectedDealId(dealIdFromUrl);
      }
    }
  }, [dealIdFromUrl, fundedDeals, selectedDealId]);
  const selectedDeal = useMemo(() => {
    return fundedDeals.find((d) => d.id === selectedDealId) || null;
  }, [fundedDeals, selectedDealId]);
  const selectedSchedule = useMemo(() => {
    if (!selectedScheduleId) {
      return schedules.length > 0 ? schedules[0] : null;
    }
    return schedules.find((s) => s.id === selectedScheduleId) || null;
  }, [schedules, selectedScheduleId]);
  useEffect(() => {
    if (schedules.length > 0 && !selectedScheduleId) {
      setSelectedScheduleId(schedules[0].id);
    } else if (schedules.length === 0) {
      setSelectedScheduleId(null);
    }
  }, [schedules, selectedScheduleId]);
  useEffect(() => {
    setSelectedScheduleId(null);
    setHighlightPayment(0);
  }, [selectedDealId]);
  const computedSchedule = useMemo(() => {
    if (!selectedSchedule) return null;
    const {
      funded_amount,
      factor_rate,
      interest_rate,
      payment_amount,
      payment_frequency,
      first_payment_date
    } = selectedSchedule;
    if (factor_rate && factor_rate > 1) {
      return calculateMCASchedule({
        fundedAmount: funded_amount,
        factorRate: factor_rate,
        paymentAmount: payment_amount,
        paymentFrequency: payment_frequency,
        firstPaymentDate: first_payment_date ? new Date(first_payment_date) : /* @__PURE__ */ new Date()
      });
    } else if (interest_rate && interest_rate > 0) {
      const annualRate = interest_rate / 100;
      const estimatedTermMonths = selectedSchedule.term_length || 12;
      return calculateTermLoanSchedule({
        principal: funded_amount,
        annualRate,
        termMonths: estimatedTermMonths,
        firstPaymentDate: first_payment_date ? new Date(first_payment_date) : /* @__PURE__ */ new Date()
      });
    }
    return null;
