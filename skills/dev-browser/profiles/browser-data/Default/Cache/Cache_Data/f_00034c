    setSelectedDealId(dealId);
    setShowForm(false);
  };
  const handleScheduleChange = (scheduleId) => {
    setSelectedScheduleId(scheduleId);
    setHighlightPayment(0);
    const schedule = schedules.find((s) => s.id === scheduleId);
    if (schedule?.early_payoff_type) {
      setPayoffType(schedule.early_payoff_type);
    }
  };
  const handleFormSubmit = async (formData) => {
    const apiData = {
      deal_id: selectedDealId || null,
      lender_id: formData.lenderId || null,
      funded_amount: formData.fundedAmount,
      factor_rate: formData.scheduleType === "mca" ? formData.factorRate : null,
      interest_rate: formData.scheduleType === "term_loan" ? formData.interestRate : null,
      payment_amount: formData.paymentAmount,
      payment_frequency: formData.paymentFrequency,
      term_length: formData.termUnit === "months" ? formData.termLength : Math.ceil(formData.termLength / 30),
      first_payment_date: formData.firstPaymentDate,
      early_payoff_type: formData.earlyPayoffType,
      notes: formData.notes || null
    };
    if (formData.scheduleType === "mca" && formData.factorRate) {
      apiData.total_payback = formData.fundedAmount * formData.factorRate;
    }
    await create(apiData);
    await refetchSchedules();
    setShowForm(false);
  };
  const handleDeleteSchedule = async (scheduleId) => {
    if (!confirm("Are you sure you want to delete this debt schedule?")) return;
    await remove(scheduleId);
    await refetchSchedules();
    if (selectedScheduleId === scheduleId) {
      setSelectedScheduleId(null);
    }
  };
  const handlePaymentChange = (paymentNumber) => {
    setHighlightPayment(paymentNumber);
  };
  const handlePayoffTypeChange = (type) => {
    setPayoffType(type);
  };
