    }));
    const csv = arrayToCSV(rows, {
      rank: headers[0],
      vendor: headers[1],
      grade: headers[2],
      score: headers[3],
      revenue: headers[4],
      profit: headers[5],
      margin: headers[6],
      orders: headers[7],
      avgOrderValue: headers[8],
      revenueTrend: headers[9],
      profitTrend: headers[10],
      marginTrend: headers[11],
      ordersTrend: headers[12],
      revenueRank: headers[13],
      profitRank: headers[14],
      marginRank: headers[15],
      ordersRank: headers[16]
    });
    const filename = `vendor-report-card-${selectedPreset}.csv`;
    downloadCSV(csv, filename);
  };
