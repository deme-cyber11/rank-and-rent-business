_c2 = VendorCard;
function getPeriodType(period) {
  const preset = PERIOD_PRESETS.find((p) => p.value === period);
  if (preset) return preset.value;
  if (/^\d{4}-\d{2}-\d{2}_\d{4}-\d{2}-\d{2}$/.test(period)) return "custom";
  return "legacy";
}
export function VendorReportCardClient({
  reportCardData
}) {
  _s();
  const navigate = useNavigate();
  const [isPending, setIsPending] = useState(false);
  const initialPeriod = reportCardData.data.period || "all-time";
  const [selectedPreset, setSelectedPreset] = useState(
    getPeriodType(initialPeriod) === "legacy" ? "all-time" : getPeriodType(initialPeriod)
  );
  const [customStartDate, setCustomStartDate] = useState("");
  const [customEndDate, setCustomEndDate] = useState("");
  const [showCustomPicker, setShowCustomPicker] = useState(false);
  const [lastUpdated, setLastUpdated] = useState(null);
  const [expandedVendors, setExpandedVendors] = useState(/* @__PURE__ */ new Set());
  const [filterGrade, setFilterGrade] = useState("all");
  useEffect(() => {
    setLastUpdated(new Date(reportCardData.refreshedAt).toLocaleString());
  }, [reportCardData.refreshedAt]);
  useEffect(() => {
    if (/^\d{4}-\d{2}-\d{2}_\d{4}-\d{2}-\d{2}$/.test(initialPeriod)) {
      const [start, end] = initialPeriod.split("_");
      setCustomStartDate(start);
      setCustomEndDate(end);
      setSelectedPreset("custom");
      setShowCustomPicker(true);
    }
  }, [initialPeriod]);
  const { vendors, summary } = reportCardData.data;
  const handlePresetChange = (preset) => {
    setSelectedPreset(preset);
    if (preset === "custom") {
      setShowCustomPicker(true);
      return;
    }
    setShowCustomPicker(false);
    setIsPending(true);
    navigate(`/sales/vendor-report-card?period=${preset}`);
    setTimeout(() => setIsPending(false), 500);
  };
  const handleCustomDateApply = () => {
    if (!customStartDate || !customEndDate) return;
    const period = `${customStartDate}_${customEndDate}`;
    setIsPending(true);
    navigate(`/sales/vendor-report-card?period=${period}`);
    setTimeout(() => setIsPending(false), 500);
  };
  const toggleVendor = (vendorName) => {
    setExpandedVendors((prev) => {
      const newSet = new Set(prev);
      if (newSet.has(vendorName)) {
        newSet.delete(vendorName);
      } else {
        newSet.add(vendorName);
      }
      return newSet;
    });
  };
  const expandAll = () => {
    setExpandedVendors(new Set(vendors.map((v) => v.vendor)));
  };
  const collapseAll = () => {
    setExpandedVendors(/* @__PURE__ */ new Set());
  };
