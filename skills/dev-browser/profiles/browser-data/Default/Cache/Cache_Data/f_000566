    ];
    const rows = vendors.map((vendor, index) => ({
      rank: (index + 1).toString(),
      vendor: vendor.vendor,
      grade: vendor.grade,
      score: vendor.score.toString(),
      revenue: formatCurrencyForCSV(vendor.revenue),
      profit: formatCurrencyForCSV(vendor.profit),
      margin: (vendor.margin * 100).toFixed(2),
      orders: vendor.orders.toString(),
      avgOrderValue: formatCurrencyForCSV(vendor.avgOrderValue),
      revenueTrend: vendor.revenueTrend.toFixed(2),
      profitTrend: vendor.profitTrend.toFixed(2),
      marginTrend: vendor.marginTrend.toFixed(2),
      ordersTrend: vendor.ordersTrend.toFixed(2),
      revenueRank: vendor.revenueRank.toString(),
      profitRank: vendor.profitRank.toString(),
