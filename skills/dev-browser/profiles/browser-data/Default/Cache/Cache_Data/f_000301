var _s = $RefreshSig$();
import __vite__cjsImport1_react from "/node_modules/.vite/deps/react.js?v=aadb9797"; const useState = __vite__cjsImport1_react["useState"]; const useMemo = __vite__cjsImport1_react["useMemo"];
import { X, Activity, ChevronUp, ChevronDown, Pencil, Trash2, Loader2, Plus, RefreshCw } from "/node_modules/.vite/deps/lucide-react.js?v=aadb9797";
import { supabase } from "/src/lib/supabase.ts";
import EditFundingPositionModal from "/src/components/Deals/EditFundingPositionModal.tsx";
import AddFundingPositionModal from "/src/components/Deals/AddFundingPositionModal.tsx";
export default function FundingPositionsModal({ isOpen, onClose, positions, statements, onRefresh, onReScrub }) {
  _s();
  const [filterStatus, setFilterStatus] = useState("all");
  const [sortColumn, setSortColumn] = useState("date");
  const [sortDirection, setSortDirection] = useState("desc");
  const [editingPosition, setEditingPosition] = useState(null);
  const [deletingPositionId, setDeletingPositionId] = useState(null);
  const [updatingStatusId, setUpdatingStatusId] = useState(null);
  const [showAddModal, setShowAddModal] = useState(false);
  const [isReScrubbing, setIsReScrubbing] = useState(false);
  const transactions = useMemo(() => {
    const flat = positions.flatMap(
      (position) => position.detected_dates.map((date) => ({
        date,
        lender: position.lender_name,
        amount: position.amount,
        frequency: position.frequency,
        month: position.statement_month,
        status: position.status,
        originalPosition: position
      }))
    ).filter((tx) => filterStatus === "all" || tx.status.toLowerCase() === filterStatus);
    return flat.sort((a, b) => {
      let comparison = 0;
      switch (sortColumn) {
        case "date":
          comparison = new Date(a.date).getTime() - new Date(b.date).getTime();
          break;
        case "status":
          comparison = a.status.localeCompare(b.status);
          break;
        case "lender":
          comparison = a.lender.localeCompare(b.lender);
          break;
        case "amount":
          comparison = a.amount - b.amount;
          break;
        case "frequency":
          comparison = (a.frequency || "").localeCompare(b.frequency || "");
          break;
        case "month":
          comparison = (a.month || "").localeCompare(b.month || "");
          break;
      }
      return sortDirection === "asc" ? comparison : -comparison;
    });
  }, [positions, filterStatus, sortColumn, sortDirection]);
  const totalAmount = transactions.reduce((sum, t) => sum + t.amount, 0);
  const handleSort = (column) => {
    if (sortColumn === column) {
      setSortDirection((prev) => prev === "asc" ? "desc" : "asc");
    } else {
      setSortColumn(column);
      setSortDirection(column === "amount" ? "desc" : "asc");
    }
  };
  const handleEdit = (position) => {
    setEditingPosition(position);
  };
  const handleDelete = async (positionId, lenderName) => {
    if (!window.confirm(`Delete funding position for "${lenderName}"? This cannot be undone.`)) {
      return;
    }
    try {
      setDeletingPositionId(positionId);
      const { error } = await supabase.from("deal_funding_positions").delete().eq("id", positionId);
      if (error) throw error;
      onRefresh?.();
    } catch (err) {
      console.error("Error deleting funding position:", err);
      alert(err.message || "Failed to delete funding position");
    } finally {
      setDeletingPositionId(null);
    }
  };
  const handleEditSuccess = () => {
    setEditingPosition(null);
    onRefresh?.();
  };
  const handleAddSuccess = () => {
    setShowAddModal(false);
    onRefresh?.();
  };
  const handleReScrub = async () => {
    if (!onReScrub) return;
    const confirmMessage = "This will delete all AI-detected positions and re-analyze the bank statements. Manual positions will be preserved. Continue?";
    if (!window.confirm(confirmMessage)) return;
    try {
      setIsReScrubbing(true);
      await onReScrub();
    } catch (err) {
      console.error("Error re-scrubbing positions:", err);
      alert(err.message || "Failed to re-scrub positions");
    } finally {
      setIsReScrubbing(false);
    }
  };
  const handleStatusChange = async (positionId, newStatus) => {
    try {
      setUpdatingStatusId(positionId);
      const { error } = await supabase.from("deal_funding_positions").update({ status: newStatus }).eq("id", positionId);
      if (error) throw error;
      onRefresh?.();
    } catch (err) {
      console.error("Error updating funding position status:", err);
      alert(err.message || "Failed to update status");
    } finally {
      setUpdatingStatusId(null);
    }
  };
  if (!isOpen) return null;
  const getStatusSelectColor = (status) => {
    const normalizedStatus = status.toLowerCase();
    switch (normalizedStatus) {
      case "active":
        return "bg-green-500/10 border-green-500/30 text-green-400";
      case "unclear":
        return "bg-yellow-500/10 border-yellow-500/30 text-yellow-400";
      case "closed":
        return "bg-blue-500/10 border-blue-500/30 text-blue-400";
      default:
        return "bg-gray-500/10 border-gray-500/30 text-gray-400";
    }
  };
