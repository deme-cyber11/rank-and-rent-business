      active: "bg-gray-600 border-gray-500 text-white",
      selected: "bg-gray-600/70 border-gray-500/70 text-gray-200",
      inactive: "bg-gray-800/50 border-gray-700/50 text-gray-400 hover:bg-gray-800/70 hover:text-gray-300",
