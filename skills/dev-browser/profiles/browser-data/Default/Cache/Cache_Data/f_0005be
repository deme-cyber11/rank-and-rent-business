export function ImageGenerationPanel({
  onImageClick,
  className
}) {
  _s();
  const auth = useAuth();
  const [prompt, setPrompt] = useState("");
  const [imageType, setImageType] = useState("hero");
  const [isGenerating, setIsGenerating] = useState(false);
  const [error, setError] = useState(null);
  const [generatedImages, setGeneratedImages] = useState([]);
  const handleGenerate = useCallback(async () => {
    const token = auth.session?.access_token;
    if (!token) {
      setError("Please log in to generate images.");
      return;
    }
    if (!prompt.trim()) {
      setError("Please enter a prompt.");
      return;
    }
    setIsGenerating(true);
    setError(null);
    try {
      const response = await fetch("/api/marketing/blog/generate-image", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Authorization: `Bearer ${token}`
        },
        body: JSON.stringify({
          prompt: prompt.trim(),
          imageType
        })
      });
      const result = await response.json();
      if (!response.ok || !result.success) {
        throw new Error(result.message || result.error || "Failed to generate image");
      }
      const newImage = {
        id: `img-${Date.now()}`,
        dataUrl: result.data.dataUrl,
        promptUsed: result.data.promptUsed,
        type: result.data.type,
        generatedAt: /* @__PURE__ */ new Date()
      };
      setGeneratedImages((prev) => [newImage, ...prev]);
      setPrompt("");
      console.log("[ImageGenerationPanel] Image generated successfully");
    } catch (err) {
      console.error("[ImageGenerationPanel] Generation error:", err);
      setError(err.message || "Failed to generate image. Please try again.");
    } finally {
      setIsGenerating(false);
    }
  }, [auth.session?.access_token, prompt, imageType]);
  const handleRemoveImage = useCallback((imageId) => {
    setGeneratedImages((prev) => prev.filter((img) => img.id !== imageId));
  }, []);
  const handleImageSelect = useCallback((image) => {
    onImageClick?.(image);
  }, [onImageClick]);
