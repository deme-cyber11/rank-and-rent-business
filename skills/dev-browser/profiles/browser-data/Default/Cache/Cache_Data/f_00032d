var _s = $RefreshSig$();
import __vite__cjsImport1_react from "/node_modules/.vite/deps/react.js?v=aadb9797"; const useState = __vite__cjsImport1_react["useState"]; const useCallback = __vite__cjsImport1_react["useCallback"];
import {
  X,
  Loader,
  AlertCircle,
  CheckCircle,
  Upload,
  FileText,
  Trash2,
  RefreshCw
} from "/node_modules/.vite/deps/lucide-react.js?v=aadb9797";
import { supabase } from "/src/lib/supabase.ts";
const INITIAL_STAGES = [
  { key: "upload", label: "Upload Files", status: "pending" },
  { key: "parse", label: "Parse Statements", status: "pending" },
  { key: "save", label: "Save to Database", status: "pending" },
  { key: "match", label: "Re-match Lenders", status: "pending" }
];
const createId = () => typeof crypto !== "undefined" && typeof crypto.randomUUID === "function" ? crypto.randomUUID() : Math.random().toString(36).slice(2);
const fileToBase64 = (file) => new Promise((resolve, reject) => {
  const reader = new FileReader();
  reader.onload = () => {
    const result = reader.result;
    if (typeof result === "string") {
      const base64 = result.split(",")[1] || result;
      resolve(base64);
    } else {
      reject(new Error("Failed to read file"));
    }
  };
  reader.onerror = () => reject(reader.error ?? new Error("Failed to read file"));
  reader.readAsDataURL(file);
});
const numberOrNull = (value) => {
  if (typeof value === "number" && !Number.isNaN(value)) return value;
  if (typeof value === "string" && value.trim().length) {
    const parsed = Number(value.replace(/[^0-9.-]+/g, ""));
    return Number.isNaN(parsed) ? null : parsed;
  }
  return null;
};
export default function AddStatementsModal({
  isOpen,
  onClose,
  dealId,
  dealName,
  loanType,
  existingStatementMonths,
  onSuccess
}) {
  _s();
  const [files, setFiles] = useState([]);
  const [stages, setStages] = useState([...INITIAL_STAGES]);
  const [isWorking, setIsWorking] = useState(false);
  const [mode, setMode] = useState("upload");
  const [errorMessage, setErrorMessage] = useState(null);
  const [addedStatements, setAddedStatements] = useState(0);
  const [duplicateMonths, setDuplicateMonths] = useState([]);
  const resetModal = useCallback(() => {
    setFiles([]);
    setStages([...INITIAL_STAGES]);
    setIsWorking(false);
    setMode("upload");
    setErrorMessage(null);
    setAddedStatements(0);
    setDuplicateMonths([]);
  }, []);
  const handleClose = useCallback(() => {
    if (isWorking) return;
    resetModal();
    onClose();
  }, [isWorking, onClose, resetModal]);
  const updateStage = useCallback((key, updates) => {
    setStages(
      (prev) => prev.map((stage) => stage.key === key ? { ...stage, ...updates } : stage)
    );
  }, []);
  const handleDrop = useCallback((e) => {
    e.preventDefault();
    if (isWorking) return;
    const droppedFiles = Array.from(e.dataTransfer.files).filter(
      (file) => file.type === "application/pdf"
    );
    if (droppedFiles.length > 0) {
      setFiles(
        (prev) => [
          ...prev,
          ...droppedFiles.map((file) => ({
            id: createId(),
            file,
            name: file.name,
            size: file.size,
            status: "pending"
          }))
        ]
      );
    }
  }, [isWorking]);
  const handleFileSelect = useCallback((e) => {
    if (isWorking || !e.target.files) return;
    const selectedFiles = Array.from(e.target.files).filter(
      (file) => file.type === "application/pdf"
    );
    if (selectedFiles.length > 0) {
      setFiles(
        (prev) => [
          ...prev,
          ...selectedFiles.map((file) => ({
            id: createId(),
            file,
            name: file.name,
            size: file.size,
            status: "pending"
          }))
        ]
      );
    }
    e.target.value = "";
  }, [isWorking]);
  const removeFile = useCallback((id) => {
    if (isWorking) return;
    setFiles((prev) => prev.filter((f) => f.id !== id));
  }, [isWorking]);
  const handleSubmit = useCallback(async () => {
    if (files.length === 0 || isWorking) return;
    setIsWorking(true);
    setMode("processing");
    setErrorMessage(null);
    setStages([...INITIAL_STAGES]);
    try {
      updateStage("upload", { status: "in_progress", detail: "Preparing files..." });
      setFiles((prev) => prev.map((f) => ({ ...f, status: "uploading" })));
      const payload = await Promise.all(
        files.map(async (f) => ({
          name: f.name,
          type: f.file.type,
          content: await fileToBase64(f.file)
        }))
      );
      updateStage("upload", { status: "success", detail: `${files.length} file(s) ready` });
      setFiles((prev) => prev.map((f) => ({ ...f, status: "parsing" })));
      updateStage("parse", { status: "in_progress", detail: "Analyzing bank statements..." });
      const { data: parseResult, error: parseError } = await supabase.functions.invoke(
        "parse-bank-statements",
        { body: { files: payload } }
      );
      if (parseError) {
        throw new Error(`Failed to parse bank statements: ${parseError.message}`);
      }
      if (!parseResult?.statements || parseResult.statements.length === 0) {
        throw new Error("No statement data could be extracted from the uploaded files.");
      }
      const parsedStatements = parseResult.statements;
      const parsedPositions = parseResult.fundingPositions || [];
      updateStage("parse", {
        status: "success",
        detail: `Extracted ${parsedStatements.length} statement(s)`
      });
      setFiles((prev) => prev.map((f) => ({ ...f, status: "success" })));
      updateStage("save", { status: "in_progress", detail: "Saving to database..." });
      const newStatements = [];
      const duplicates = [];
      for (const stmt of parsedStatements) {
        const month = stmt.statement_month || "";
        if (existingStatementMonths.includes(month)) {
          duplicates.push(month);
        } else {
          newStatements.push(stmt);
        }
      }
      setDuplicateMonths(duplicates);
      if (newStatements.length === 0) {
        throw new Error(
          `All ${parsedStatements.length} statement(s) are for months that already exist on this deal.`
        );
      }
      const insertedStatementIds = [];
      for (const statement of newStatements) {
        const statementIdentifier = statement.statement_id || `${dealId}-statement-${createId()}`;
        const { data: inserted, error: insertError } = await supabase.from("deal_bank_statements").insert({
          deal_id: dealId,
          bank_name: statement.bank_name || "Unknown Bank",
          statement_month: statement.statement_month || "Unknown",
          statement_id: statementIdentifier,
          statement_file_url: null,
          credits: numberOrNull(statement.credits),
          debits: numberOrNull(statement.debits),
          nsfs: numberOrNull(statement.nsfs) ?? 0,
          overdrafts: numberOrNull(statement.overdrafts) ?? 0,
          negative_days: numberOrNull(statement.negative_days) ?? 0,
          average_daily_balance: numberOrNull(statement.average_daily_balance),
          deposit_count: numberOrNull(statement.deposit_count)
        }).select("id").single();
        if (insertError) {
          console.error("Failed to insert statement:", insertError);
          continue;
        }
        if (inserted?.id) {
          insertedStatementIds.push({
            id: inserted.id,
            month: statement.statement_month || ""
          });
        }
      }
      if (parsedPositions.length > 0 && insertedStatementIds.length > 0) {
        const monthToId = /* @__PURE__ */ new Map();
        for (const { id, month } of insertedStatementIds) {
          monthToId.set(month, id);
        }
        for (const fp of parsedPositions) {
          const fpMonth = fp.statement_month || "";
          const targetStatementId = monthToId.get(fpMonth) || insertedStatementIds[0]?.id;
          if (!targetStatementId) continue;
          await supabase.from("deal_funding_positions").insert({
            statement_id: targetStatementId,
            lender_name: fp.lender_name || "Unknown Lender",
            amount: numberOrNull(fp.amount) ?? 0,
            frequency: fp.frequency || "daily",
            detected_dates: Array.isArray(fp.detected_dates) ? fp.detected_dates : []
          });
        }
      }
      setAddedStatements(insertedStatementIds.length);
      updateStage("save", {
        status: "success",
        detail: `Added ${insertedStatementIds.length} statement(s)${duplicates.length > 0 ? `, skipped ${duplicates.length} duplicate(s)` : ""}`
      });
      updateStage("match", { status: "in_progress", detail: "Re-matching lenders..." });
      try {
        const { data: allStatements } = await supabase.from("deal_bank_statements").select("*").eq("deal_id", dealId).order("statement_month", { ascending: false });
        const { data: dealInfo } = await supabase.from("deals").select("*").eq("id", dealId).single();
        if (dealInfo && allStatements) {
          await supabase.functions.invoke("match-deal-to-lenders", {
            body: {
              dealId,
              loanType,
              deal: {
                deal: {
                  legal_business_name: dealInfo.legal_business_name,
                  dba_name: dealInfo.dba_name,
                  ein: dealInfo.ein,
                  business_type: dealInfo.business_type,
                  address: dealInfo.address,
                  city: dealInfo.city,
                  state: dealInfo.state,
                  zip: dealInfo.zip,
                  average_monthly_sales: dealInfo.average_monthly_sales,
                  average_monthly_card_sales: dealInfo.average_monthly_card_sales,
                  desired_loan_amount: dealInfo.desired_loan_amount,
                  reason_for_loan: dealInfo.reason_for_loan,
                  business_start_date: dealInfo.business_start_date,
                  product_service_sold: dealInfo.product_service_sold,
                  seasonal_business: dealInfo.seasonal_business,
                  franchise_business: dealInfo.franchise_business
                },
                owners: [],
                statements: allStatements.map((s) => ({
                  bank_name: s.bank_name,
                  statement_month: s.statement_month,
                  credits: s.credits,
                  debits: s.debits,
                  nsfs: s.nsfs ?? 0,
                  overdrafts: s.overdrafts ?? 0,
                  average_daily_balance: s.average_daily_balance,
                  deposit_count: s.deposit_count
                })),
                fundingPositions: []
              }
            }
          });
        }
        updateStage("match", { status: "success", detail: "Lender matches updated" });
      } catch (matchErr) {
        console.error("Lender re-matching failed:", matchErr);
        updateStage("match", {
          status: "error",
          detail: "Lender re-matching failed (statements saved successfully)"
        });
      }
      setMode("success");
      setIsWorking(false);
    } catch (err) {
      console.error("Add statements error:", err);
      const message = err instanceof Error ? err.message : "An unexpected error occurred.";
      setErrorMessage(message);
      setMode("error");
      setIsWorking(false);
      setFiles((prev) => prev.map((f) => ({ ...f, status: "error", error: message })));
    }
  }, [files, isWorking, dealId, existingStatementMonths, loanType, updateStage]);
  if (!isOpen) return null;
  return /* @__PURE__ */ jsxDEV("div", { className: "fixed inset-0 bg-black/60 flex items-center justify-center z-50 p-4", children: /* @__PURE__ */ jsxDEV("div", { className: "bg-gray-900 border border-gray-700 rounded-lg max-w-2xl w-full max-h-[85vh] overflow-y-auto shadow-2xl", children: [
    /* @__PURE__ */ jsxDEV("div", { className: "sticky top-0 flex items-center justify-between bg-gray-900 border-b border-gray-800 px-6 py-4", children: [
      /* @__PURE__ */ jsxDEV("div", { children: [
